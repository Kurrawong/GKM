# baseURI: http://geoscience.ca/3D/ontology/2019/1/ex-timescale
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://purl.org/dc/elements/1.1/

@prefix : <http://geoscience.ca/3D/ontology/2019/1/ex-timescale#> .
@prefix common: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tim: <http://geoscience.ca/3D/ontology/2019/1/ex-timescale#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://geoscience.ca/3D/ontology/2019/1/ex-timescale>
  a owl:Ontology ;
  rdfs:comment "Example encoding of a geochronologic time interval" ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
.
tim:Lower_Jurassic_Chronostrat_Unit
  a geo:Series ;
  geo:realizes tim:Lower_Jurassic_Epoch ;
  rdfs:comment "ISC2017 time scale does not define stratotypes for the Geochronologic Eras it defines" ;
  rdfs:comment "see Table 3 in http://www.stratigraphy.org/upload/bak/chron.htm for hierarchy of chronostrat and geochron unit names" ;
  rdfs:label "Lower Jurassic rocks" ;
.
tim:Lower_Jurassic_Epoch
  a geo:Epoch ;
  common:isPartOf <http://resource.geosciml.org/vocabulary/timescale/isc2017> ;
  geo:hasOlderDate [
      geo:hasDate 201.3 ;
      geo:hasUOM [
          rdfs:type common:ma ;
        ] ;
      geo:hasUncertainty 0.2 ;
      geo:isRealizedBy <http://resource.geosciml.org/classifier/ics/ischart/BaseJurassic> ;
      geo:stratotype <http://resource.geosciml.org/classifier/ics/ischart/BaseJurassicSP> ;
      rdfs:type geo:Geochronologic_Boundary ;
    ] ;
  geo:hasYoungerDate [
      geo:hasDate 174.1 ;
      geo:hasUOM [
          rdfs:type common:ma ;
        ] ;
      geo:hasUncertainty 1.0 ;
      geo:isRealizedBy <http://resource.geosciml.org/classifier/ics/ischart/BaseMiddleJurassic> ;
      geo:stratotype <http://resource.geosciml.org/classifier/ics/ischart/BaseMiddleJurassicSP> ;
      rdfs:comment "the Base Middle Jurassic class represents all rock body surfaces correlated with the surface at the Global Stratotype Point" ;
      rdfs:type geo:Geochronologic_Boundary ;
    ] ;
  rdfs:comment "Epoch is a subclass of Geochronologic Era" ;
  rdfs:comment "This is a temporal interval" ;
  rdfs:label "Lower Jurassic time interval" ;
.
