@prefix con: <http://loop3d.org/GKO/ontology/2019/1/ex-complexcontact#> .
@prefix ejs: <http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix env: <http://loop3d.org/GKO/ontology/2019/1/eventenvironment> .
@prefix gkoc: <http://loop3d.org/GKO/ontology/2019/1/common/> .
@prefix gkog: <http://loop3d.org/GKO/ontology/2019/1/geologicfeature/> .
@prefix gkop: <http://loop3d.org/GKO/ontology/2019/1/geologicproperty/> .
@prefix isca: <http://loop3d.org/GKO/ontology/2019/1/ischart/> .
@prefix lith: <http://loop3d.org/GKO/ontology/2019/1/lithology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pro: <http://loop3d.org/GKO/ontology/2019/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix unit: <http://loop3d.org/GKO/ontology/2019/1/uom/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs>
  a owl:Ontology ;
  rdfs:comment "Stephen M. Richard 2019-05-02" ;
  rdfs:comment "details of description of Js unit in GeologyScenarioForTesting.pptx" ;
  owl:imports <http://loop3d.org/GKO/ontology/2019/1/loop3dmaster> ;
.

ejs:JsFormation
  a gkog:Formation ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.1.4> ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.2.4> ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-complexcontact#KgInJsIntrusiveContact-4.2> ;
  gkoc:hasPart ejs:JsFormation-lower ;
  gkoc:hasPart ejs:JsFormation-upper ;
  gkoc:hasPart ejs:baseJs-6 ;
  gkoc:hasPart ejs:topJs-2 ;
  gkoc:hasTemporalProperty [
      a gkog:Chronostratigraphic_Age_Interval ;
      gkog:hasOlderBound isca:LowerJurassic ;
      gkog:hasYoungerBound isca:LowerJurassic ;
    ] ;
  rdfs:comment "clasts of Cb Quartzite are abundant in the lower part of the unit"@en ;
  rdfs:comment "lower part is fining-upward sequence from conglomeratic sandstone to fine grained sandstone"@en ;
  rdfs:comment "marker bed is a tuff in the upper part of the lower clastic interval"@en ;
  rdfs:comment "the following parts are not elucidated in this example, but are referenced in the Contact instances below. These surfaces would participate in intrusion and also ?contact metamorphism? processes"@en ;
  rdfs:comment "upper part is massive limestone with abundant ammonites"@en ;
  rdfs:label "Js Formation"@en ;
.
ejs:JsFormation-lower
  a gkog:Rock_Body_Part ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4> ;
  gkoc:hasPart ejs:baseJs-6 ;
  gkoc:hasPart ejs:lith_1 ;
  gkoc:hasPart ejs:lith_2 ;
  gkoc:hasPart ejs:lith_3 ;
  gkog:underlies ejs:JsFormation-upper ;
  rdfs:comment "underlies, overlies, within are subproperty of relatedTo" ;
.
<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4>
  a gkog:Bed ;
  gkoc:hasConstituent <http://uri.org/Ballyloghlin_tuff> ;
  gkoc:hasPart [
      a gkog:Rock_Body_Bottom ;
      gkoc:isParticipantIn [
          a pro:deposition ;
        ] ;
    ] ;
  gkoc:hasPart [
      a gkog:Rock_Body_Top ;
    ] ;
  gkog:within ejs:lith_3 ;
  rdfs:comment "Bed is viewed as a degenerate volume, considered a surface that has a hasConstiuent property, and hasPart that represents contacts of the bed" ;
.
ejs:baseJs-6
  a gkog:Rock_Body_Bottom ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.1> ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.2> ;
  gkoc:hasPart <http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.3> ;
.
<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.1>
  a gkog:Rock_Body_Bottom ;
  gkoc:isParticipantIn [
      a pro:deposition ;
    ] ;
  rdfs:comment "Js on Ss surface" ;
.
<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.2>
  a gkog:Rock_Body_Bottom ;
  gkoc:isParticipantIn [
      a pro:deposition ;
    ] ;
  rdfs:comment "Js on Os surface" ;
.
<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#baseJs-6.3>
  a gkog:Rock_Body_Bottom ;
  gkoc:isParticipantIn [
      a pro:deposition ;
    ] ;
  rdfs:comment "Js on Xm surface" ;
.
ejs:contactJs-lith_1-lith_2
  a gkog:Gradational_Contact_Zone ;
  gkog:hasBoundaryThickness [
      a gkop:Thickness ;
      gkoc:hasUOM [a unit:meter ];
      gkoc:hasValue 10 ;
    ] ;
  gkog:hasHost ejs:lith_1 ;
  gkog:hasHost ejs:lith_2 ;
  rdfs:label "internal gradational contact " ;
.
ejs:contactJs-lith_2-lith_3
  a gkog:Gradational_Contact_Zone ;
  gkog:hasBoundaryThickness [
      a gkop:Thickness ;
      gkoc:hasUOM [a unit:meter ] ;
      gkoc:hasValue 4 ;
    ] ;
  gkog:hasHost ejs:lith_2 ;
  gkog:hasHost ejs:lith_3 ;
  rdfs:label "internal gradational contact " ;
.
ejs:lith_1
  a gkog:Rock_Body_Part ;
  gkoc:hasConstituent lith:arenite ;
  gkoc:hasConstituent lith:clastic_conglomerate ;
  gkoc:hasPart ejs:baseJs-6 ;
  gkog:underlies ejs:lith_2 ;
  rdfs:comment "need to capture gradational fining upward relationship" ;
.
ejs:lith_2
  a gkog:Rock_Body_Part ;
  gkoc:hasConstituent [
      a lith:clastic_sandstone ;
    ] ;
  gkoc:hasPart [
      a gkog:Rock_Body_Bottom ;
      gkoc:isParticipantIn [
          a pro:deposition ;
        ] ;
    ] ;
  gkog:overlies ejs:lith_1 ;
  gkog:underlies ejs:lith_3 ;
.
ejs:lith_3
  a gkog:Rock_Body_Part ;
  gkoc:hasConstituent [
	  a lith:clastic_sandstone ;
      rdfs:label "fine-Grained sandstone" ;
    ] ;
  gkoc:hasPart [
      a gkog:Rock_Body_Bottom ;
      gkoc:isParticipantIn [
          a pro:deposition ;
        ] ;
    ] ;
  gkog:overlies ejs:lith_2 ;
.
<http://loop3d.org/GKO/ontology/2019/1/ex-formationJs#topJs-2.1>
  a gkog:Rock_Body_Top ;
  gkoc:isParticipantIn [
      a pro:erosion ;
    ] ;
.
