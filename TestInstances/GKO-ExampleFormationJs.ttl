# baseURI: http://loop3d.org/GSO/ontology/2019/1/ex-formationJs
# imports: http://loop3d.org/GSO/ontology/2019/1/loop3dmaster
# prefix: ejs

@prefix con: <http://loop3d.org/GSO/ontology/2019/1/ex-complexcontact#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ejs: <http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#> .
@prefix gsen: <http://loop3d.org/GSO/ontology/2019/1/eventenvironment> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2019/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2019/1/geologicfeature/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2019/1/geologicproperty/> .
@prefix gstime: <http://loop3d.org/GSO/ontology/2019/1/ischart/> .
@prefix gslth: <http://loop3d.org/GSO/ontology/2019/1/lithology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2019/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix gsuom: <http://loop3d.org/GSO/ontology/2019/1/uom/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs>
  a owl:Ontology ;
  rdfs:comment "Stephen M. Richard 2019-05-02" ;
  rdfs:comment "details of description of Js unit in GeologyScenarioForTesting.pptx" ;
  owl:imports <http://loop3d.org/GSO/ontology/2019/1/loop3dmaster> ;
.
ejs:JsFormation
  a gsog:Formation ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.1.4> ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.2.4> ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-complexcontact#KgInJsIntrusiveContact-4.2> ;
  gsoc:hasPart ejs:JsFormation-lower ;
  gsoc:hasPart ejs:JsFormation-upper ;
  gsoc:hasPart ejs:baseJs-6 ;
  gsoc:hasPart ejs:topJs-2 ;
  gsoc:hasTemporalProperty [
      a gsog:Chronostratigraphic_Age_Interval ;
      gsog:hasOlderBound gstime:LowerJurassic ;
      gsog:hasYoungerBound gstime:LowerJurassic ;
    ] ;
  rdfs:comment "clasts of Cb Quartzite are abundant in the lower part of the unit"@en ;
  rdfs:comment "lower part is fining-upward sequence from conglomeratic sandstone to fine grained sandstone"@en ;
  rdfs:comment "marker bed is a tuff in the upper part of the lower clastic interval"@en ;
  rdfs:comment "the following parts are not elucidated in this example, but are referenced in the Contact instances below. These surfaces would participate in intrusion and also ?contact metamorphism? processes"@en ;
  rdfs:comment "upper part is massive limestone with abundant ammonites"@en ;
  rdfs:label "Js Formation"@en ;
.
ejs:JsFormation-lower
  a gsog:Rock_Body_Part ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4> ;
  gsoc:hasPart ejs:baseJs-6 ;
  gsoc:hasPart ejs:lith_1 ;
  gsoc:hasPart ejs:lith_2 ;
  gsoc:hasPart ejs:lith_3 ;
  gsog:underlies ejs:JsFormation-upper ;
  rdfs:comment "underlies, overlies, within are subproperty of relatedTo" ;
.
<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4>
  a gsog:Bed ;
  gsoc:hasConstituent <http://uri.org/Ballyloghlin_tuff> ;
  gsoc:hasPart [
      a gsog:Rock_Body_Bottom ;
      gsoc:isParticipantIn [
          a gspr:Deposition ;
        ] ;
    ] ;
  gsoc:hasPart [
      a gsog:Rock_Body_Top ;
    ] ;
  gsog:within ejs:lith_3 ;
  rdfs:comment "Bed is viewed as a degenerate volume, considered a surface that has a hasConstiuent property, and hasPart that represents contacts of the bed" ;
.
ejs:baseJs-6
  a gsog:Rock_Body_Bottom ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.1> ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.2> ;
  gsoc:hasPart <http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.3> ;
.
<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.1>
  a gsog:Rock_Body_Bottom ;
  gsoc:isParticipantIn [
      a gspr:Deposition ;
    ] ;
  rdfs:comment "Js on Ss surface" ;
.
<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.2>
  a gsog:Rock_Body_Bottom ;
  gsoc:isParticipantIn [
      a gspr:Deposition ;
    ] ;
  rdfs:comment "Js on Os surface" ;
.
<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#baseJs-6.3>
  a gsog:Rock_Body_Bottom ;
  gsoc:isParticipantIn [
      a gspr:Deposition ;
    ] ;
  rdfs:comment "Js on Xm surface" ;
.
ejs:contactJs-lith_1-lith_2
  a gsog:Gradational_Contact_Zone ;
  gsog:hasBoundaryThickness [
      a gsop:Thickness ;
      gsoc:hasUOM [
          a gsuom:meter ;
        ] ;
      gsoc:hasValue 10 ;
    ] ;
  gsog:hasHost ejs:lith_1 ;
  gsog:hasHost ejs:lith_2 ;
  rdfs:label "internal gradational contact " ;
.
ejs:contactJs-lith_2-lith_3
  a gsog:Gradational_Contact_Zone ;
  gsog:hasBoundaryThickness [
      a gsop:Thickness ;
      gsoc:hasUOM [
          a gsuom:meter ;
        ] ;
      gsoc:hasValue 4 ;
    ] ;
  gsog:hasHost ejs:lith_2 ;
  gsog:hasHost ejs:lith_3 ;
  rdfs:label "internal gradational contact " ;
.
ejs:lith_1
  a gsog:Rock_Body_Part ;
  gsoc:hasConstituent gslth:arenite ;
  gsoc:hasConstituent gslth:clastic_conglomerate ;
  gsoc:hasPart ejs:baseJs-6 ;
  gsog:underlies ejs:lith_2 ;
  rdfs:comment "need to capture gradational fining upward relationship" ;
.
ejs:lith_2
  a gsog:Rock_Body_Part ;
  gsoc:hasConstituent [
      a gslth:clastic_sandstone ;
    ] ;
  gsoc:hasPart [
      a gsog:Rock_Body_Bottom ;
      gsoc:isParticipantIn [
          a gspr:Deposition ;
        ] ;
    ] ;
  gsog:overlies ejs:lith_1 ;
  gsog:underlies ejs:lith_3 ;
.
ejs:lith_3
  a gsog:Rock_Body_Part ;
  gsoc:hasConstituent [
      a gslth:clastic_sandstone ;
      rdfs:label "fine-Grained sandstone" ;
    ] ;
  gsoc:hasPart [
      a gsog:Rock_Body_Bottom ;
      gsoc:isParticipantIn [
          a gspr:Deposition ;
        ] ;
    ] ;
  gsog:overlies ejs:lith_2 ;
.
<http://loop3d.org/GSO/ontology/2019/1/ex-formationJs#topJs-2.1>
  a gsog:Rock_Body_Top ;
  gsoc:isParticipantIn [
      a gspr:Erosion ;
    ] ;
.
