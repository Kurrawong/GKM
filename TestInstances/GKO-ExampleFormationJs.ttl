# baseURI: http://geoscience.ca/3D/ontology/2019/1/ex-formationJs
# imports: http://geoscience.ca/3D/ontology/2019/1/common
# imports: http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact
# imports: http://geoscience.ca/3D/ontology/2019/1/ischart/Loop3DTimeScale
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://purl.org/dc/elements/1.1/
# prefix: ejs

@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix con: <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#> .
@prefix ejs: <http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix isca: <http://geoscience.ca/3D/ontology/2019/1/ischart/> .
@prefix unit: <http://qudt.org/vocab/unit#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix lith: <http://geoscience.ca/3D/ontology/2019/1/lithology/> .

<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs>
  a owl:Ontology ;
  rdfs:comment "Example encoding of a geochronologic time interval" ;
  rdfs:comment "Stephen M. Richard 2019-05-02" ;
  rdfs:comment "details of description of Js unit in GeologyScenarioForTesting.pptx" ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ischart/Loop3DTimeScale> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
.
ejs:JsFormation
  a geo:Formation ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.1.4> ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#KdInJsDikeContact-3.2.4> ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#KgInJsIntrusiveContact-4.2> ;
  com:hasPart ejs:JsFormation-lower ;
  com:hasPart ejs:JsFormation-upper ;
  com:hasPart ejs:baseJs-6 ;
  com:hasPart ejs:topJs-2 ;
  com:hasTemporalProperty [
      a geo:Chronostratigraphic_Age_Interval ;
      geo:hasOlderBound isca:LowerJurassic ;
      geo:hasYoungerBound isca:LowerJurassic ;
    ] ;
  rdfs:comment "clasts of Cb Quartzite are abundant in the lower part of the unit"@en ;
  rdfs:comment "lower part is fining-upward sequence from conglomeratic sandstone to fine grained sandstone"@en ;
  rdfs:comment "marker bed is a tuff in the upper part of the lower clastic interval"@en ;
  rdfs:comment "the following parts are not elucidated in this example, but are referenced in the Contact instances below. These surfaces would participate in intrusion and also ?contact metamorphism? processes"@en ;
  rdfs:comment "upper part is massive limestone with abundant ammonites"@en ;
  rdfs:label "Js Formation"@en ;
.
ejs:JsFormation-lower
  a geo:Rock_Body_Part ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4> ;
  com:hasPart ejs:baseJs-6 ;
  com:hasPart ejs:lith_1 ;
  com:hasPart ejs:lith_2 ;
  com:hasPart ejs:lith_3 ;
  geo:underlies ejs:JsFormation-upper ;
  rdfs:comment "underlies, overlies, within are subproperty of relatedTo" ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#JsFormationMarker-6.4>
  a geo:Bed ;
  com:hasConstituent <http://uri.org/Ballyloghlin_tuff> ;
  com:hasPart [
      a geo:Rock_Body_Bottom ;
      com:participantIn [
          a geo:Deposition ;
        ] ;
    ] ;
  com:hasPart [
      a geo:Rock_Body_Top ;
    ] ;
  geo:within ejs:lith_3 ;
  rdfs:comment "Bed is viewed as a degenerate volume, considered a surface that has a hasConstiuent property, and hasPart that represents contacts of the bed" ;
.
ejs:baseJs-6
  a geo:Rock_Body_Bottom ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.1> ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.2> ;
  com:hasPart <http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.3> ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.1>
  a geo:Rock_Body_Bottom ;
  com:participantIn [
      a geo:Deposition ;
    ] ;
  rdfs:comment "Js on Ss surface" ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.2>
  a geo:Rock_Body_Bottom ;
  com:participantIn [
      a geo:Deposition ;
    ] ;
  rdfs:comment "Js on Os surface" ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#baseJs-6.3>
  a geo:Rock_Body_Bottom ;
  com:participantIn [
      a geo:Deposition ;
    ] ;
  rdfs:comment "Js on Xm surface" ;
.
ejs:contactJs-lith_1-lith_2
  a geo:Gradational_Contact_Zone ;
  geo:hasBoundaryThickness [
      a geo:Thickness ;
      com:hasUOM unit:Meter ;
      com:hasValue 10 ;
    ] ;
  geo:hasOlderHost ejs:lith_1 ;
  geo:hasYoungerHost ejs:lith_2 ;
  rdfs:label "internal gradational contact " ;
.
ejs:contactJs-lith_2-lith_3
  a geo:Gradational_Contact_Zone ;
  geo:hasBoundaryThickness [
      a geo:Thickness ;
      com:hasUOM unit:Meter ;
      com:hasValue 4 ;
    ] ;
  geo:olderHost ejs:lith_2 ;
  geo:youngerHost ejs:lith_3 ;
  rdfs:label "internal gradational contact " ;
.
ejs:lith_1
  a geo:Rock_Body_Part ;
  com:hasConstituent lith:clastic_conglomerate, lith:arenite ;
  rdfs:comment "need to capture gradational fining upward relationship" ;
  com:hasPart ejs:baseJs-6 ;
  geo:underlies ejs:lith_2 ;
.
ejs:lith_2
  a geo:Rock_Body_Part ;
  com:hasConstituent [
      a geo:Sandstone ;
    ] ;
  com:hasPart [
      a geo:Rock_Body_Bottom ;
      com:participantIn [
          a geo:Deposition ;
        ] ;
    ] ;
  geo:overlies ejs:lith_1 ;
  geo:underlies ejs:lith_3 ;
.
ejs:lith_3
  a geo:Rock_Body_Part ;
  com:hasConstituent [
      a geo:Fine_Grained_Sandstone ;
    ] ;
  com:hasPart [
      a geo:Rock_Body_Bottom ;
      com:participantIn [
          a geo:Deposition ;
        ] ;
    ] ;
  geo:overlies ejs:lith_2 ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-formationJs#topJs-2.1>
  a geo:Rock_Body_Top ;
  com:participantIn [
      a geo:Erosion ;
    ] ;
.
