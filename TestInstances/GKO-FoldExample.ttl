<http://loop3d.org/GKO/ontology/2019/1/foldexample>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GKO/ontology/2019/1/loop3dmaster> ;
  owl:versionIRI <http://loop3d.org/GKO/ontology/2019/1/foldexample> ;
.
exfold:Fold_1
  a gkog:Fold ;
  gkoc:hasPart [
      a gkof:Fold_Hinge ;
      gkoc:hasProperty [
          a gkop:Orientation ;
          gkoc:hasPhysicalProperty [
              a gkop:Plunge ;
              gkoc:hasNumericValue "33" ;
            ] ;
          gkoc:hasPhysicalProperty [
              a gkop:Trend ;
              gkoc:hasNumericValue "330" ;
            ] ;
        ] ;
      rdfs:label "Fold hinge: 33 to 330" ;
    ] ;
  gkoc:hasPhysicalProperty [
      a gkof:Fold_Profile_Type ;
    ] ;
  rdfs:comment "test fold properties" ;
  rdfs:label "is test fold" ;
.
