gsoc:hostedBy
  owl:inverseOf gsoc:hosts ;
.
<http://loop3d.org/GSO/ontology/2019/1/foldexample>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GSO/ontology/2019/1/loop3dmaster> ;
  owl:versionIRI <http://loop3d.org/GSO/ontology/2019/1/foldexample> ;
.
exfold:Fold_1
  a gsog:Fold ;
  a owl:NamedIndividual ;
  gsoc:hasPart [
      a gssf:Fold_Hinge ;
      gsoc:hasProperty [
          a gsop:Orientation ;
          gsoc:hasPhysicalProperty [
              a gsop:Plunge ;
              gsoc:hasNumericValue "33" ;
            ] ;
          gsoc:hasPhysicalProperty [
              a gsop:Trend ;
              gsoc:hasNumericValue "330" ;
            ] ;
        ] ;
      rdfs:label "Fold hinge: 33 to 330" ;
    ] ;
  gsoc:hasPhysicalProperty [
      a gssf:Fold_Amplitude ;
      gsoc:hasNumericValue "10" ;
      gsoc:hasUOM <http://loop3d.org/GSO/ontology/2019/1/uom/meter> ;
    ] ;
  gsoc:hasPhysicalProperty [
      a gssf:Fold_Profile_Type ;
    ] ;
  rdfs:comment "test fold properties" ;
  rdfs:label "is test fold" ;
.
gspr:Biological_Evolution
  rdfs:subClassOf gsoc:Process ;
.
gspr:Geomagnetic_Process
  rdfs:subClassOf gsog:Transformation ;
.
gspr:Human_Activity
  rdfs:subClassOf gsoc:Process ;
.
gspr:Material_Transport
  rdfs:subClassOf gsoc:Process ;
.
gspr:bolide_impact
  rdfs:subClassOf gsog:Geologic_Event ;
.
gspr:deep_water_oxygen_depletion
