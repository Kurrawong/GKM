# baseURI: http://geoscience.ca/3D/ontology/2019/1/ex-materialCb
# imports: http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact
# imports: http://geoscience.ca/3D/ontology/2019/1/ex-timescale
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://purl.org/dc/elements/1.1/
# prefix: bol

@prefix bol: <http://geoscience.ca/3D/ontology/2019/1/ex-materialCb#> .
@prefix common: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix con: <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tim: <http://geoscience.ca/3D/ontology/2019/1/ex-timescale#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://geoscience.ca/3D/ontology/2019/1/ex-materialCb>
  a owl:Ontology ;
  rdfs:comment "Example encoding of a lithology description for a formation" ;
  rdfs:comment "Stephen M. Richard 2019-05-03" ;
  rdfs:comment "description of rock material constituents of Bolsa Quartzite Formation" ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ex-timescale> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
.
bol:Bolsa_Quartzite_Material
  a geo:Rock_Material ;
  common:hasConstituent bol:Clay_matrix ;
  common:hasConstituent bol:Feldspar_grains ;
  common:hasConstituent bol:Quartz_grains ;
  common:isConstituentOf con:CbFormation ;
  geo:hasPhysicalProperty bol:constituent_part2 ;
  geo:hasPhysicalProperty bol:constituent_part3 ;
  geo:hasPhysicalProperty [
      a geo:Earth_Material_Color ;
      common:hasTermValue <http://colors.org/maroonbrown> ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Proportion ;
      geo:hasNumericValue 50 ;
      geo:hasUOM [
          a common:percent ;
        ] ;
      geo:proportionFor bol:Bolsa_Quarzite_Material ;
      geo:proportionOf bol:Feldspar_grains ;
    ] ;
  rdfs:comment "describe proportions of constituents, feldspar with blank node, quartz and matrix with label nodes to test variations"@en ;
  rdfs:label "Bolsa Quartzite, basal arkose"@en ;
.
bol:Clay_matrix
  a geo:Granular_Material ;
  common:hasConstituent geo:Clay_Mineral ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      geo:hasNumericValue .005 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
    ] ;
  rdfs:label "Clay matrix constituent"@en ;
.
bol:Feldspar_grains
  a geo:Granular_Material ;
  common:hasConstituent bol:Feldspar_mineral ;
  geo:hasPhysicalProperty [
      a geo:Grain_Shape ;
      geo:hasTermValue <http://geosciml.org/particleShape/subangular> ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      geo:hasNumericValue 5 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
      rdfs:label "5 mm maximum"@en ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Min ;
      geo:hasNumericValue .02 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
      rdfs:label "0.02 mm minimum"@en ;
    ] ;
.
bol:Feldspar_mineral
  a geo:Feldspar ;
  common:hasConstituent [
      a common:Calcium_Element ;
    ] ;
  common:hasConstituent [
      a common:Oxygen_Element ;
    ] ;
  common:hasConstituent [
      a common:Potassium_Element ;
    ] ;
  common:hasConstituent [
      a common:Silicon_Element ;
    ] ;
  common:hasConstituent [
      a common:Sodium_Element ;
    ] ;
  rdfs:label "specifc feldspar in this material" ;
.
bol:Quartz_grains
  a geo:Granular_Material ;
  common:hasConstituent bol:Quartz_mineral ;
  geo:hasPhysicalProperty [
      a geo:Grain_Shape ;
      geo:hasTermValue <http://geosciml.org/particleShape/subangular> ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Average ;
      common:hasAbstractProperty [
          a common:First_Standard_Deviation_Calculation ;
          geo:hasNumericValue .06 ;
          geo:hasUOM [
              a common:mm ;
            ] ;
        ] ;
      geo:hasNumericValue 3 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      geo:hasNumericValue 8 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
    ] ;
  geo:hasPhysicalProperty [
      a geo:Grain_Size_Min ;
      geo:hasNumericValue .05 ;
      geo:hasUOM [
          a common:mm ;
        ] ;
    ] ;
.
bol:Quartz_mineral
  a geo:Quartz ;
  common:hasConstituent [
      a common:Oxygen_Element ;
    ] ;
  common:hasConstituent [
      a common:Silicon_Element ;
    ] ;
  rdfs:comment "include instance of mineral to describe particular physical character and composition of mineral in this material" ;
  rdfs:label "specific quartz in this material" ;
.
bol:constituent_part2
  a geo:Proportion ;
  geo:hasNumericValue 45 ;
  geo:hasUOM [
      a common:percent ;
    ] ;
  geo:proportionFor bol:Bolsa_Quarzite_Material ;
  geo:proportionOf bol:Quartz_grains ;
.
bol:constituent_part3
  a geo:Proportion ;
  geo:hasNumericValue 5 ;
  geo:hasUOM [
      a common:percent ;
    ] ;
  geo:proportionFor bol:Bolsa_Quarzite_Material ;
  geo:proportionOf bol:Clay_matrix ;
.
