# baseURI: http://geoscience.ca/3D/ontology/2019/1/ex-materialCb
# imports: http://geoscience.ca/3D/ontology/2019/1/common
# imports: http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact
# imports: http://geoscience.ca/3D/ontology/2019/1/ex-timescale
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://purl.org/dc/elements/1.1/
# prefix: bol

@prefix bol: <http://geoscience.ca/3D/ontology/2019/1/ex-materialCb#> .
@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix con: <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact#> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tim: <http://geoscience.ca/3D/ontology/2019/1/ex-timescale#> .
@prefix unit:    <http://qudt.org/vocab/unit#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://geoscience.ca/3D/ontology/2019/1/ex-materialCb>
  a owl:Ontology ;
  rdfs:comment "Example encoding of a lithology description for a formation"@en ;
  rdfs:comment "Stephen M. Richard 2019-05-03" ;
  rdfs:comment "description of rock material constituents of Bolsa Quartzite Formation"@en ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ex-complexcontact> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ex-timescale> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
.
bol:Bolsa_Quartzite_Material
  a geo:Rock_Material ;
  com:hasConstituent bol:Clay_matrix ;
  com:hasConstituent bol:Feldspar_grains ;
  com:hasConstituent bol:Quartz_grains ;
  com:hasPhysicalProperty bol:constituent_part2 ;
  com:hasPhysicalProperty bol:constituent_part3 ;
  com:hasPhysicalProperty [
      a com:Proportion ;
      com:hasUOM unit:Percent ;
      com:hasValue 50 ;
      com:proportionFor bol:Bolsa_Quarzite_Material ;
      com:proportionOf bol:Feldspar_grains ;
      rdfs:label "proportion of feldspar grains"@en ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Rock_Material_Color ;
      com:hasValue <http://colors.org/maroonbrown> ;
    ] ;
  com:isConstituentOf con:CbFormation ;
  rdfs:comment "describe proportions of constituents, feldspar with blank node, quartz and matrix with label nodes to test variations"@en ;
  rdfs:label "Bolsa Quartzite, basal arkose"@en ;
.
bol:Clay_matrix
  a geo:Granular_Material ;
  com:hasConstituent geo:Clay_Mineral ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      com:hasUOM unit:Millimeter ;
      com:hasValue .005 ;
    ] ;
  rdfs:label "Clay matrix constituent"@en ;
.
bol:Feldspar_grains
  a geo:Granular_Material ;
  com:hasConstituent bol:Feldspar_mineral ;
  com:hasPhysicalProperty [
      a geo:Grain_Shape ;
      com:hasValue <http://geosciml.org/particleShape/subangular> ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      com:hasUOM unit:Millimeter ;
      com:hasValue 5 ;
      rdfs:label "5 mm maximum"@en ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Min ;
      com:hasUOM unit:Millimeter ;
      com:hasValue .02 ;
      rdfs:label "0.02 mm minimum"@en ;
    ] ;
.
bol:Feldspar_mineral
  a geo:Feldspar ;
  com:hasConstituent [
      a com:Calcium_Element ;
    ] ;
  com:hasConstituent [
      a com:Oxygen_Element ;
    ] ;
  com:hasConstituent [
      a com:Potassium_Element ;
    ] ;
  com:hasConstituent [
      a com:Silicon_Element ;
    ] ;
  com:hasConstituent [
      a com:Sodium_Element ;
    ] ;
  rdfs:label "specifc feldspar in this material" ;
.
bol:Quartz_grains
  a geo:Granular_Material ;
  com:hasConstituent bol:Quartz_mineral ;
  com:hasPhysicalProperty [
      a geo:Grain_Shape ;
      com:hasValue <http://geosciml.org/particleShape/subangular> ;
      rdfs:label "subangular" ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Average ;
      com:hasProperty [
          a com:First_Standard_Deviation_Calculation ;
          com:hasUOM unit:Millimeter ;
          com:hasValue .06 ;
        ] ;
      com:hasUOM unit:Millimeter ;
      com:hasValue 3 ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Max ;
      com:hasUOM unit:Millimeter ;
      com:hasValue 8 ;
    ] ;
  com:hasPhysicalProperty [
      a geo:Grain_Size_Min ;
      com:hasUOM unit:Millimeter ;
      com:hasValue .05 ;
    ] ;
.
bol:Quartz_mineral
  a geo:Quartz ;
  com:hasConstituent [
      a com:Oxygen_Element ;
    ] ;
  com:hasConstituent [
      a com:Silicon_Element ;
    ] ;
  rdfs:comment "include instance of mineral to describe particular physical character and composition of mineral in this material" ;
  rdfs:label "specific quartz in this material" ;
.
bol:constituent_part2
  a com:Proportion ;
  com:hasUOM unit:Percent ;
  com:hasValue 45 ;
  com:proportionFor bol:Bolsa_Quarzite_Material ;
  com:proportionOf bol:Quartz_grains ;
.
bol:constituent_part3
  a com:Proportion ;
  com:hasUOM unit:Percent ;
  com:hasValue 5 ;
  com:proportionFor bol:Bolsa_Quarzite_Material ;
  com:proportionOf bol:Clay_matrix ;
.
[
  rdfs:label "Jurassic deposition of Js Formation"@en ;
].
