# baseURI: http://loop3d.org/GKO/ontology/2019/1/faultexample2
# imports: http://loop3d.org/GKO/ontology/2019/1/eventenvironment
# imports: http://loop3d.org/GKO/ontology/2019/1/geologicfeature
# imports: http://loop3d.org/GKO/ontology/2019/1/geologicprocess
# imports: http://loop3d.org/GKO/ontology/2019/1/ischart

@prefix : <http://loop3d.org/GKO/ontology/2019/1/faultexample2#> .
@prefix gkoc: <http://loop3d.org/GKO/ontology/2019/1/common/> .
@prefix env: <http://loop3d.org/GKO/ontology/2019/1/eventenvironment/> .
@prefix fau2: <http://loop3d.org/GKO/ontology/2019/1/faultstest2/> .
@prefix gkog: <http://loop3d.org/GKO/ontology/2019/1/geologicfeature/> .
@prefix isca: <http://loop3d.org/GKO/ontology/2019/1/ischart/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pro: <http://loop3d.org/GKO/ontology/2019/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://loop3d.org/GKO/ontology/2019/1/faultexample2>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GKO/ontology/2019/1/loop3dmaster> ;
  owl:versionIRI <http://loop3d.org/GKO/ontology/2019/1/faultexample2> ;
.
fau2:Basement_Cover_Shear_Event
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:LaramideDuctileBasementInterval ;
  gkoc:isManifestationOf [
      a pro:ductile_flow ;
      rdfs:label "Shear along basement-cover contact"@en ;
    ] ;
  gkoc:isPartOf fau2:Laramide_Ductile_deformation ;
  gkog:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:comment "Strain localization at the boundary between Proterozoic crytalline rock and overlying Paleozoic sediments, apparently coeval with intrusion of two-mica granitoid (Wilderness Suite)" ;
  rdfs:label "Late Laramide Shear along basement-cover contact"@en ;
.
fau2:Basin_Fill_Tilting_Event
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:TertiaryExtensionBasinFillInterval ;
  gkoc:isManifestationOf [
      a pro:ductile_flow ;
      rdfs:label "Shear along basement-cover contact"@en ;
    ] ;
  gkog:hasSetting [
      a env:extended_terrane ;
    ] ;
  rdfs:comment "Strain localization at the boundary between Proterozoic crytalline rock and overlying Paleozoic sediments, apparently coeval with intrusion of two-mica granitoid (Wilderness Suite)" ;
  rdfs:label "Late Laramide Shear along basement-cover contact"@en ;
.
fau2:EarlyProterozoic1780-1690
  a gkog:Geologic_Age_Interval ;
  gkog:hasOlderAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ];
      gkog:hasDate "1780"^^xsd:decimal ;
      gkog:hasUncertainty "12"^^xsd:decimal ;
    ] ;
  gkog:hasYoungerAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ] ;
      gkog:hasDate "1690"^^xsd:decimal ;
      gkog:hasUncertainty "18"^^xsd:decimal ;
    ] ;
  rdfs:label "Early Proterozoic 1780-1690 Ma" ;
.
fau2:LaramideDuctileBasementInterval
  a gkog:Geologic_Age_Interval ;
  gkog:hasOlderAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ] ;
      gkog:hasDate "61"^^xsd:decimal ;
      gkog:hasUncertainty .04 ;
    ] ;
  gkog:hasYoungerAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ] ;
      gkog:hasDate "45"^^xsd:decimal ;
      gkog:hasUncertainty 1.0 ;
    ] ;
  rdfs:label "Laramide 61-45 Ma" ;
.
fau2:LaramideFaultingInterval
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:Campanian ;
  gkog:hasYoungerAge isca:Danian ;
  rdfs:label "Early Laramide" ;
.
fau2:Laramide_Ductile_deformation
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:LaramideDuctileBasementInterval ;
  gkoc:isManifestationOf [
      a pro:ductile_flow ;
      rdfs:label "Mid-crustal remobilization"@en ;
    ] ;
  gkog:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Late Laramide ductile flow in basement"@en ;
.
fau2:MesozoicDepositionInterval
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:MiddleTriassic ;
  gkog:hasYoungerAge isca:Campanian ;
  rdfs:label "Middle Triassic to Campanian" ;
.
fau2:MesozoicFaultingInterval
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:MiddleTriassic ;
  gkog:hasYoungerAge isca:LateJurassic ;
  rdfs:label "Middle Triassic to LateJurassic" ;
.
fau2:Mesozoic_Basin_Formation
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:MesozoicFaultingInterval ;
  gkoc:isManifestationOf [
      a pro:faulting ;
      rdfs:label "Intra or back arc basin formation"@en ;
    ] ;
  gkog:hasSetting [
      a env:back_arc_setting ;
      a env:extended_terrane ;
    ] ;
  rdfs:label "Mesozoic basin formation faulting"@en ;
.
fau2:Mesozoic_Sediment_Deposition
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:MesozoicDepositionInterval ;
  gkoc:isManifestationOf [
      a pro:deposition ;
      rdfs:label "Intra and back arc sediment accumulation"@en ;
    ] ;
  gkog:hasSetting [
      a env:alluvial_fan ;
      a env:alluvial_plain ;
      a env:floodplain ;
    ] ;
  rdfs:label "Mesozoic basin sediment deposition"@en ;
.
fau2:PaleozoicDepositionInterval
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:Cambrian ;
  gkog:hasYoungerAge isca:Permian ;
  rdfs:label "Paleozoic" ;
.
fau2:Paleozoic_Sediment_Deposition
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:PaleozoicDepositionInterval ;
  gkoc:isManifestationOf [
      a pro:deposition ;
      rdfs:label "Passive margin sediment accumulation"@en ;
    ] ;
  gkog:hasSetting env:passive_continental_margin_setting ;
  rdfs:label "Paleozoic passive margin sediment deposition"@en ;
.
fau2:Proterozoic_igneous_intrusion
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:EarlyProterozoic1780-1690 ;
  gkoc:isManifestationOf [
      a pro:intrusion ;
    ] ;
  gkog:hasSetting [
      a env:earth_interior_setting ;
    ] ;
  gkog:hasSetting [
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Early Proterozoic granitoid intrusion"@en ;
.
fau2:TertiaryExtensionBasinFillInterval
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:Chattian ;
  gkog:hasYoungerAge isca:Burdigalian ;
  rdfs:label "Mid-Tertiary extensional basin formation interval" ;
.
fau2:WildernessGraniteIntrusionInterval
  a gkog:Geologic_Age_Interval ;
  gkog:hasOlderAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ] ;
      gkog:hasDate "55"^^xsd:decimal ;
      gkog:hasUncertainty 0.3 ;
    ] ;
  gkog:hasYoungerAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [a unit:ma ] ;
      gkog:hasDate "42"^^xsd:decimal ;
      gkog:hasUncertainty 1.7 ;
    ] ;
  rdfs:label "Laramide 55-42 Ma" ;
.
fau2:Wilderness_Granite_Intrusion_Event
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty fau2:WildernessGraniteIntrusionInterval ;
  gkoc:isManifestationOf [
      a pro:intrusion ;
      rdfs:label "granitoid intrusion"@en ;
    ] ;
  gkoc:isPartOf fau2:Laramide_Ductile_deformation ;
  gkog:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Two-mica granite intrusion"@en ;
.
