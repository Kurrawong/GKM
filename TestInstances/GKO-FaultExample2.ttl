# baseURI: http://geoscience.ca/3D/ontology/2019/1/faultexample2
# imports: http://geoscience.ca/3D/ontology/2019/1/eventenvironment
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicprocess
# imports: http://geoscience.ca/3D/ontology/2019/1/ischart

@prefix : <http://geoscience.ca/3D/ontology/2019/1/faultexample2#> .
@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix env: <http://geoscience.ca/3D/ontology/2019/1/eventenvironment/> .
@prefix fau2: <http://geoscience.ca/3D/ontology/2019/1/faultstest2/> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix isca: <http://geoscience.ca/3D/ontology/2019/1/ischart/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pro: <http://geoscience.ca/3D/ontology/2019/1/geologicprocess/> .
@prefix qudt: <http://qudt.org/schema/qudt#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://geoscience.ca/3D/ontology/2019/1/faultexample2>
  a owl:Ontology ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/eventenvironment> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicprocess> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ischart> ;
  owl:versionIRI <http://geoscience.ca/3D/ontology/2019/1/faultexample2> ;
.
fau2:Basement_Cover_Shear_Event
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:LaramideDuctileBasementInterval ;
  com:isManifestationOf [
      a pro:ductile_flow_class ;
      rdfs:label "Shear along basement-cover contact"@en ;
    ] ;
  com:isPartOf fau2:Laramide_Ductile_deformation ;
  geo:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:comment "Strain localization at the boundary between Proterozoic crytalline rock and overlying Paleozoic sediments, apparently coeval with intrusion of two-mica granitoid (Wilderness Suite)" ;
  rdfs:label "Late Laramide Shear along basement-cover contact"@en ;
.
fau2:Basin_Fill_Tilting_Event
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:TertiaryExtensionBasinFillInterval ;
  com:isManifestationOf [
      a geo:Ductile_Deformation ;
      rdfs:label "Shear along basement-cover contact"@en ;
    ] ;
  geo:hasSetting [
      a env:extended_terrane ;
    ] ;
  rdfs:comment "Strain localization at the boundary between Proterozoic crytalline rock and overlying Paleozoic sediments, apparently coeval with intrusion of two-mica granitoid (Wilderness Suite)" ;
  rdfs:label "Late Laramide Shear along basement-cover contact"@en ;
.
fau2:EarlyProterozoic1780-1690
  a geo:Geologic_Age_Interval ;
  geo:hasOlderAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "1780"^^xsd:decimal ;
      geo:hasUncertainty "12"^^xsd:decimal ;
    ] ;
  geo:hasYoungerAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "1690"^^xsd:decimal ;
      geo:hasUncertainty "18"^^xsd:decimal ;
    ] ;
  rdfs:label "Early Proterozoic 1780-1690 Ma" ;
.
fau2:LaramideDuctileBasementInterval
  a geo:Geologic_Age_Interval ;
  geo:hasOlderAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "61"^^xsd:decimal ;
      geo:hasUncertainty .04 ;
    ] ;
  geo:hasYoungerAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "45"^^xsd:decimal ;
      geo:hasUncertainty 1.0 ;
    ] ;
  rdfs:label "Laramide 61-45 Ma" ;
.
fau2:LaramideFaultingInterval
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:Campanian ;
  geo:hasYoungerAge isca:Danian ;
  rdfs:label "Early Laramide" ;
.
fau2:Laramide_Ductile_deformation
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:LaramideDuctileBasementInterval ;
  com:isManifestationOf [
      a geo:Ductile_Deformation ;
      rdfs:label "Mid-crustal remobilization"@en ;
    ] ;
  geo:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Late Laramide ductile flow in basement"@en ;
.
fau2:MesozoicDepositionInterval
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:MiddleTriassic ;
  geo:hasYoungerAge isca:Campanian ;
  rdfs:label "Middle Triassic to Campanian" ;
.
fau2:MesozoicFaultingInterval
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:MiddleTriassic ;
  geo:hasYoungerAge isca:LateJurassic ;
  rdfs:label "Middle Triassic to LateJurassic" ;
.
fau2:Mesozoic_Basin_Formation
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:MesozoicFaultingInterval ;
  com:isManifestationOf [
      a geo:Faulting ;
      rdfs:label "Intra or back arc basin formation"@en ;
    ] ;
  geo:hasSetting [
      a env:back_arc_setting ;
      a env:extended_terrane ;
    ] ;
  rdfs:label "Mesozoic basin formation faulting"@en ;
.
fau2:Mesozoic_Sediment_Deposition
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:MesozoicDepositionInterval ;
  com:isManifestationOf [
      a geo:Deposition ;
      rdfs:label "Intra and back arc sediment accumulation"@en ;
    ] ;
  geo:hasSetting [
      a env:alluvial_fan ;
      a env:alluvial_plain ;
      a env:floodplain ;
    ] ;
  rdfs:label "Mesozoic basin sediment deposition"@en ;
.
fau2:PaleozoicDepositionInterval
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:Cambrian ;
  geo:hasYoungerAge isca:Permian ;
  rdfs:label "Paleozoic" ;
.
fau2:Paleozoic_Sediment_Deposition
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:PaleozoicDepositionInterval ;
  com:isManifestationOf [
      a geo:Deposition ;
      rdfs:label "Passive margin sediment accumulation"@en ;
    ] ;
  geo:hasSetting env:passive_continental_margin_setting ;
  rdfs:label "Paleozoic passive margin sediment deposition"@en ;
.
fau2:Proterozoic_igneous_intrusion
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:EarlyProterozoic1780-1690 ;
  com:isManifestationOf [
      a geo:Intrusion ;
    ] ;
  geo:hasSetting [
      a env:earth_interior_setting ;
    ] ;
  geo:hasSetting [
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Early Proterozoic granitoid intrusion"@en ;
.
fau2:TertiaryExtensionBasinFillInterval
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:Chattian ;
  geo:hasYoungerAge isca:Burdigalian ;
  rdfs:label "Mid-Tertiary extensional basin formation interval" ;
.
fau2:WildernessGraniteIntrusionInterval
  a geo:Geologic_Age_Interval ;
  geo:hasOlderAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "55"^^xsd:decimal ;
      geo:hasUncertainty 0.3 ;
    ] ;
  geo:hasYoungerAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "42"^^xsd:decimal ;
      geo:hasUncertainty 1.7 ;
    ] ;
  rdfs:label "Laramide 55-42 Ma" ;
.
fau2:Wilderness_Granite_Intrusion_Event
  a geo:Geologic_Event ;
  com:hasTemporalProperty fau2:WildernessGraniteIntrusionInterval ;
  com:isManifestationOf [
      a geo:Intrusion ;
      rdfs:label "granitoid intrusion"@en ;
    ] ;
  com:isPartOf fau2:Laramide_Ductile_deformation ;
  geo:hasSetting [
      a env:hinterland_setting ;
      a env:middle_continental_crustal_setting ;
    ] ;
  rdfs:label "Two-mica granite intrusion"@en ;
.
