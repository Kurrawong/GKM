con:downSideKd-3_1_2
  a gkog:Rock_Body_Boundary ;
.
con:downSideKd-3_1_3
  a gkog:Rock_Body_Boundary ;
.
con:downSideKd-3_1_4
  a gkog:Rock_Body_Boundary ;
  rdfs:comment "dike intrudes Js"@en ;
.
con:topJs-2
  a gkog:Rock_Body_Top ;
  gkoc:hasPart con:topJs-2_1 ;
  gkoc:isParticipantIn [
      a pro:erosion ;
    ] ;
.
