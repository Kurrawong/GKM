con:JsFormation
  a geo:Formation ;
  common:hasPart con:KdInJsDikeContact-3_1_4 ;
  common:hasPart con:KdInJsDikeContact-3_2_4 ;
  common:hasPart con:KgInJsIntrusiveContact-4_2 ;
  common:hasPart con:baseJs-6 ;
  common:hasPart con:topJs-2 ;
  geo:hasTemporalProperty [
      a geo:Chronostratigraphic_Age_Interval ;
      geo:hasOlderAge <http://resource.geosciml.org/classifier/ics/ischart/Jurassic> ;
      geo:hasYoungerAge <http://resource.geosciml.org/classifier/ics/ischart/Jurassic> ;
    ] ;
  rdfs:comment "details of unit description are in a separate Example: GKO-ExampleFormationJs.txt"@en ;
  rdfs:comment "the following parts are not elucidated in this example, but are referenced in the Contact instances below. These surfaces would participate in intrusion and also contact metamorphism processes"@en ;
  rdfs:label "Js Formation"@en ;
.
con:KdDike
  a geo:Formation ;
  common:hasConstituent [
      a geo:Diorite ;
      geo:hasPhysicalProperty [
          geo:hasNumericValue .05 ;
          geo:hasUOM [
              rdfs:type geo:mm ;
            ] ;
          rdfs:type geo:GrainSizeAverage ;
        ] ;
    ] ;
  common:hasPart con:KdUnderMs ;
  common:hasPart con:downSideKd-3_1 ;
  common:hasPart con:upSideKd-3_2 ;
  geo:Intrudes con:CbFormation ;
  geo:Intrudes con:JsFormation ;
  geo:Intrudes con:KgGranite ;
  geo:Intrudes con:XmRockBody ;
  rdfs:comment "dike has lots of intrusive contacts, on the hanging wall (up) and footwall (down) side of the dike, each contact is with a different unit."@en ;
  rdfs:comment "intrudes is subproperty of relatedTo"@en ;
  rdfs:label "Mafic Dike (Cretaceous)"@en ;
.
