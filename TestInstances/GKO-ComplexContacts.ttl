  gkoc:hostedBy con:KdInJsDikeContact-3_2_4 ;
  gkoc:hostedBy con:downSide-3_1_4 ;
  gkoc:hostedBy con:upSideKd-3_2_4 ;
  rdfs:label "relational feature, Kd dike intrudes Js formation"@en ;
.
con:contact_Kg_in_Js
  a gkog:Contact ;
  gkoc:hostedBy con:JsFormation ;
  gkoc:hostedBy con:KgGranite ;
  gkoc:hostedBy con:KgInJsIntrusiveContact-4_2 ;
  gkoc:hostedBy con:KgIntrusiveBoundary-4_2 ;
  rdfs:label "relational feature, Kg granite intrudes Js formation"@en ;
.
con:contact_Ms_on_Js
  a gkog:Contact ;
  gkoc:hostedBy con:JsFormation ;
  gkoc:hostedBy con:MsFormation ;
  gkoc:hostedBy con:baseMs-2_1 ;
  gkoc:hostedBy con:topJs-2_1 ;
  rdfs:label "relational feature, Ms formation overlies Js formation"@en ;
.
con:contact_Ms_on_Kg
  a gkog:Contact ;
  gkoc:hostedBy con:KgErosionSurface-2_3 ;
  gkoc:hostedBy con:KgGranite ;
  gkoc:hostedBy con:MsFormation ;
  gkoc:hostedBy con:baseMs-2_3 ;
  rdfs:label "relational feature, Ms formation overlies Kg Granite on erosion surface"@en ;
.
