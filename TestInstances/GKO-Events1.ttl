# baseURI: http://geoscience.ca/3D/ontology/2019/1/ex-gkoevent1
# imports: http://geoscience.ca/3D/ontology/2019/1/common
# imports: http://geoscience.ca/3D/ontology/2019/1/eventenvironment
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://geoscience.ca/3D/ontology/2019/1/ischart

@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix env: <http://geoscience.ca/3D/ontology/2019/1/eventenvironment/> .
@prefix evn1: <http://geoscience.ca/3D/ontology/2019/1/ex-gkoevent1/> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix gkm: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> .
@prefix isca: <http://geoscience.ca/3D/ontology/2019/1/ischart/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

com:hosts
  owl:inverseOf com:hostedBy ;
.
com:isPartOf
  a owl:TransitiveProperty ;
.
<http://geoscience.ca/3D/ontology/2019/1/ex-gkoevent1>
  a owl:Ontology ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/eventenvironment> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/ischart> ;
  owl:versionIRI <http://geoscience.ca/3D/ontology/2019/1/ex-gkoevent1> ;
.
evn1:Cretaceous90Ma
  a geo:Geologic_Age_Instant ;
  geo:hasAgeDate [
      a geo:Geologic_Time_Date ;
      com:hasUOM com:ma ;
      geo:hasDate "90"^^xsd:decimal ;
      geo:hasUncertainty "8"^^xsd:decimal ;
    ] ;
  rdfs:label "90 Ma" ;
.
evn1:Cretaceous_dike_intrusion
  a geo:Geologic_Event ;
  com:hasTemporalProperty evn1:Cretaceous90Ma ;
  com:isManifestationOf [
      a geo:Intrusion ;
    ] ;
  geo:hasSetting [
      a env:upper_continental_crustal_setting ;
    ] ;
  geo:younger_than evn1:Kg_Intrusion ;
  rdfs:label "90 Ma Dike Intrusion"@en ;
.
evn1:Date110Ma
  a geo:Geologic_Time_Date ;
  com:hasUOM com:ma ;
  geo:hasDate "110"^^xsd:decimal ;
  geo:hasUncertainty "9"^^xsd:decimal ;
  rdfs:label "110 Ma" ;
.
evn1:HsGenesis
  a geo:Geologic_Event ;
  com:hasTemporalProperty [
      a geo:Chronostratigraphic_Age_Interval ;
      geo:hasOlderAge isca:Holocene ;
      geo:hasYoungerAge isca:Holocene ;
      rdfs:label "Holocene age" ;
    ] ;
  com:isManifestationOf [
      a geo:Deposition ;
      rdfs:label "process of sedimentary deposition"@en ;
    ] ;
  geo:hasSetting [
      a env:alluvial_fan ;
    ] ;
  geo:younger_than evn1:MsGenesis ;
  rdfs:comment "Deposition of Holocene sediments"@en ;
  rdfs:label "Holocene sediment deposition"@en ;
.
evn1:JsGenesis
  a geo:Geologic_Event ;
  com:hasTemporalProperty [
      a geo:Chronostratigraphic_Age_Interval ;
      geo:hasOlderAge isca:Jurassic ;
      geo:hasYoungerAge isca:Jurassic ;
      rdfs:label "Jurassic Age" ;
    ] ;
  com:isManifestationOf [
      a geo:Deposition ;
    ] ;
  geo:hasSetting [
      a env:coastal_plain_setting ;
    ] ;
  geo:hasSetting [
      a env:inner_neritic ;
    ] ;
  geo:hasSetting [
      a env:shoreline_settings ;
    ] ;
  rdfs:label "Jurassic sediment Deposition"@en ;
.
evn1:Kg_Intrusion
  a geo:Geologic_Event ;
  com:hasTemporalProperty [
      a geo:Geologic_Age_Instant ;
      com:determinedBy evn1:upbconcordantanalysis ;
      geo:hasAgeDate evn1:Date110Ma ;
      rdfs:label "Cretaceous 110 Ma Age Date"@en ;
    ] ;
  com:isManifestationOf [
      a geo:Intrusion ;
    ] ;
  geo:hasSetting [
      a env:middle_continental_crust_setting ;
    ] ;
  geo:younger_than evn1:JsGenesis ;
  rdfs:label "Cretaceous Intrusion Event"@en ;
.
evn1:Miocene_Age
  a geo:Chronostratigraphic_Age_Interval ;
  geo:hasOlderAge isca:Miocene ;
  geo:hasYoungerAge isca:Miocene ;
  rdfs:label "Miocene Age"@en ;
.
evn1:MsGenesis
  a geo:Geologic_Event ;
  com:hasTemporalProperty [
      a geo:Chronostratigraphic_Age_Interval ;
      geo:hasOlderAge isca:Miocene ;
      geo:hasYoungerAge isca:Miocene ;
      rdfs:label "Miocene Age"@en ;
    ] ;
  com:isManifestationOf [
      a geo:Deposition ;
    ] ;
  geo:hasSetting [
      a env:lacustrine_setting ;
    ] ;
  geo:younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:label "Miocene sediment Deposition"@en ;
.
evn1:Pre_Cretaceous_Burial
  a geo:Geologic_Event ;
  rdfs:comment "Burial of Js to depth of Kg intrusion, must predate Kg "@en ;
  rdfs:label "Pre Kg burial "@en ;
.
evn1:Pre_Jurassic_Erosion
  a geo:Geologic_Event ;
  rdfs:comment "Erosional unroofing of tilted Xm, Cb, Os and Ss "@en ;
  rdfs:label "Pre Jurassic Erosion "@en ;
.
evn1:Pre_Miocene_Erosion
  a geo:Geologic_Event ;
  geo:older_than evn1:MsGenesis ;
  geo:younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:comment "Erosional unroofing of Kg and Js "@en ;
  rdfs:label "Pre Miocene Erosion "@en ;
.
evn1:upbconcordantanalysis
  a com:Determining_Event ;
  rdfs:label "U-Pb isotopic analysis, concordant"@en ;
.
geo:older_than
  a owl:TransitiveProperty ;
.
geo:younger_than
  a owl:TransitiveProperty ;
.
