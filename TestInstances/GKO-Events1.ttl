# baseURI: http://loop3d.org/GKO/ontology/2019/1/ex-gkoevent1
# imports: http://loop3d.org/GKO/ontology/2019/1/common
# imports: http://loop3d.org/GKO/ontology/2019/1/eventenvironment
# imports: http://loop3d.org/GKO/ontology/2019/1/geologicfeature
# imports: http://loop3d.org/GKO/ontology/2019/1/ischart

@prefix evn1: <http://loop3d.org/GKO/ontology/2019/1/ex-gkoevent1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix env: <http://loop3d.org/GKO/ontology/2019/1/eventenvironment> .
@prefix gkoc: <http://loop3d.org/GKO/ontology/2019/1/common/> .
@prefix gkog: <http://loop3d.org/GKO/ontology/2019/1/geologicfeature/> .
@prefix gkop: <http://loop3d.org/GKO/ontology/2019/1/geologicproperty/> .
@prefix isca: <http://loop3d.org/GKO/ontology/2019/1/ischart/> .
@prefix lith: <http://loop3d.org/GKO/ontology/2019/1/lithology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pro: <http://loop3d.org/GKO/ontology/2019/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix unit: <http://loop3d.org/GKO/ontology/2019/1/uom/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://loop3d.org/GKO/ontology/2019/1/ex-gkoevent1>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GKO/ontology/2019/1/loop3dmaster> ;
  owl:versionIRI <http://loop3d.org/GKO/ontology/2019/1/ex-gkoevent1> ;
.
evn1:Cretaceous90Ma
  a gkog:Geologic_Age_Instant ;
  gkog:hasAgeDate [
      a gkog:Geologic_Time_Date ;
      gkoc:hasUOM [ a unit:ma ] ;
      gkog:hasDate "90"^^xsd:decimal ;
      gkog:hasUncertainty "8"^^xsd:decimal ;
    ] ;
  rdfs:label "90 Ma" ;
.
evn1:Cretaceous_dike_intrusion
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty evn1:Cretaceous90Ma ;
  gkoc:isManifestationOf [
      a pro:intrusion ;
    ] ;
  gkog:hasSetting [
      a env:upper_continental_crustal_setting ;
    ] ;
  gkoc:age_younger_than evn1:Kg_Intrusion ;
  rdfs:label "90 Ma Dike Intrusion"@en ;
.
evn1:Date110Ma
  a gkog:Geologic_Time_Date ;
  gkoc:hasUOM [a unit:ma ] ;
  gkog:hasDate "110"^^xsd:decimal ;
  gkog:hasUncertainty "9"^^xsd:decimal ;
  rdfs:label "110 Ma" ;
.
evn1:HsGenesis
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty [
      a gkog:Chronostratigraphic_Age_Interval ;
      gkog:hasOlderAge isca:Holocene ;
      gkog:hasYoungerAge isca:Holocene ;
      rdfs:label "Holocene age" ;
    ] ;
  gkoc:isManifestationOf [
      a pro:deposition ;
      rdfs:label "process of sedimentary deposition"@en ;
    ] ;
  gkog:hasSetting [
      a env:alluvial_fan ;
    ] ;
  gkoc:age_younger_than evn1:MsGenesis ;
  rdfs:comment "Deposition of Holocene sediments"@en ;
  rdfs:label "Holocene sediment deposition"@en ;
.
evn1:JsGenesis
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty [
      a gkog:Chronostratigraphic_Age_Interval ;
      gkog:hasOlderAge isca:Jurassic ;
      gkog:hasYoungerAge isca:Jurassic ;
      rdfs:label "Jurassic Age" ;
    ] ;
  gkoc:isManifestationOf [
      a pro:deposition ;
    ] ;
  gkog:hasSetting [
      a env:coastal_plain_setting ;
    ] ;
  gkog:hasSetting [
      a env:inner_neritic ;
    ] ;
  gkog:hasSetting [
      a env:shoreline_settings ;
    ] ;
  rdfs:label "Jurassic sediment Deposition"@en ;
.
evn1:Kg_Intrusion
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty [
      a gkog:Geologic_Age_Instant ;
      gkoc:determinedBy evn1:upbconcordantanalysis ;
      gkog:hasAgeDate evn1:Date110Ma ;
      rdfs:label "Cretaceous 110 Ma Age Date"@en ;
    ] ;
  gkoc:isManifestationOf [
      a pro:intrusion ;
    ] ;
  gkog:hasSetting [
      a env:middle_continental_crust_setting ;
    ] ;
  gkoc:age_younger_than evn1:JsGenesis ;
  rdfs:label "Cretaceous Intrusion Event"@en ;
.
evn1:Miocene_Age
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge isca:Miocene ;
  gkog:hasYoungerAge isca:Miocene ;
  rdfs:label "Miocene Age"@en ;
.
evn1:MsGenesis
  a gkog:Geologic_Event ;
  gkoc:hasTemporalProperty [
      a gkog:Chronostratigraphic_Age_Interval ;
      gkog:hasOlderAge isca:Miocene ;
      gkog:hasYoungerAge isca:Miocene ;
      rdfs:label "Miocene Age"@en ;
    ] ;
  gkoc:isManifestationOf [
      a pro:deposition ;
    ] ;
  gkog:hasSetting [
      a env:lacustrine_setting ;
    ] ;
  gkoc:age_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:label "Miocene sediment Deposition"@en ;
.
evn1:Pre_Cretaceous_Burial
  a gkog:Geologic_Event ;
  rdfs:comment "Burial of Js to depth of Kg intrusion, must predate Kg "@en ;
  rdfs:label "Pre Kg burial "@en ;
.
evn1:Pre_Jurassic_Erosion
  a gkog:Geologic_Event ;
  rdfs:comment "Erosional unroofing of tilted Xm, Cb, Os and Ss "@en ;
  rdfs:label "Pre Jurassic Erosion "@en ;
.
evn1:Pre_Miocene_Erosion
  a gkog:Geologic_Event ;
  gkoc:age_older_than evn1:MsGenesis ;
  gkoc:age_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:comment "Erosional unroofing of Kg and Js "@en ;
  rdfs:label "Pre Miocene Erosion "@en ;
.
evn1:upbconcordantanalysis
  a gkoc:Determining_Event ;
  rdfs:label "U-Pb isotopic analysis, concordant"@en ;
.


