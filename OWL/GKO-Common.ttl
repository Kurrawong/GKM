      a owl:Restriction ;
      owl:allValuesFrom :Temporal_Property ;
      owl:onProperty :hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :isPropertyOf ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Endurant ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Perdurant ;
                  rdf:rest [
                      a rdf:List ;
                      rdf:first :Temporal_Property ;
                      rdf:rest () ;
                    ] ;
                ] ;
            ] ;
        ] ;
    ] ;
