      owl:onProperty :hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Perdurant ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Physical_Endurant ;
