com:Spatial_Location
  a owl:Class ;
  rdfs:comment "A physical property that is quantified by a category from a classification scheme." ;
  rdfs:label "Spatial Location" ;
  rdfs:subClassOf com:Discrete_Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:hasValue ;
      owl:someValuesFrom com:Spatial_Region ;
    ] ;
.
com:Spatial_Region
