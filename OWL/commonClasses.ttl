com:Statistical_Calculation
  a owl:Class ;
  rdfs:comment "Determination of statistics associated with a measured value, e.g. minimum, maximum, mean, mode, standard deviation."@en ;
  rdfs:label "Statistical Calculation"@en ;
  rdfs:subClassOf com:Calculation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:hasUOM ;
      owl:someValuesFrom com:uom ;
    ] ;
.
com:Temporal_Property
  a owl:Class ;
  rdfs:comment "Temporal Property" ;
  rdfs:label "Temporal Property" ;
  rdfs:subClassOf com:Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom com:Temporal_Property ;
      owl:onProperty com:hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:isPropertyOf ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              com:Endurant
              com:Perdurant
              com:Temporal_Property
            ) ;
        ] ;
    ] ;
.
com:Temporal_Region
