        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Endurant"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/hasSpatialLocation -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/hasSpatialLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
