        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/haseventhistory -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/haseventhistory">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
