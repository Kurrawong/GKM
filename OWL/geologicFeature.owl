    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bed">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/LIthostratigraphic_Unit"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/Morphologic_Feature"/>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Structure"/>
