        <owl:imports rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common"/>
        <rdfs:comment xml:lang="en">Base geologic feature classes and high level constraints and relationships</rdfs:comment>
    </owl:Ontology>
    


