     -->

    


    <!-- http://geoscience.ca/3D/ontology/2019/1/hasHost -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/hasHost">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicFeature"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/hasSpatialExtent -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/hasSpatialExtent">
