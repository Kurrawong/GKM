    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Boundary_Stratotype -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Boundary_Stratotype">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Stratotype"/>
        <rdfs:comment xml:lang="en">A stratigraphic section that contains a designated point in a stratigraphic sequence of essentially continuous deposition, preferably marine, chosen for its correlation potential. http://www.stratigraphy.org/upload/bak/chron.htm</rdfs:comment>
        <rdfs:label xml:lang="en">Boundary Stratotype</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Brittle_Deformation -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Brittle_Deformation">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation"/>
        <rdfs:label xml:lang="en">Brittle Deformation</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Categorical_Physical_Geologic_Property -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Categorical_Physical_Geologic_Property">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/Categorical_Value"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Age -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Age">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Age"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Unit -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Unit">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Unit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/realizes"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Time_Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A body of rocks that includes all rocks, layered or unlayered, formed during a specific interval of geologic time, and only those rocks formed during that time span. Chronostratigraphic units are bounded by synchronous horizons.

The rank and relative magnitude of the units in the chronostratigraphic hierarchy are a function of the length of the time interval that their rocks subtend, rather than of their physical thickness.

(http://www.stratigraphy.org/upload/bak/chron.htm)</rdfs:comment>
        <rdfs:label xml:lang="en">Chronostratigraphic unit</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clast -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clast">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Rock_Body_Part"/>
        <rdfs:label xml:lang="en">Clast</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clastic_Granular_Material -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clastic_Granular_Material">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Granular_Material"/>
        <rdfs:comment xml:lang="en">Use for clastic sedimentary components --conglomerate clast types, sand components, clay matrix. 
Also for deformation-producted clasts in tectonic breccia.</rdfs:comment>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex_Physical_Geologic_Property -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex_Physical_Geologic_Property">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Structure"/>
        <rdfs:comment>Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies.
A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) &lt;--&gt; exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]</rdfs:comment>
