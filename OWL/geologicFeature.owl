        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/hasParticipant -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/hasParticipant">
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/participantIn -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/participantIn">
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/relatedto -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/relatedto">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasConstituent -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasConstituent">
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Rock_Material"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Feature"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderHost -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderHost">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialExtent -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialExtent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialLocation -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:comment xml:lang="en">Geographic disposition of an endurant.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUOM -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUOM">
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/uom"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/haseventhistory -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/haseventhistory">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Event"/>
        <rdfs:label xml:lang="en">hasEventHistory</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasmethod -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasmethod">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Property"/>
        <rdfs:label xml:lang="en">hasMethod</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hassetting -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hassetting">
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Setting"/>
        <rdfs:label xml:lang="en">hasSetting</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isRealizedBy -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isRealizedBy"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/ispropertyof -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/ispropertyof">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Property"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
        <rdfs:label xml:lang="en">isPropertyOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionFor -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionFor">
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionOf -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue"/>
        <rdfs:comment xml:lang="en">numeric value for a time coordinate. Associated hasUOM property asserts the units of measure; the temporal reference system used to assert dates should be explained in metadata for the data instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue"/>
        <rdfs:comment xml:lang="en">numeric assertion of the one-side confidence interval for a measured value. The statistic used to assert this uncertainty interval should be explained in the metadata for instances using this property.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue">
        <rdfs:label xml:lang="en">has numeric value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/Surface -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/Surface">
        <rdfs:label xml:lang="en">Surface</rdfs:label>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/uom -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/uom">
        <rdfs:comment xml:lang="en">PRobably need to specialize for units of length, mass, time, etc.  Use QUDT unit types? (http://www.qudt.org/doc/2017/DOC_SCHEMA-QUDT-SCIENCE-v2.0.html)</rdfs:comment>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Age_Rank -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Age_Rank">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Time_Interval"/>
        <rdfs:comment xml:lang="en">most granular geochronolgoic time interval; one or more Ages are grouped into an Epoch</rdfs:comment>
        <rdfs:label xml:lang="en">Age (time scale rank)</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Amount -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Amount">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Numeric_Physical_Geologic_Property"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Structure"/>
        <rdfs:label xml:lang="en">Bedding</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Brittle_Deformation -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Brittle_Deformation">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation"/>
        <rdfs:label xml:lang="en">Brittle Deformation</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Categorical_Physical_Geologic_Property -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Categorical_Physical_Geologic_Property">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/Categorical_Value"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clast -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clast">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Rock_Body_Part"/>
        <rdfs:label xml:lang="en">Clast</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex_Physical_Geologic_Property -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex_Physical_Geologic_Property">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Physical_Geologic_Property"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Structure"/>
        <rdfs:comment>Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies.
A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) &lt;--&gt; exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]</rdfs:comment>
        <rdfs:label xml:lang="en">Contact</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact_Metamorphism -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact_Metamorphism">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Metamorphism"/>
        <rdfs:label xml:lang="en">Contact Metamorphism</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deposition -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deposition">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex_Physical_Geologic_Property"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Deformation -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Deformation">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation"/>
        <rdfs:label xml:lang="en">Ductile Deformation</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Shear_Zone -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Shear_Zone">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/participantIn"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Deformation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Structure"/>
        <rdfs:label xml:lang="en">Ductile Shear Zone</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Eon -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Eon">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Era"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Time_Interval"/>
        <rdfs:comment xml:lang="en">How will this be defined?  Must contain at least one Era and have duration longer than some Era?</rdfs:comment>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Epoch -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Epoch">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Age_Rank"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Time_Interval"/>
        <rdfs:comment xml:lang="en">An Epoch may be subdivided into Ages. Epochs are grouped into Periods. Example epochs: Eocene, Pleistocene</rdfs:comment>
        <rdfs:label xml:lang="en">Epoch</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Era -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Era">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Period"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Time_Interval"/>
        <rdfs:comment xml:lang="en">Named rank for geochronologic time interval. An Era may be subdivided into Periods. Eras are grouped into Eons</rdfs:comment>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Facies -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Facies">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Rock_Body_Part"/>
        <rdfs:label xml:lang="en">Facies</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fault -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fault">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fracture"/>
        <rdfs:label xml:lang="en">Fault</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Faulting -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Faulting">
