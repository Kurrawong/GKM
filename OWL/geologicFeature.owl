    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Porespace">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/RockBodyVoid"/>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/RockBody -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/RockBody">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicEndurant"/>
        <rdfs:comment xml:lang="en">An endurant in which some Rock Material inheres, either as an &apos;amount of matter&apos; (disembodied matter), or as a geologic unit.</rdfs:comment>
