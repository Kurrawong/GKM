        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Endurant"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue"/>
        <rdfs:comment xml:lang="en">numeric value for a time coordinate. Associated hasUOM property asserts the units of measure; the temporal reference system used to assert dates should be explained in metadata for the data instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty">
        <rdfs:subPropertyOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue"/>
        <rdfs:comment xml:lang="en">numeric assertion of the one-side confidence interval for a measured value. The statistic used to assert this uncertainty interval should be explained in the metadata for instances using this property.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue -->

    <owl:DatatypeProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasValue">
        <rdfs:label xml:lang="en">has numeric value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Shape -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Shape">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/abstractvalue"/>
        <rdfs:label xml:lang="en">any shape</rdfs:label>
    </owl:Class>

    <!-- http://geoscience.ca/3D/ontology/2019/1/common/subangular -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Subangular">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Shape"/>
    </owl:Class>
	


    <!-- http://geoscience.ca/3D/ontology/2019/1/Bedding -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Bedding">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:label xml:lang="en">Bedding</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Contact -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Contact">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:comment>Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies.
A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) &lt;--&gt; exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]</rdfs:comment>
        <rdfs:label xml:lang="en">Contact</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/DuctileShearZone -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/DuctileShearZone">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/participantIn"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Deformation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:label xml:lang="en">Ductile Shear Zone</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Eon -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Eon">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Era"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geochronologic_Time_Interval"/>
        <rdfs:comment xml:lang="en">How will this be defined?  Must contain at least one Era and have duration longer than some Era?</rdfs:comment>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Fault -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Fault">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Fracture"/>
        <rdfs:label xml:lang="en">Fault</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Fold -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Fold">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:label xml:lang="en">Fold</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Foliation -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Foliation">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Dissective_Morphologic_Feature"/>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:label xml:lang="en">Foliation</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Fracture -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Fracture">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/participantIn"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Brittle_Deformation"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
        <rdfs:label xml:lang="en">Fracture</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GSSA -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GSSA">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Time_Date"/>
        <rdfs:comment xml:lang="en">A Geochronologic Age defined by the International Stratigraphic Commission, based on fiat selection of defined older and younger bounding time coordinates</rdfs:comment>
        <rdfs:label xml:lang="en">GSSA</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Geochronologic_Time_Interval -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Geochronologic_Time_Interval">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Time_Interval"/>
        <rdfs:label xml:lang="en">Geochronologic Time Interval</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GeologicEvent -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GeologicEvent">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Perdurant"/>
        <rdfs:comment xml:lang="en">If an event is caused by a process, then any restrictions on participants of the process also apply to participants of the event

causedBy (x,y) ^ GeologicEvent (x) ^ GeologicProcess (y) --&gt; forall z  [participates(z,x) --&gt; participates (z,y)]</rdfs:comment>
        <rdfs:label xml:lang="en">Geologic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GeologicFeature -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GeologicFeature">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/hasHost"/>
                <owl:someValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/RockBody"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Endurant"/>
        <rdfs:comment xml:lang="en">Geologic Feature (GF):
Geologic features that are specifially dependent on their host -  that is, the host relation is a subrelation of specific dependence.

GF(x) &lt;--&gt; exists y [hasHost(y,x)] and geologic(x)</rdfs:comment>
        <rdfs:label xml:lang="en">Geologic Feature</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GeologicProcess -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GeologicProcess">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/Geologic_Perdurant"/>
        <rdfs:label xml:lang="en">Geologic Process</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GeologicStructure -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/hasHost"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/RockBody"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/isPartOf"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicStructure"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/GeologicFeature"/>
        <rdfs:label xml:lang="en">Geologic Structure</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/GeologicUnit -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/GeologicUnit">
        <rdfs:subClassOf rdf:resource="http://geoscience.ca/3D/ontology/2019/1/RockBody"/>
        <rdfs:comment xml:lang="en">A geologic unit and (the amount of) its rock material are co-located, but different entities. They can share properties, e.g. porosity, thickness, density, but also have differences:

- different persistence conditions: the material can exist without the unit; e.g. a unit can be cease to exist due to natural (seismic) or artificial (explosion) means, but the material can persist
- different parthood conditions:  
-- a unit can be part of a stratigraphic lexicon, but its material cannot be part of the lexicon. 
-- parts of the unit are not parts of the material, e.g. the western arm of formation X is not a part of its sandstone - the sandstone making up the western part of X is a part of the sandstone 
-- different identity conditions: a unit can have some material added or removed and retain identity, but the material changes identity with the addition or removal of some material</rdfs:comment>
        <rdfs:label xml:lang="en">Geologic Unit</rdfs:label>
    </owl:Class>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/Geologic_Endurant -->

    <owl:Class rdf:about="http://geoscience.ca/3D/ontology/2019/1/Geologic_Endurant">
        <rdfs:comment xml:lang="en">A Physical Endurant that is part of the Earth.</rdfs:comment>
