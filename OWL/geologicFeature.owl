    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUOM -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUOM">
        <rdfs:range rdf:resource="http://geoscience.ca/3D/ontology/2019/1/common/uom"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost">
