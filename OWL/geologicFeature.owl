    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderDate -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderDate">
