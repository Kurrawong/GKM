    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/hasConstituent -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/hasConstituent">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
                    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/hasPart -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/hasPart">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
    </owl:ObjectProperty>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/hasParticipant -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/hasParticipant">
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Perdurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/participantIn -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/participantIn">
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/common/relatedto -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/common/relatedto">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Endurant"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement"/>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost -->

    <rdf:Description rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasHost">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Feature"/>
    </rdf:Description>
    


    <!-- http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasMaterialConstituent -->

    <owl:ObjectProperty rdf:about="http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasMaterialConstituent">
