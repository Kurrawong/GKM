  rdfs:subClassOf :Rock_Body_Part ;
.
:ParticleShape
  a owl:Class ;
  rdfs:label "Any Particle Shape"@en ;
  rdfs:subClassOf gkoc:Shape ;
.
:Pendant
  a owl:Class ;
  rdfs:comment "a mass of country rock that is entirely surrounded by an igneous intrusion such as a batholith or other pluton. "@en ;
  rdfs:label "Pendant "@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Period
  a owl:Class ;
  rdfs:comment "Named rank for geochronologic time interval. Periods may be subdivided into Epochs. Periods are grouped into an Era."@en ;
  rdfs:subClassOf :Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Epoch ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass :Era ;
      owl:onProperty gkoc:isPartOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
:Porespace
  a owl:Class ;
  rdfs:subClassOf :Rock_Body_Void ;
.
:Pyroclastic_Granular_Material
  a owl:Class ;
  rdfs:comment "Use for description of pyroclastic rocks"@en ;
  rdfs:subClassOf :Granular_Material ;
.
:Rock_Body
  a owl:Class ;
  rdfs:comment "An endurant in which some Rock Material inheres, either as an 'amount of matter' (disembodied matter), or as a geologic unit."@en ;
  rdfs:label "Rock Body"@en ;
  rdfs:subClassOf gkoc:Physical_Endurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gkoc:Amount_Of_Matter ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gkoc:Physical_Feature ;
      owl:onProperty gkoc:hosts ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Geologic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasPart ;
    ] ;
.
:Rock_Body_Bottom
  a owl:Class ;
  rdfs:comment """Rock Body Bottom (RBB):
The bottom surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [isPartOf (y,z) ^ hasHost (z,x) -> above (y,x) v isPartOf (y,x)]"""@en ;
  rdfs:label "Rock Body Bottom"@en ;
  rdfs:subClassOf :Rock_Body_Boundary ;
.
:Rock_Body_Boundary
  a owl:Class ;
  rdfs:comment """a physical boundary hosted by a rock body.
RBD (x) <--> SF (x) ^ exists y [RB (y) ^ hosts (y,x)] 
The exterior-facing material of a rock body."""@en ;
  rdfs:label "Rock Body Boundary"@en ;
  rdfs:subClassOf gkoc:Physical_Boundary ;
  rdfs:subClassOf :Rock_Body_Part ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Body_Boundary ;
      owl:onProperty gkoc:hasPart ;
    ] ;
.
:Rock_Body_Part
  a owl:Class ;
  rdfs:label "Rock Body Part"@en ;
  rdfs:subClassOf :Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Body_Part ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Material ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:isPartOf ;
      owl:someValuesFrom :Rock_Body ;
    ] ;
.
:Rock_Body_Top
  a owl:Class ;
  rdfs:comment """Rock Body Top (RBT):
The top surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [(isPartOf (y,z) ^ hasHost (z,x)) -> (below (y,x) v isPartOf (y,x))]"""@en ;
  rdfs:label "Rock Body Top"@en ;
  rdfs:subClassOf :Rock_Body_Boundary ;
.
:Rock_Body_Void
  a owl:Class ;
  rdfs:label "Rock Body Void"@en ;
  rdfs:subClassOf gkoc:Physical_Void ;
.
:Rock_Material
  a owl:Class ;
  rdfs:comment "hook into CGI simple lithology ontology. SMR has one, and Clinton Smyth has an extension of that with more rock types."@en ;
  rdfs:label "Rock Material"@en ;
  rdfs:subClassOf gkoc:Amount_Of_Matter ;
  rdfs:subClassOf :Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Body_Part ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gkoc:Element ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Glass ;
                  rdf:rest [
                      a rdf:List ;
                      rdf:first :Granular_Material ;
                      rdf:rest [
                          a rdf:List ;
                          rdf:first :Mineral ;
                          rdf:rest () ;
                        ] ;
                    ] ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Geologic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass :Rock_Body ;
      owl:onProperty gkoc:isPartOf ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
.
:Series
  a owl:Class ;
  rdfs:subClassOf :Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :realizes ;
      owl:someValuesFrom :Epoch ;
    ] ;
.
:Specimen
  a owl:Class ;
  rdfs:label "Rock Material Specimen"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Stage
  a owl:Class ;
  rdfs:comment "The stage includes all rocks formed during an age. A stage is normally the lowest ranking unit in the chronostratigraphic hierarchy that can be recognized on a global scale. The stage has been called the basic working unit of chronostratigraphy because it is suited in scope and rank to the practical needs and purposes of intraregional chronostratigraphic classification. It is a subdivision of a series."@en ;
  rdfs:subClassOf :Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:isPartOf ;
      owl:someValuesFrom :Series ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :realizes ;
      owl:someValuesFrom :Age_Rank ;
    ] ;
.
:Stratigraphic_Point
  a owl:Class ;
  rdfs:comment "A spatially restricted part of a Contact feature, typically located by a point location. Could be thought of as an instance of a Contact, or a sample of a Contact."@en ;
  rdfs:label "Stratigraphic Point"@en ;
  rdfs:subClassOf :Contact ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasReference ;
      owl:someValuesFrom :Stratotype ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasSpatialProperty ;
      owl:someValuesFrom gkoc:Spatial_Extent ;
    ] ;
.
:Stratigraphic_Section
  a owl:Class ;
  rdfs:label "Stratigraphic Section"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
