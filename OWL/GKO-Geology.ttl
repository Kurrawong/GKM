@prefix : <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gkoc: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://geoscience.ca/3D/ontology/2019/1/geologicfeature> a owl:Ontology ;
	owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ;
	rdfs:comment "Base geologic feature classes, constraints, and relationships"@en .
# 
# 
# #################################################################
# #
# #    Annotation properties
# #
# #################################################################
# 
# 
# http://www.w3.org/2002/07/owl#maxQualifiedCardinality

owl:maxQualifiedCardinality a owl:AnnotationProperty .
# 
# http://www.w3.org/2002/07/owl#qualifiedCardinality

owl:qualifiedCardinality a owl:AnnotationProperty .
# 
# 
# 
# #################################################################
# #
# #    Object Properties
# #
# #################################################################
# 
# 
# http://geoscience.ca/3D/ontology/2019/1/common/HostedBy

gkoc:HostedBy a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/containedBy

:containedBy a owl:ObjectProperty ;
	rdfs:comment "Links a stratigraphic point to a stratigraphic section or rock body" ;
	rdfs:label "contained by" .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty

:hasAbstractProperty a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasBoundaryThickness

:hasBoundaryThickness a owl:ObjectProperty ;
	rdfs:domain :Gradational_Contact_Zone ;
	rdfs:range :Thickness .

:Thickness a owl:Class .

:hasBoundaryThickness rdfs:comment "thickness (length) of boundary zone at a gradational contact "@en ;
	rdfs:label "has boundary thickness "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement

:hasDisplacement a owl:ObjectProperty ;
	rdfs:domain _:genid1 .

_:genid1 a owl:Class ;
	owl:unionOf _:genid3 .

_:genid3 a rdf:List ;
	rdf:first :Ductile_Shear_Zone ;
	rdf:rest _:genid2 .

_:genid2 a rdf:List ;
	rdf:first :Fault ;
	rdf:rest rdf:nil .

:hasDisplacement rdfs:range :Displacement .

:Displacement a owl:Class .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasMaterialConstituent

:hasMaterialConstituent a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:hasConstituent ;
	rdfs:range :Rock_Material .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderAge

:hasOlderAge a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderBound

:hasOlderBound a owl:ObjectProperty ;
	rdfs:range gkoc:Time_Instant ;
	rdfs:label "has Older Bounding Age"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderHost

:hasOlderHost a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:HostedBy .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasProperty

:hasProperty a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasReference

:hasReference a owl:ObjectProperty ;
	owl:inverseOf :isReferenceFor .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialProperty

:hasSpatialProperty a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasProperty ;
	a owl:InverseFunctionalProperty ;
	rdfs:comment "Geographic disposition of an endurant."@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue

:hasTermValue a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerAge

:hasYoungerAge a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerBound

:hasYoungerBound a owl:ObjectProperty ;
	rdfs:range gkoc:Time_Instant ;
	rdfs:comment "assign a younger age boundary for a time interval"@en ;
	rdfs:label "has Younger Bounding Age"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost

:hasYoungerHost a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:HostedBy .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/intrudes

:intrudes a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:isPhysicallyRelatedTo ;
	rdfs:comment "subject rock body intrudes object rock body. Implies that subject rock body is igneous rock, and that object rock body is older than subject rock body" ;
	rdfs:label "intrudes" .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isPartOfTimeScale

:isPartOfTimeScale a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:isPartOf ;
	rdfs:range :Geologic_Time_Scale ;
	rdfs:comment "" ;
	rdfs:label "in Time Scale "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isRealizedBy

:isRealizedBy a owl:ObjectProperty ;
	owl:inverseOf :realizes .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isReferenceFor

:isReferenceFor a owl:ObjectProperty .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isSettingFor

:isSettingFor a owl:ObjectProperty ;
	rdfs:domain gkoc:Physical_Setting .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/overlies

:overlies a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:isPhysicallyRelatedTo ;
	rdfs:domain :Geologic_Unit ;
	rdfs:range :Geologic_Unit ;
	rdfs:comment "relationship between striatified geologic units "@en ;
	rdfs:label "overlies "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/position

:position a owl:ObjectProperty ;
	rdfs:comment "implement temporal location for GeochronologicBoundary" ;
	rdfs:label "position" .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/realizes

:realizes a owl:ObjectProperty ;
	rdfs:comment "" ;
	rdfs:label "realizes"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/underlies

:underlies a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:isPhysicallyRelatedTo ;
	rdfs:domain :Geologic_Unit ;
	rdfs:range :Geologic_Unit ;
	rdfs:comment "relationship between stratified geologic units "@en ;
	rdfs:label "underlies "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/within

:within a owl:ObjectProperty ;
	rdfs:subPropertyOf gkoc:isPhysicallyRelatedTo ;
	rdfs:domain :Geologic_Unit ;
	rdfs:range :Geologic_Unit ;
	rdfs:comment "relationship between stratified geologic units "@en ;
	rdfs:label "within "@en .
# 
# 
# 
# #################################################################
# #
# #    Data properties
# #
# #################################################################
# 
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate

:hasDate a owl:DatatypeProperty ;
	rdfs:subPropertyOf gkoc:hasNumericValue ;
	rdfs:comment "numeric value for a time coordinate. Associated hasUOM property asserts the units of measure; the temporal reference system used to assert dates should be explained in metadata for the data instances."@en .
# 
# 
# 
# #################################################################
# #
# #    Classes
# #
# #################################################################
# 
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Age_Rank

:Age_Rank a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Interval , _:genid4 .

_:genid4 a owl:Restriction ;
	owl:onProperty gkoc:hasPart ;
	owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
	owl:onClass :Geologic_Time_Interval .

:Age_Rank rdfs:subClassOf _:genid5 .

_:genid5 a owl:Restriction ;
	owl:onProperty gkoc:isPartOf ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Epoch .

:Age_Rank rdfs:comment "most granular geochronologic time interval; one or more Ages are grouped into an Epoch"@en ;
	rdfs:label "Age (time scale rank)"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Alteration_Zone

:Alteration_Zone a owl:Class ;
	rdfs:subClassOf :Geologic_Unit .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bed

:Bed a owl:Class ;
	rdfs:subClassOf :Lithostratigraphic_Unit .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding

:Bedding a owl:Class ;
	rdfs:subClassOf :Foliation ;
	rdfs:label "Bedding"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding_Package

:Bedding_Package a owl:Class ;
	rdfs:subClassOf :Lithosome ;
	rdfs:comment "A sub-map scale sequence of strata, e.g. bouma sequence, fining-upward sequence, interbedded sandstone and mudstone... "@en ;
	rdfs:label "Bedding Package "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Boundary_Stratotype

:Boundary_Stratotype a owl:Class ;
	rdfs:subClassOf :Stratotype ;
	rdfs:comment "A stratigraphic section that contains a designated point in a stratigraphic sequence of essentially continuous deposition, preferably marine, chosen for its correlation potential. http://www.stratigraphy.org/upload/bak/chron.htm"@en ;
	rdfs:label "Boundary Stratotype"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Age_Interval

:Chronostratigraphic_Age_Interval a owl:Class ;
	rdfs:subClassOf :Geologic_Age , _:genid6 .

_:genid6 a owl:Restriction ;
	owl:onProperty :hasOlderAge ;
	owl:allValuesFrom :Geologic_Time_Interval .

:Chronostratigraphic_Age_Interval rdfs:subClassOf _:genid7 .

_:genid7 a owl:Restriction ;
	owl:onProperty :hasYoungerAge ;
	owl:allValuesFrom :Geologic_Time_Interval .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Chronostratigraphic_Unit

:Chronostratigraphic_Unit a owl:Class ;
	rdfs:subClassOf :Geologic_Unit , _:genid8 .

_:genid8 a owl:Restriction ;
	owl:onProperty :realizes ;
	owl:someValuesFrom :Geologic_Time_Interval .

:Chronostratigraphic_Unit rdfs:subClassOf _:genid9 .

_:genid9 a owl:Restriction ;
	owl:onProperty gkoc:hasPart ;
	owl:allValuesFrom _:genid10 .

_:genid10 a owl:Class ;
	owl:unionOf _:genid12 .

_:genid12 a rdf:List ;
	rdf:first :Chronostratigraphic_Unit ;
	rdf:rest _:genid11 .

_:genid11 a rdf:List ;
	rdf:first :Rock_Body_Part ;
	rdf:rest rdf:nil .

:Chronostratigraphic_Unit rdfs:subClassOf _:genid13 .

_:genid13 a owl:Restriction ;
	owl:onProperty gkoc:isPartOf ;
	owl:allValuesFrom :Chronostratigraphic_Unit .

:Chronostratigraphic_Unit rdfs:comment """A body of rocks that includes all rocks, layered or unlayered, formed during a specific interval of geologic time, and only those rocks formed during that time span. Chronostratigraphic units are bounded by synchronous horizons.

The rank and relative magnitude of the units in the chronostratigraphic hierarchy are a function of the length of the time interval that their rocks subtend, rather than of their physical thickness.

(http://www.stratigraphy.org/upload/bak/chron.htm)"""@en ;
	rdfs:label "Chronostratigraphic unit"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clast

:Clast a owl:Class ;
	rdfs:subClassOf :Rock_Body_Part ;
	rdfs:label "Clast"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clastic_Granular_Material

:Clastic_Granular_Material a owl:Class ;
	rdfs:subClassOf :Granular_Material ;
	rdfs:comment """Use for clastic sedimentary components --conglomerate clast types, sand components, clay matrix. 
Also for deformation-producted clasts in tectonic breccia."""@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Clay_Mineral

:Clay_Mineral a owl:Class ;
	rdfs:subClassOf :Mineral ;
	rdfs:comment "general class for all clay minerals "@en ;
	rdfs:label "Clay Mineral "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Complex

:Complex a owl:Class ;
	rdfs:subClassOf :Lithostratigraphic_Unit ;
	rdfs:label "Complex"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Contact

:Contact a owl:Class ;
	rdfs:subClassOf gkoc:Low_Dimension_Feature , :Geologic_Structure , _:genid14 .

_:genid14 a owl:Restriction ;
	owl:onProperty gkoc:HostedBy ;
	owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
	owl:onClass :Rock_Body .

:Contact rdfs:subClassOf _:genid15 .

_:genid15 a owl:Restriction ;
	owl:onProperty :hasDisplacement ;
	owl:cardinality "0"^^xsd:nonNegativeInteger .

:Contact rdfs:subClassOf _:genid16 .

_:genid16 a owl:Restriction ;
	owl:onProperty :hasReference ;
	owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Stratotype .

:Contact rdfs:subClassOf _:genid17 .

_:genid17 a owl:Restriction ;
	owl:onProperty :realizes ;
	owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Geochronologic_Boundary .

:Contact rdfs:comment """Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies. A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) <--> exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]"""@en ;
	rdfs:label "Contact"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Crystallized_Granular_Material

:Crystallized_Granular_Material a owl:Class ;
	rdfs:subClassOf :Granular_Material ;
	rdfs:comment "use to describe crystalline grain types in igneous, metamorphic and metasomatic rocks, e.g. the plagioclase phenocrysts, groundmass quartz, biotite phenocrysts, Andalusite porphyroblasts..."@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation

:Deformation a owl:Class .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Deformation_Unit

:Deformation_Unit a owl:Class ;
	rdfs:subClassOf :Tectonic_Unit .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Displacement
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Deformation

:Ductile_Deformation a owl:Class .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Ductile_Shear_Zone

:Ductile_Shear_Zone a owl:Class ;
	rdfs:subClassOf gkoc:Material_Spatial_Feature , :Geologic_Structure , _:genid18 .

_:genid18 a owl:Restriction ;
	owl:onProperty gkoc:isParticipantIn ;
	owl:someValuesFrom :Ductile_Deformation .

:Ductile_Shear_Zone rdfs:subClassOf _:genid19 .

_:genid19 a owl:Restriction ;
	owl:onProperty :hasDisplacement ;
	owl:someValuesFrom :Displacement .

:Ductile_Shear_Zone rdfs:subClassOf _:genid20 .

_:genid20 a owl:Restriction ;
	owl:onProperty gkoc:isParticipantIn ;
	owl:allValuesFrom :Ductile_Deformation .

:Ductile_Shear_Zone rdfs:label "Ductile Shear Zone"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Eon

:Eon a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Interval , _:genid21 .

_:genid21 a owl:Restriction ;
	owl:onProperty gkoc:hasPart ;
	owl:allValuesFrom _:genid22 .

_:genid22 a owl:Class ;
	owl:unionOf _:genid24 .

_:genid24 a rdf:List ;
	rdf:first :Eon ;
	rdf:rest _:genid23 .

_:genid23 a rdf:List ;
	rdf:first :Era ;
	rdf:rest rdf:nil .

:Eon rdfs:subClassOf _:genid25 .

_:genid25 a owl:Restriction ;
	owl:onProperty gkoc:isPartOf ;
	owl:allValuesFrom :Eon .

:Eon rdfs:comment "How will this be defined?  Must contain at least one Era and have duration longer than some Era?"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Eonothem

:Eonothem a owl:Class ;
	rdfs:subClassOf :Chronostratigraphic_Unit , _:genid26 .

_:genid26 a owl:Restriction ;
	owl:onProperty :realizes ;
	owl:someValuesFrom :Eon .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Epoch

:Epoch a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Interval , _:genid27 .

_:genid27 a owl:Restriction ;
	owl:onProperty gkoc:hasPart ;
	owl:allValuesFrom :Age_Rank .

:Epoch rdfs:subClassOf _:genid28 .

_:genid28 a owl:Restriction ;
	owl:onProperty gkoc:isPartOf ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Period .

:Epoch rdfs:comment "An Epoch may be subdivided into Ages. Epochs are grouped into Periods. Example epochs: Eocene, Pleistocene"@en ;
	rdfs:label "Epoch"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Era

:Era a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Interval , _:genid29 .

_:genid29 a owl:Restriction ;
	owl:onProperty gkoc:isPartOf ;
	owl:someValuesFrom :Eon .

:Era rdfs:subClassOf _:genid30 .

_:genid30 a owl:Restriction ;
	owl:onProperty gkoc:hasPart ;
	owl:allValuesFrom :Period .

:Era rdfs:comment "Named rank for geochronologic time interval. An Era may be subdivided into Periods. Eras are grouped into Eons"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Erathem

:Erathem a owl:Class ;
	rdfs:subClassOf :Chronostratigraphic_Unit , _:genid31 .

_:genid31 a owl:Restriction ;
	owl:onProperty :realizes ;
	owl:someValuesFrom :Era .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Facies

:Facies a owl:Class ;
	rdfs:subClassOf :Rock_Body_Part ;
	rdfs:label "Facies"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fault

:Fault a owl:Class ;
	rdfs:subClassOf gkoc:Low_Dimension_Feature , :Geologic_Structure , _:genid32 .

_:genid32 a owl:Restriction ;
	owl:onProperty gkoc:isParticipantIn ;
	owl:allValuesFrom :Deformation .

:Fault rdfs:subClassOf _:genid33 .

_:genid33 a owl:Restriction ;
	owl:onProperty :hasDisplacement ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Displacement .

:Fault rdfs:label "Fault"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fault_Zone

:Fault_Zone a owl:Class ;
	rdfs:subClassOf gkoc:Material_Spatial_Feature ;
	rdfs:comment "" ;
	rdfs:label "Fault Zone "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fold

:Fold a owl:Class ;
	rdfs:subClassOf gkoc:Morphologic_Feature , :Geologic_Structure ;
	rdfs:label "Fold"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Foliation

:Foliation a owl:Class ;
	rdfs:subClassOf gkoc:Integral_Feature , :Geologic_Structure ;
	rdfs:label "Foliation"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Formation

:Formation a owl:Class ;
	rdfs:subClassOf :Lithostratigraphic_Unit .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fossil

:Fossil a owl:Class ;
	rdfs:subClassOf :Rock_Body_Part ;
	rdfs:label "Fossil"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fossil_Granular_Material

:Fossil_Granular_Material a owl:Class ;
	rdfs:subClassOf :Granular_Material ;
	rdfs:comment "use to describe body fossil components in a fossiliferous rock, e.g. crinoid columnals, radiolaria in chert, ammonites, shell hash...."@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Fracture

:Fracture a owl:Class ;
	rdfs:subClassOf gkoc:Low_Dimension_Feature , :Geologic_Structure , _:genid34 .

_:genid34 a owl:Restriction ;
	owl:onProperty gkoc:isParticipantIn ;
	owl:someValuesFrom :Geologic_Process .

:Fracture rdfs:subClassOf _:genid35 .

_:genid35 a owl:Restriction ;
	owl:onProperty :hasDisplacement ;
	owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass gkoc:Complex_Physical_Property .

:Fracture rdfs:comment "axioms for hasDisplacment and gkoc:participantIn are place holders for more specific properties in pro and gkop. Trying to avoid imports here."@en ;
	rdfs:label "Fracture"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/GSSA

:GSSA a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Date ;
	rdfs:comment "A Geochronologic Age defined by the International Stratigraphic Commission, based on fiat selection of defined older and younger bounding time coordinates"@en ;
	rdfs:label "GSSA"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/GSSP

:GSSP a owl:Class ;
	rdfs:subClassOf :Stratigraphic_Point , _:genid36 .

_:genid36 a owl:Restriction ;
	owl:onProperty :containedBy ;
	owl:someValuesFrom :Stratotype .

:GSSP rdfs:subClassOf _:genid37 .

_:genid37 a owl:Restriction ;
	owl:onProperty :hasReference ;
	owl:someValuesFrom :Stratotype .

:GSSP rdfs:comment "A stratigraphic point that is hosted by a top and bottom segment of adjacent chronostratigraphic units. The top and bottom are part of an outcrop and part of a stratotype (type section) for the unit. "@en ;
	rdfs:label "GSSP "@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Age_Interval

:Geochronologic_Age_Interval a owl:Class ;
	rdfs:subClassOf :Geologic_Age , _:genid38 .

_:genid38 a owl:Restriction ;
	owl:onProperty :hasOlderBound ;
	owl:allValuesFrom :Geologic_Time_Date .

:Geochronologic_Age_Interval rdfs:subClassOf _:genid39 .

_:genid39 a owl:Restriction ;
	owl:onProperty :hasYoungerBound ;
	owl:allValuesFrom :Geologic_Time_Date .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geochronologic_Boundary

:Geochronologic_Boundary a owl:Class ;
	rdfs:subClassOf :Geologic_Time_Date , _:genid40 .

_:genid40 a owl:Restriction ;
	owl:onProperty :isRealizedBy ;
	owl:allValuesFrom :Stratigraphic_Point .

:Geochronologic_Boundary rdfs:label "Geochronologic Boundary"@en .
# 
# http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Age

