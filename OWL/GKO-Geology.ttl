# baseURI: http://loop3d.org/GKO/ontology/2019/1/geologicfeature
# imports: http://loop3d.org/GKO/ontology/2019/1/common

@prefix : <http://loop3d.org/GKO/ontology/2019/1/geologicfeature/> .
@prefix gkoc: <http://loop3d.org/GKO/ontology/2019/1/common/> .
@prefix gkop: <http://loop3d.org/GKO/ontology/2019/1/geologicproperty/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pro: <http://loop3d.org/GKO/ontology/2019/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://loop3d.org/GKO/ontology/2019/1/geologicfeature>
  a owl:Ontology ;
  rdfs:comment "Base geologic feature classes, constraints, and relationships"@en ;
  owl:imports <http://loop3d.org/GKO/ontology/2019/1/common> ;
.
:Additive_Process
  a owl:Class ;
  rdfs:comment "process that introduces new matter into the model space" ;
  rdfs:label "Additive Process" ;
  rdfs:subClassOf :Geologic_Process ;
.
:Age_Rank
  a owl:Class ;
  rdfs:comment "most granular geochronologic time interval; one or more Ages are grouped into an Epoch"@en ;
  rdfs:label "Age (time scale rank)"@en ;
  rdfs:subClassOf :Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass :Epoch ;
      owl:onProperty gkoc:isPartOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass :Geologic_Time_Interval ;
      owl:onProperty gkoc:hasPart ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
.
:Alteration_Zone
  a owl:Class ;
  rdfs:subClassOf :Geologic_Unit ;
.
:Bed
  a owl:Class ;
  rdfs:subClassOf :Lithostratigraphic_Unit ;
.
:Bedding
  a owl:Class ;
  rdfs:label "Bedding"@en ;
  rdfs:subClassOf :Foliation ;
.
:Bedding_Package
  a owl:Class ;
  rdfs:comment "A sub-map scale sequence of strata, e.g. bouma sequence, fining-upward sequence, interbedded sandstone and mudstone... "@en ;
  rdfs:label "Bedding Package "@en ;
  rdfs:subClassOf :Lithosome ;
.
:Boundary_Stratotype
  a owl:Class ;
  rdfs:comment "A stratigraphic section that contains a designated point in a stratigraphic sequence of essentially continuous deposition, preferably marine, chosen for its correlation potential. http://www.stratigraphy.org/upload/bak/chron.htm"@en ;
  rdfs:label "Boundary Stratotype"@en ;
  rdfs:subClassOf :Stratotype ;
.
:Chronostratigraphic_Age_Interval
  a owl:Class ;
  rdfs:subClassOf :Geologic_Age ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty :hasOlderAge ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty :hasYoungerAge ;
    ] ;
.
:Chronostratigraphic_Unit
  a owl:Class ;
  rdfs:comment """A body of rocks that includes all rocks, layered or unlayered, formed during a specific interval of geologic time, and only those rocks formed during that time span. Chronostratigraphic units are bounded by synchronous horizons.

The rank and relative magnitude of the units in the chronostratigraphic hierarchy are a function of the length of the time interval that their rocks subtend, rather than of their physical thickness.

(http://www.stratigraphy.org/upload/bak/chron.htm)"""@en ;
  rdfs:label "Chronostratigraphic unit"@en ;
  rdfs:subClassOf :Geologic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Chronostratigraphic_Unit ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Chronostratigraphic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :realizes ;
      owl:someValuesFrom :Geologic_Time_Interval ;
    ] ;
.
:Clast
  a owl:Class ;
  rdfs:label "Clast"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Clastic_Granular_Material
  a owl:Class ;
  rdfs:comment """Use for clastic sedimentary components --conglomerate clast types, sand components, clay matrix. 
Also for deformation-producted clasts in tectonic breccia."""@en ;
  rdfs:subClassOf :Granular_Material ;
.
:Clay_Mineral
  a owl:Class ;
  rdfs:comment "general class for all clay minerals "@en ;
  rdfs:label "Clay Mineral "@en ;
  rdfs:subClassOf :Mineral ;
.
:Complex
  a owl:Class ;
  rdfs:label "Complex"@en ;
  rdfs:subClassOf :Lithostratigraphic_Unit ;
.
:Contact
  a owl:Class ;
  rdfs:comment """Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies. A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) <--> exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]"""@en ;
  rdfs:label "Contact"@en ;
  rdfs:subClassOf gkoc:Low_Dimension_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty :hasDisplacement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :Geochronologic_Boundary ;
      owl:onProperty :realizes ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :Stratotype ;
      owl:onProperty :hasReference ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass :Rock_Body ;
      owl:onProperty gkoc:HostedBy ;
    ] ;
.
:Crystallized_Granular_Material
  a owl:Class ;
  rdfs:comment "use to describe crystalline grain types in igneous, metamorphic and metasomatic rocks, e.g. the plagioclase phenocrysts, groundmass quartz, biotite phenocrysts, Andalusite porphyroblasts..."@en ;
  rdfs:subClassOf :Granular_Material ;
.
:Deformation
  a owl:Class ;
  rdfs:subClassOf :Geologic_Process ;
.
:Deformation_Unit
  a owl:Class ;
  rdfs:subClassOf :Tectonic_Unit ;
.
:Ductile_Shear_Zone
  a owl:Class ;
  rdfs:label "Ductile Shear Zone"@en ;
  rdfs:subClassOf gkoc:Material_Spatial_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom pro:ductile_flow ;
      owl:onProperty gkoc:isParticipantIn ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:isParticipantIn ;
      owl:someValuesFrom pro:ductile_flow ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasDisplacement ;
      owl:someValuesFrom gkop:Displacement ;
    ] ;
.
:Eon
  a owl:Class ;
  rdfs:comment "How will this be defined?  Must contain at least one Era and have duration longer than some Era?"@en ;
  rdfs:subClassOf :Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Eon ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Eon ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Era ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasPart ;
    ] ;
.
:Eonothem
  a owl:Class ;
  rdfs:subClassOf :Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :realizes ;
      owl:someValuesFrom :Eon ;
    ] ;
.
:Epoch
  a owl:Class ;
  rdfs:comment "An Epoch may be subdivided into Ages. Epochs are grouped into Periods. Example epochs: Eocene, Pleistocene"@en ;
  rdfs:label "Epoch"@en ;
  rdfs:subClassOf :Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Age_Rank ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass :Period ;
      owl:onProperty gkoc:isPartOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
:Era
  a owl:Class ;
  rdfs:comment "Named rank for geochronologic time interval. An Era may be subdivided into Periods. Eras are grouped into Eons"@en ;
  rdfs:subClassOf :Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Period ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:isPartOf ;
      owl:someValuesFrom :Eon ;
    ] ;
.
:Erathem
  a owl:Class ;
  rdfs:subClassOf :Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :realizes ;
      owl:someValuesFrom :Era ;
    ] ;
.
:Facies
  a owl:Class ;
  rdfs:label "Facies"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Fault
  a owl:Class ;
  rdfs:label "Fault"@en ;
  rdfs:subClassOf gkoc:Low_Dimension_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Deformation ;
      owl:onProperty gkoc:isParticipantIn ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass gkop:Displacement ;
      owl:onProperty :hasDisplacement ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
:Fault_Zone
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Fault Zone "@en ;
  rdfs:subClassOf gkoc:Material_Spatial_Feature ;
.
:Fold
  a owl:Class ;
  rdfs:label "Fold"@en ;
  rdfs:subClassOf gkoc:Morphologic_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
.
:Foliation
  a owl:Class ;
  rdfs:label "Foliation"@en ;
  rdfs:subClassOf gkoc:Integral_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
.
:Formation
  a owl:Class ;
  rdfs:subClassOf :Lithostratigraphic_Unit ;
.
:Fossil
  a owl:Class ;
  rdfs:label "Fossil"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Fossil_Granular_Material
  a owl:Class ;
  rdfs:comment "use to describe body fossil components in a fossiliferous rock, e.g. crinoid columnals, radiolaria in chert, ammonites, shell hash...."@en ;
  rdfs:subClassOf :Granular_Material ;
.
:Fracture
  a owl:Class ;
  rdfs:comment "axioms for hasDisplacment and gkoc:participantIn are place holders for more specific properties in pro and gkop. Trying to avoid imports here."@en ;
  rdfs:label "Fracture"@en ;
  rdfs:subClassOf gkoc:Low_Dimension_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass gkoc:Complex_Physical_Property ;
      owl:onProperty :hasDisplacement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:isParticipantIn ;
      owl:someValuesFrom :Geologic_Process ;
    ] ;
.
:GSSA
  a owl:Class ;
  rdfs:comment "A Geochronologic Age defined by the International Stratigraphic Commission, based on fiat selection of defined older and younger bounding time coordinates"@en ;
  rdfs:label "GSSA"@en ;
  rdfs:subClassOf :Geologic_Time_Date ;
.
:GSSP
  a owl:Class ;
  rdfs:comment "A stratigraphic point that is hosted by a top and bottom segment of adjacent chronostratigraphic units. The top and bottom are part of an outcrop and part of a stratotype (type section) for the unit. "@en ;
  rdfs:label "GSSP "@en ;
  rdfs:subClassOf :Stratigraphic_Point ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :containedBy ;
      owl:someValuesFrom :Stratotype ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasReference ;
      owl:someValuesFrom :Stratotype ;
    ] ;
.
:Geochronologic_Age_Interval
  a owl:Class ;
  rdfs:subClassOf :Geologic_Age ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Date ;
      owl:onProperty :hasOlderBound ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Date ;
      owl:onProperty :hasYoungerBound ;
    ] ;
.
:Geochronologic_Boundary
  a owl:Class ;
  rdfs:label "Geochronologic Boundary"@en ;
  rdfs:subClassOf :Geologic_Time_Date ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Stratigraphic_Point ;
      owl:onProperty :isRealizedBy ;
    ] ;
.
:Geologic_Age
  a owl:Class ;
  rdfs:subClassOf gkoc:Temporal_Property ;
.
:Geologic_Age_Instant
  a owl:Class ;
  rdfs:comment "A Geologic age that is specified by either a numeric temporal coordinate or correlation with a stratigraphic point via GeochronologicBoundary/isRealizedBy/StratigraphicPoint"@en ;
  rdfs:label "Geologic Age Instant"@en ;
  rdfs:subClassOf :Geologic_Age ;
.
:Geologic_Event
  a owl:Class ;
  rdfs:comment """If an event is caused by a process, then any restrictions on participants of the process also apply to participants of the event 
  causedBy (x,y) ^ GeologicEvent (x) ^ GeologicProcess (y) --> forall z  [participates(z,x) --> participates (z,y)]"""@en ;
  rdfs:comment "TBD: Sub class from dolce:event, but need to verify that this doesn't carry any undesirable implications"@en ;
  rdfs:label "Geologic Event"@en ;
  rdfs:subClassOf gkoc:Event ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Age ;
      owl:onProperty gkoc:hasTemporalProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Process ;
      owl:onProperty gkoc:isManifestationOf ;
    ] ;
.
:Geologic_Process
  a owl:Class ;
  rdfs:label "Geologic Process"@en ;
  rdfs:subClassOf gkoc:Process ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Age ;
      owl:onProperty gkoc:hasTemporalProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Event ;
      owl:onProperty gkoc:manifests ;
    ] ;
.
:Geologic_Structure
  a owl:Class ;
  rdfs:label "Geologic Structure"@en ;
  rdfs:subClassOf gkoc:Physical_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Structure ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Structure ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Body ;
      owl:onProperty gkoc:HostedBy ;
    ] ;
.
:Geologic_Time_Date
  a owl:Class ;
  rdfs:comment """A temporal coordinate value, located either by a point position (with uncertainty) on a time line, specified by a numeric coordinate (generally MYPB, but definitions of 'present' vary). Can be a GeochronologicBoundary if it is associated with a location in a particular stratigraphic section, or a GSSA if the numeric time coordinate is arbitrarily assigned.
Probably should specify a Temporal Reference System used to assign coordinate values."""@en ;
  rdfs:label "Geologic Time Date"@en ;
  rdfs:subClassOf gkoc:Time_Instant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gkoc:Statistical_Calculation ;
      owl:onProperty gkoc:hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:double ;
      owl:onProperty gkoc:hasUncertainty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass gkoc:UOM ;
      owl:onProperty gkoc:hasUOM ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange xsd:double ;
      owl:onProperty :hasDate ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:disjointWith :Geologic_Time_Interval ;
.
:Geologic_Time_Interval
  a owl:Class ;
  rdfs:label "Geologic Time Interval"@en ;
  rdfs:subClassOf gkoc:Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty gkoc:nextTimeInterval ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty gkoc:previousTimeInterval ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :Chronostratigraphic_Unit ;
      owl:onProperty :isRealizedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :Geologic_Time_Interval ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasOlderBound ;
      owl:someValuesFrom :Geologic_Time_Date ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasYoungerBound ;
      owl:someValuesFrom :Geologic_Time_Date ;
    ] ;
.
:Geologic_Time_Scale
  a owl:Class ;
  rdfs:comment "A collection of Geochronologic Age entities that obey a special topology (Cox and Richard, 2010)."@en ;
  rdfs:label "Geologic Time Scale"@en ;
  rdfs:subClassOf gkoc:Temporal_Region ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Time_Interval ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gkoc:hasPart ;
      owl:someValuesFrom :Geologic_Time_Interval ;
    ] ;
.
:Geologic_Unit
  a owl:Class ;
  rdfs:comment """A geologic unit and (the amount of) its rock material are co-located, but different entities. They can share properties, e.g. porosity, thickness, density, but also have differences:
- different persistence conditions: the material can exist without the unit; e.g. a unit can be cease to exist due to natural (seismic) or artificial (explosion) means, but the material can persist
- different parthood conditions:  
-- a unit can be part of a stratigraphic lexicon, but its material cannot be part of the lexicon. 
-- parts of the unit are not parts of the material, e.g. the western arm of formation X is not a part of its sandstone - the sandstone making up the western part of X is a part of the sandstone 
-- different identity conditions: a unit can have some material added or removed and retain identity, but the material changes identity with the addition or removal of some material"""@en ;
  rdfs:label "Geologic Unit"@en ;
  rdfs:subClassOf gkoc:Physical_Object ;
  rdfs:subClassOf :Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Geologic_Unit ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Material ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Stratotype ;
      owl:onProperty :hasReference ;
    ] ;
.
:Glass
  a owl:Class ;
  rdfs:comment "Amorphous product of rapid coolimh of melted earth material. Generally product of igneous activity, frictional melting or bolide impact."@en ;
  rdfs:subClassOf gkoc:Glass ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Granular_Material ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Material ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:isConstituentOf ;
    ] ;
.
:Gradational_Contact_Zone
  a owl:Class ;
  rdfs:label "Gradational Contact Zone"@en ;
  rdfs:subClassOf gkoc:Material_Spatial_Feature ;
.
:Granular_Material
  a owl:Class ;
  rdfs:comment "Represents a rock body constituent composed of particles that share a set of characteristics, e.g. particle size (distribution), mineralogy, shape.  E.g. the sand that is a constituent in a sandstone, or the feldspar phenocrysts that are a constituent in a granite"@en ;
  rdfs:comment "example top level subclasses shown (probably not complete); differentiated based on genesis for the most part."@en ;
  rdfs:label "Granular Material"@en ;
  rdfs:subClassOf gkoc:Amount_Of_Matter ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Rock_Material ;
      owl:onProperty gkoc:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gkoc:Element ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Glass ;
                  rdf:rest [
                      a rdf:List ;
                      rdf:first :Mineral ;
                      rdf:rest () ;
                    ] ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
.
:Group
  a owl:Class ;
  rdfs:subClassOf :Lithostratigraphic_Unit ;
.
:Inclusion
  a owl:Class ;
  rdfs:label "Inclusion"@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Joint
  a owl:Class ;
  rdfs:label "Joint"@en ;
  rdfs:subClassOf :Fracture ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onClass gkop:Displacement ;
      owl:onProperty :hasDisplacement ;
    ] ;
.
:Lineation
  a owl:Class ;
  rdfs:subClassOf gkoc:Integral_Feature ;
  rdfs:subClassOf :Geologic_Structure ;
.
:Lithosome
  a owl:Class ;
  rdfs:comment "A kind of rock body that has multiple occurrences in a single geologic unit. A mass of rock of uniform character, characterized by geometry, composition, and internal structure. (http://inspire.ec.europa.eu/codelist/CompositionPartRoleValue/lithosome)"@en ;
  rdfs:label "Lithosome "@en ;
  rdfs:subClassOf :Rock_Body_Part ;
.
:Lithostratigraphic_Unit
  a owl:Class ;
  rdfs:label "Lithostratigraphic Unit"@en ;
  rdfs:subClassOf :Geologic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom :Lithostratigraphic_Unit ;
      owl:onProperty gkoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Lithostratigraphic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :hasMaterialConstituent ;
      owl:someValuesFrom :Rock_Material ;
    ] ;
.
:Member
  a owl:Class ;
  rdfs:subClassOf :Lithostratigraphic_Unit ;
.
:Metamorphic_Zone
  a owl:Class ;
  rdfs:subClassOf :Geologic_Unit ;
.
:Mineral
  a owl:Class ;
  rdfs:comment "\"A mineral is an element or chemical compound that is normally crystalline and that has been formed as a result of geological processes.\" Nickel, Ernest H. (1995), The definition of a mineral,  The Canadian Mineralogist. 33 (3): 689–90."@en ;
  rdfs:label "Mineral"@en ;
  rdfs:subClassOf gkoc:Amount_Of_Matter ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gkoc:Element ;
      owl:onProperty gkoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first :Granular_Material ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first :Rock_Material ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gkoc:isConstituentOf ;
    ] ;
.
:Outcrop
