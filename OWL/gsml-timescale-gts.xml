<?xml version="1.0"?>
<rdf:RDF xmlns="http://resource.geosciml.org/ontology/timescale/gts#"
     xml:base="http://resource.geosciml.org/ontology/timescale/gts"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:gts="http://resource.geosciml.org/ontology/timescale/gts#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rank="http://resource.geosciml.org/ontology/timescale/rank/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:voaf="http://purl.org/vocommons/voaf#"
     xmlns:thors="http://resource.geosciml.org/ontology/timescale/thors#">
    <owl:Ontology rdf:about="http://resource.geosciml.org/ontology/timescale/gts">
        <owl:versionIRI rdf:resource="https://github.com/GeoscienceAustralia/geosciml.org/commit/24d302bf3f0fa315c1dddc2c734b84f52299cd02#diff-73b17e22072459ab7e06e9ab664a9d48"/>
        <owl:imports rdf:resource="http://resource.geosciml.org/ontology/timescale/thors"/>
        <dc:contributor>Stephen M RICHARD</dc:contributor>
        <dc:description>This is an RDF/OWL representation of the GeoSciML Geologic Timescale model, which has been adapted from the model described in Cox, S.J.D, &amp; Richard, S.M. (2005) A formal model for the geologic timescale and GSSP, compatible with geospatial information transfer standards, Geosphere, Geological Society of America 1/3, 119–137.</dc:description>
        <dc:rights>Copyright © 2012-2017 CSIRO</dc:rights>
        <dc:source>Cox, S.J.D, &amp; Richard, S.M. (2005) A formal model for the geologic timescale and GSSP, compatible with geospatial information transfer standards, Geosphere, Geological Society of America 1/3, 119–137.</dc:source>
        <dc:title>Geologic Timescale model</dc:title>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2011-01-01</dct:created>
        <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-04-28</dct:modified>
        <vann:preferredNamespacePrefix>gts</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#"/>
        <rdfs:seeAlso rdf:resource="doi:10.1130/GES00022.1"/>
        <rdfs:seeAlso rdf:resource="http://www.geosciml.org"/>
        <rdfs:seeAlso rdf:resource="http://dx.doi.org/10.1130/GES00022.1"/>
        <rdfs:seeAlso rdf:resource="http://geosphere.gsapubs.org/cgi/doi/10.1130/GES00022.1"/>
        <owl:priorVersion rdf:resource="https://www.seegrid.csiro.au/subversion/xmml/ontologies/tags/201402/Geoscience/GeologicTimeScale/gts.rdf"/>
        <skos:changeNote>2017-04-28 - removed imports ISO ontologies</skos:changeNote>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#pattern -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2001/XMLSchema#pattern"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://org.semanticweb.owlapi/error#Error8 -->

    <rdfs:Datatype rdf:about="http://org.semanticweb.owlapi/error#Error8"/>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#EraCode -->

    <rdfs:Datatype rdf:about="http://resource.geosciml.org/ontology/timescale/gts#EraCode">
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>Era code</rdfs:label>
        <owl:equivalentClass rdf:resource="http://org.semanticweb.owlapi/error#Error8"/>
    </rdfs:Datatype>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#boundary -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#boundary">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
        <rdfs:range rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
        <rdfs:comment>geochronologic boundary corresponding with this point</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>geochronologic boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#correlatesWith -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#correlatesWith">
        <owl:inverseOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#manifestedBy"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>This property points from a geologic feature or event to a geochronologic element</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>correlating boundary or era</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#era -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#era">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicSection"/>
        <rdfs:range rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
        <rdfs:comment>geochronologic era corresponding with this section</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>geochronologic era</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#event -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#event">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicEvent"/>
        <rdfs:comment>stratigraphic event corresponding with this boundary or point</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>stratigraphic event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#manifestedBy -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#manifestedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>This property points from a geochronologic element to a geologic feature</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>geologic manifestation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#positionalUncertainty -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#positionalUncertainty">
        <rdfs:label>Uncertainty in time position</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#rank -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#rank">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
        <rdfs:range rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEraRank"/>
        <rdfs:comment>Elements of all ranks are included in the class &apos;gts:GeochronologicEra&apos;. 
The rank may be indicated by membership of a sub-class, or is indicated using the &apos;gts:rank&apos; property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>Geochronologic era rank</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#stratotype -->

    <owl:ObjectProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#stratotype">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicSection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>the characteristic point or section corresponding with a geochronologic concept</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>stratotype</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#boundaryLevel -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#boundaryLevel">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
        <rdfs:comment>the level within the section of the point characterizing the boundary</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>boundary level</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#correlationEvent -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#correlationEvent">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
        <rdfs:comment>the stratigraphic event that is intended to be represented by this stratigraphic point</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>correlation event</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#geologicDescription -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#geologicDescription">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicSection"/>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>geologic description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#geologicSetting -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#geologicSetting">
        <rdfs:domain rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicSection"/>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>geologic setting</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#ratifiedGSSP -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#ratifiedGSSP">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>&apos;true&apos; if ratified by ICS</rdfs:comment>
        <rdfs:label>ratified</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#status -->

    <owl:DatatypeProperty rdf:about="http://resource.geosciml.org/ontology/timescale/gts#status">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary"/>
                    <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>formal ICS status of this boundary and point</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label>status</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#notation -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#notation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#Age -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#Age">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#Eon -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#Eon">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#Epoch -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#Epoch">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#Era -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#Era">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#EraBoundary"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#stratotype"/>
                <owl:allValuesFrom rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary"/>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Geochronologic Boundary</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#Era"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#stratotype"/>
                <owl:allValuesFrom rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#notation"/>
                <owl:someValuesFrom rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#EraCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Geochronologic Era</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEraRank -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEraRank">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Geochronologic Era Rank</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#GeologicTimescale -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeologicTimescale">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#ReferenceSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#component"/>
                <owl:allValuesFrom rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#referencePoint"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicBoundary"/>
                            <rdf:Description rdf:about="http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Geologic Timescale</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#NumericEraBoundary">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/thors#EraBoundary"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#status"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Numeric Era Boundary</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#Period -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#Period">
        <rdfs:subClassOf rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#GeochronologicEra"/>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#StratigraphicEvent -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicEvent">
        <rdfs:comment>A subclass of Geologic Event (placeholder) </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://resource.geosciml.org/ontology/timescale/gts"/>
        <rdfs:label xml:lang="en">Stratigraphic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint -->

    <owl:Class rdf:about="http://resource.geosciml.org/ontology/timescale/gts#StratigraphicPoint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#correlationEvent"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://resource.geosciml.org/ontology/timescale/gts#status"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
