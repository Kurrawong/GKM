      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Period ;
    ] ;
.
geo:Tectonic_Unit
  a owl:Class ;
  rdfs:comment "A lithotectonic unit refers to “an assemblage of rocks that is unified on the basis of structural or deformational features, mutual relations, origin, or historical evolution, and the contained material may be igneous, sedimentary or metamorphic” (Neuendorf et al. 2011 Neuendorf, K.K.E., Mehl Jr., J.P. & Jackson, J.A., 2011: Glossary of Geology, 5th ed., revised. Alexandria, VA, American Geological Institute, 800 pp.) "@en ;
  rdfs:label "Tectonic Unit "@en ;
  rdfs:subClassOf geo:Geologic_Unit ;
.
geo:Temporal_Property
  a owl:Class ;
  rdfs:label "Temporal Property"@en ;
  rdfs:subClassOf geo:Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Temporal_Property ;
      owl:onProperty com:hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#endurant>
              <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#perdurant>
              geo:Temporal_Property
            ) ;
        ] ;
      owl:onProperty geo:isPropertyOf ;
    ] ;
.
geo:Thickness
