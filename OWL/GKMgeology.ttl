  rdfs:subClassOf geo:Geologic_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Structure ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Structure ;
      owl:onProperty com:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Body ;
      owl:onProperty com:HostedBy ;
    ] ;
.
geo:Geologic_Time_Date
  a owl:Class ;
  rdfs:comment """A temporal coordinate value, located either by a point position (with uncertainty) on a time line, specified by a numeric coordinate (generally MYPB, but definitions of 'present' vary), or a GeochronologicBoundary if it is associated with a location in a particular stratigraphic section, or a GSSA if the numeric time coordinate is arbitrarily assigned.
Probably should specify a Temporal Reference System used to assign coordinate values."""@en ;
  rdfs:label "Geologic Time Date"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#time-interval> ;
  rdfs:subClassOf [
      a owl:Class ;
      owl:unionOf (
          [
            a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty geo:hasDate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          ]
          [
            a owl:Restriction ;
            owl:onProperty geo:isRealizedBy ;
            owl:someValuesFrom geo:Stratigraphic_Point ;
          ]
        ) ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom com:Statistical_Calculation ;
      owl:onProperty com:hasProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom com:uom ;
      owl:onProperty com:hasUOM ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:double ;
      owl:onProperty geo:hasUncertainty ;
    ] ;
  owl:disjointWith geo:GSSA ;
  owl:disjointWith geo:Geologic_Time_Interval ;
.
geo:Geologic_Time_Interval
  a owl:Class ;
  rdfs:label "Geologic Time Interval"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#time-interval> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty geo:nextTimeInterval ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty geo:previousTimeInterval ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Scale ;
      owl:onProperty geo:isPartOfTimeScale ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasOlderDate ;
      owl:someValuesFrom geo:Geologic_Time_Date ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasYoungerDate ;
      owl:someValuesFrom geo:Geologic_Time_Date ;
    ] ;
.
geo:Geologic_Time_Scale
  a owl:Class ;
  rdfs:comment "A collection of Geochronologic Age entities that obey a special topology (Cox and Richard, 2010)."@en ;
  rdfs:label "Geologic Time Scale"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#temporal-region> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:hasPart ;
      owl:someValuesFrom geo:Geologic_Time_Interval ;
    ] ;
.
geo:Geologic_Unit
  a owl:Class ;
  rdfs:comment """A geologic unit and (the amount of) its rock material are co-located, but different entities. They can share properties, e.g. porosity, thickness, density, but also have differences:

- different persistence conditions: the material can exist without the unit; e.g. a unit can be cease to exist due to natural (seismic) or artificial (explosion) means, but the material can persist
- different parthood conditions:  
-- a unit can be part of a stratigraphic lexicon, but its material cannot be part of the lexicon. 
-- parts of the unit are not parts of the material, e.g. the western arm of formation X is not a part of its sandstone - the sandstone making up the western part of X is a part of the sandstone 
-- different identity conditions: a unit can have some material added or removed and retain identity, but the material changes identity with the addition or removal of some material"""@en ;
  rdfs:label "Geologic Unit"@en ;
  rdfs:subClassOf geo:Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Unit ;
      owl:onProperty com:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Material ;
      owl:onProperty com:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Stratotype ;
      owl:onProperty geo:hasReference ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Geologic_Unit
              geo:Rock_Body_Part
            ) ;
        ] ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:Glass
  a owl:Class ;
  rdfs:comment "amorphous product of rapid coolign of melted earth material. Generally product of igneous activity, frictional melting or bolide impact."@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#amount-of-matter> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom com:Element ;
      owl:onProperty geo:hasMaterialConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Granular_Material
              geo:Rock_Material
            ) ;
        ] ;
      owl:onProperty com:isConstituentOf ;
    ] ;
.
geo:Gradational_Contact_Zone
  a owl:Class ;
  rdfs:label "Gradational Contact Zone"@en ;
  rdfs:subClassOf com:Material_Spatial_Feature ;
.
geo:Grain_Shape
  a owl:Class ;
  rdfs:label "Grain Shape"@en ;
  rdfs:subClassOf geo:Discrete_Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasTermValue ;
      owl:someValuesFrom geo:Shape ;
    ] ;
.
geo:Grain_Size
  a owl:Class ;
  rdfs:subClassOf geo:Quantified_Physical_Property ;
.
geo:Grain_Size_Average
  a owl:Class ;
  rdfs:comment "mean diameter for grain size distribution "@en ;
  rdfs:label "Grain Size Average "@en ;
  rdfs:subClassOf geo:Grain_Size ;
.
geo:Grain_Size_Max
  a owl:Class ;
  rdfs:comment "upper bound of 95% interval for grain size distribution "@en ;
  rdfs:label "Grain Size Maximum "@en ;
  rdfs:subClassOf geo:Grain_Size ;
.
geo:Grain_Size_Min
