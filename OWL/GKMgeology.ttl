@prefix : <http://geoscience.ca/3D/ontology/2019/1/geologicfeature#> .
@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix dol: <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> .

<http://geoscience.ca/3D/ontology/2019/1/geologicfeature> rdf:type owl:Ontology ;
                                                           owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ,
                                                                       <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl> ;
                                                           rdfs:comment "Base geologic feature classes and high level constraints and relationships"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://geoscience.ca/3D/ontology/2019/1/common/participantIn
com:participantIn rdf:type owl:AnnotationProperty ;
                  rdfs:domain geo:Geologic_Endurant .


#################################################################
#    Object Properties
#################################################################

###  http://geoscience.ca/3D/ontology/2019/1/common/HostedBy
com:HostedBy rdf:type owl:ObjectProperty ;
             rdfs:domain geo:Geologic_Feature ;
             rdfs:label "Hosted By"@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/determinedBy
com:determinedBy rdf:type owl:ObjectProperty ;
                 rdfs:range com:Determining_Event ;
                 rdfs:comment "link from a particular to an event that defines that particular"@en ;
                 rdfs:label "determined by "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasConstituent
com:hasConstituent rdf:type owl:ObjectProperty ;
                   rdfs:comment "leave domain and range unconstrained; would need to add more of the high level ontologic entities (Physical Endurant, Perdurant) to specify here."@en ;
                   rdfs:label "hasConstituent"@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasInput
com:hasInput rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf com:hasParticipant ;
             rdfs:comment "A participant in a process that must be present before the process can operate "@en ;
             rdfs:label "Input "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasOutput
com:hasOutput rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf com:hasParticipant ;
              rdfs:comment "A participant in a process that is produced by the action of the process and is present when the process is complete "@en ;
              rdfs:label "Output "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasPart
com:hasPart rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain geo:Geologic_Endurant ,
                        geo:Geologic_Perdurant .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasParticipant
com:hasParticipant rdf:type owl:ObjectProperty ;
                   rdfs:domain geo:Geologic_Perdurant ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( geo:Geologic_Endurant
                                              dol:endurant
                                            )
                              ] .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasProperty
com:hasProperty rdf:type owl:ObjectProperty ;
                rdfs:comment "" ;
                rdfs:label "has Property "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/hasUOM
com:hasUOM rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/common/isConstituentOf
com:isConstituentOf rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/common/isPartOf
com:isPartOf rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/common/relatedto
com:relatedto rdf:type owl:ObjectProperty ;
              rdfs:domain geo:Geologic_Endurant .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/age_covers
geo:age_covers rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf geo:temporallyRelatedTo ;
               rdfs:comment "A covers B -- A is immediately younger than B (there are no events between A and B)"@en ,
                            "One of six binary relations between geologic events defined by Burns (1975)"@en ;
               rdfs:label "age covers"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/age_equivalent_To
geo:age_equivalent_To rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf geo:temporallyRelatedTo ;
                      rdfs:comment "A is equivalent to B if they represent expressions of the same event"@en ,
                                   "One of six binary relations between geologic events defined by Burns (1975)"@en ;
                      rdfs:label "age equivalent"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/age_incomparable_to
geo:age_incomparable_to rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf geo:temporallyRelatedTo ;
                        rdfs:comment "A is incomparable to be if there is not enough information to determine an age relationship"@en ,
                                     "One of six binary relations between geolgoic events defined by Burns (1975)"@en ;
                        rdfs:label "age incomparable"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasAbstractProperty
geo:hasAbstractProperty rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf geo:hasProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasBoundaryThickness
geo:hasBoundaryThickness rdf:type owl:ObjectProperty ;
                         rdfs:domain geo:Gradational_Contact_Zone ;
                         rdfs:range geo:Thickness ;
                         rdfs:comment "thickness (length) of boundary zone at a gradational contact "@en ;
                         rdfs:label "has boundary thickness "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDisplacement
geo:hasDisplacement rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( geo:Ductile_Shear_Zone
                                                geo:Fault
                                              )
                                ] ;
                    rdfs:range geo:Displacement .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasEventHistory
geo:hasEventHistory rdf:type owl:ObjectProperty ;
                    rdfs:domain geo:Geologic_Endurant ;
                    rdfs:range geo:Geologic_Event ;
                    rdfs:label "hasEventHistory"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasMaterialConstituent
geo:hasMaterialConstituent rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf com:hasConstituent ;
                           rdfs:range geo:Rock_Material .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasMethod
geo:hasMethod rdf:type owl:ObjectProperty ;
              rdfs:domain geo:Property ;
              rdfs:label "hasMethod"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderAge
geo:hasOlderAge rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderDate
geo:hasOlderDate rdf:type owl:ObjectProperty ;
                 rdfs:range geo:Geologic_Time_Date ;
                 rdfs:label "has Older Date"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasOlderHost
geo:hasOlderHost rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf com:HostedBy .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasPhysicalProperty
geo:hasPhysicalProperty rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf geo:hasProperty ;
                        rdfs:range geo:Physical_Property .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasProperty
geo:hasProperty rdf:type owl:ObjectProperty ;
                rdfs:comment "" ;
                rdfs:label "has Property "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasReference
geo:hasReference rdf:type owl:ObjectProperty ;
                 owl:inverseOf geo:isReferenceFor .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSetting
geo:hasSetting rdf:type owl:ObjectProperty ;
               owl:inverseOf geo:isSettingFor ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( geo:Geologic_Endurant
                                           geo:Geologic_Perdurant
                                         )
                           ] ;
               rdfs:range geo:Physical_Setting ;
               rdfs:label "hasSetting"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasSpatialProperty
geo:hasSpatialProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf geo:hasProperty ;
                       rdf:type owl:InverseFunctionalProperty ;
                       rdfs:domain geo:Geologic_Endurant ;
                       rdfs:comment "Geographic disposition of an endurant."@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTemporalProperty
geo:hasTemporalProperty rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf geo:hasProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasTermValue
geo:hasTermValue rdf:type owl:ObjectProperty ;
                 rdfs:range com:Categorical_Value .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUOM
geo:hasUOM rdf:type owl:ObjectProperty ;
           rdfs:range com:uom .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerAge
geo:hasYoungerAge rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerDate
geo:hasYoungerDate rdf:type owl:ObjectProperty ;
                   rdfs:range geo:Geologic_Time_Date ;
                   rdfs:comment "assign a younger age boundary for a time interval"@en ;
                   rdfs:label "has Younger Date"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasYoungerHost
geo:hasYoungerHost rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf com:HostedBy .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isManifestationOf
geo:isManifestationOf rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isPartOfTimeScale
geo:isPartOfTimeScale rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf com:isPartOf ;
                      rdfs:range geo:Geologic_Time_Scale ;
                      rdfs:comment "" ;
                      rdfs:label "in Time Scale "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isPropertyOf
geo:isPropertyOf rdf:type owl:ObjectProperty ;
                 rdfs:domain geo:Property ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( geo:Geologic_Endurant
                                            geo:Geologic_Perdurant
                                          )
                            ] ;
                 rdfs:label "isPropertyOf"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isRealizedBy
geo:isRealizedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf geo:realizes .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isReferenceFor
geo:isReferenceFor rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/isSettingFor
geo:isSettingFor rdf:type owl:ObjectProperty ;
                 rdfs:domain geo:Physical_Setting ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( geo:Geologic_Endurant
                                            geo:Geologic_Perdurant
                                          )
                            ] .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/manifests
geo:manifests rdf:type owl:ObjectProperty .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/nextTimeInterval
geo:nextTimeInterval rdf:type owl:ObjectProperty ;
                     rdfs:comment "" ;
                     rdfs:label "next time interval "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/not_older_than
geo:not_older_than rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf geo:temporallyRelatedTo ;
                   rdfs:label "A is either younger than or equivalent to B"@en ,
                              "One of six binary relations between geolgoic events defined by Burns (1975)"@en ,
                              "not older than"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/older_than
geo:older_than rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf geo:temporallyRelatedTo ;
               rdfs:comment "A is older than B if A occurred before B"@en ,
                            "One of six binary relations between geolgoic events defined by Burns (1975)"@en ;
               rdfs:label "older than"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/overlies
geo:overlies rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf com:relatedto ;
             rdfs:domain geo:Geologic_Unit ;
             rdfs:range geo:Geologic_Unit ;
             rdfs:comment "relationship between striatified geologic units "@en ;
             rdfs:label "overlies "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/previousTimeInterval
geo:previousTimeInterval rdf:type owl:ObjectProperty ;
                         rdfs:comment "" ;
                         rdfs:label "previous time interval "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionFor
geo:proportionFor rdf:type owl:ObjectProperty ;
                  rdfs:range geo:Geologic_Endurant .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/proportionOf
geo:proportionOf rdf:type owl:ObjectProperty ;
                 rdfs:range geo:Geologic_Endurant .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/realizes
geo:realizes rdf:type owl:ObjectProperty ;
             rdfs:label "realizes"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/temporallyRelatedTo
geo:temporallyRelatedTo rdf:type owl:ObjectProperty ;
                        rdfs:range geo:Geologic_Event .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/underlies
geo:underlies rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf com:relatedto ;
              rdfs:domain geo:Geologic_Unit ;
              rdfs:range geo:Geologic_Unit ;
              rdfs:comment "relationship between stratified geologic units "@en ;
              rdfs:label "underlies "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/within
geo:within rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf com:relatedto ;
           rdfs:domain geo:Geologic_Unit ;
           rdfs:range geo:Geologic_Unit ;
           rdfs:comment "relationship between stratified geologic units "@en ;
           rdfs:label "within "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/younger_than
geo:younger_than rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf geo:temporallyRelatedTo ;
                 rdfs:comment "A is younger than B if B occurred before A"@en ,
                              "One of six binary relations between geolgoic events defined by Burns (1975)"@en ;
                 rdfs:label "younger than"@en .


#################################################################
#    Data properties
#################################################################

###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasDate
geo:hasDate rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf geo:hasNumericValue ;
            rdfs:comment "numeric value for a time coordinate. Associated hasUOM property asserts the units of measure; the temporal reference system used to assert dates should be explained in metadata for the data instances."@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasNumericValue
geo:hasNumericValue rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:decimal ;
                    rdfs:label "has numeric value"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/hasUncertainty
geo:hasUncertainty rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf geo:hasNumericValue ;
                   rdfs:comment "numeric assertion of the one-side confidence interval for a measured value. The statistic used to assert this uncertainty interval should be explained in the metadata for instances using this property."@en .


#################################################################
#    Classes
#################################################################

###  http://geoscience.ca/3D/ontology/2019/1/common/Abstract_Property
com:Abstract_Property rdf:type owl:Class ;
                      rdfs:subClassOf geo:Property ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty com:hasProperty ;
                                        owl:allValuesFrom com:Abstract_Property
                                      ] .


###  http://geoscience.ca/3D/ontology/2019/1/common/Amount_Of_Material
com:Amount_Of_Material rdf:type owl:Class ;
                       owl:deprecated "true"^^xsd:boolean .


###  http://geoscience.ca/3D/ontology/2019/1/common/Any_Color
com:Any_Color rdf:type owl:Class .


###  http://geoscience.ca/3D/ontology/2019/1/common/Calculation
com:Calculation rdf:type owl:Class ;
                rdfs:subClassOf com:Determining_Event ;
                rdfs:comment "A determination of the value of some property using analytical methods and other data input "@en ;
                rdfs:label "Calculation "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Categorical_Value
com:Categorical_Value rdf:type owl:Class ;
                      rdfs:subClassOf dol:physical-region .


###  http://geoscience.ca/3D/ontology/2019/1/common/Complex_Feature
com:Complex_Feature rdf:type owl:Class ;
                    rdfs:subClassOf com:Part_Feature ;
                    rdfs:comment "" ;
                    rdfs:label "Complex Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Determining_Event
com:Determining_Event rdf:type owl:Class ;
                      rdfs:subClassOf dol:event ;
                      rdfs:comment "An observation, calculation, or inference envent used to determine the value of one or more properties"@en ;
                      rdfs:label "Determining Event "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Element
com:Element rdf:type owl:Class .


###  http://geoscience.ca/3D/ontology/2019/1/common/Immaterial_Feature
com:Immaterial_Feature rdf:type owl:Class ;
                       rdfs:subClassOf dol:feature ;
                       rdfs:comment "" ;
                       rdfs:label "Immaterial_Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Inference
com:Inference rdf:type owl:Class ;
              rdfs:subClassOf com:Determining_Event ;
              rdfs:comment "An indirect determination of the value of some property based on logical reasoning from other data "@en ;
              rdfs:label "Inference "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Inherent_Feature
com:Inherent_Feature rdf:type owl:Class ;
                     rdfs:subClassOf dol:feature ;
                     rdfs:comment "" ;
                     rdfs:label "Inherent Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Integral_Feature
com:Integral_Feature rdf:type owl:Class ;
                     rdfs:subClassOf dol:feature ;
                     rdfs:comment "Integral features are dissective - every part of a foliation is also a foliation (at a scale of observation). "@en ;
                     rdfs:label "Integral Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Low_Dimension_Feature
com:Low_Dimension_Feature rdf:type owl:Class ;
                          rdfs:subClassOf com:Immaterial_Feature ;
                          rdfs:comment "" ;
                          rdfs:label "Low Dimension Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Material_Spatial_Feature
com:Material_Spatial_Feature rdf:type owl:Class ;
                             rdfs:subClassOf com:Spatial_Feature ;
                             rdfs:comment "Material Place Feature (MPF): A dependent place feature that is material.  - my house's backyard, frontyard.   LOGIC:  MPF(x) <--> DPF(x) ^ mat(x)"@en ;
                             rdfs:label "Material Spatial Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Morphologic_Feature
com:Morphologic_Feature rdf:type owl:Class .


###  http://geoscience.ca/3D/ontology/2019/1/common/Observation
com:Observation rdf:type owl:Class ;
                rdfs:subClassOf com:Determining_Event ;
                rdfs:comment "A direct determination of the value of some property "@en ;
                rdfs:label "Observation "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Part_Feature
com:Part_Feature rdf:type owl:Class ;
                 rdfs:subClassOf dol:feature ;
                 rdfs:comment "" ;
                 rdfs:label "Part Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Physical_Boundary
com:Physical_Boundary rdf:type owl:Class ;
                      rdfs:subClassOf com:Part_Feature .


###  http://geoscience.ca/3D/ontology/2019/1/common/Physical_Void
com:Physical_Void rdf:type owl:Class ;
                  rdfs:subClassOf com:Place_Feature ;
                  rdfs:comment "" ;
                  rdfs:label "Physical Void "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Place_Feature
com:Place_Feature rdf:type owl:Class ;
                  rdfs:subClassOf dol:feature ;
                  rdfs:comment "" ;
                  rdfs:label "Place Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Process
com:Process rdf:type owl:Class .


###  http://geoscience.ca/3D/ontology/2019/1/common/Spatial_Extent
com:Spatial_Extent rdf:type owl:Class ;
                   rdfs:comment "renamed from SpatialEntity in previous version"@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Spatial_Feature
com:Spatial_Feature rdf:type owl:Class ;
                    rdfs:subClassOf dol:feature ;
                    rdfs:comment "" ;
                    rdfs:label "Spatial Feature "@en .


###  http://geoscience.ca/3D/ontology/2019/1/common/Statistical_Calculation
com:Statistical_Calculation rdf:type owl:Class .


###  http://geoscience.ca/3D/ontology/2019/1/common/uom
com:uom rdf:type owl:Class ;
        rdfs:comment "PRobably need to specialize for units of length, mass, time, etc.  Use QUDT unit types? (http://www.qudt.org/doc/2017/DOC_SCHEMA-QUDT-SCIENCE-v2.0.html)"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature#Bedding_Package
:Bedding_Package rdf:type owl:Class ;
                 rdfs:subClassOf geo:Lithosome ;
                 rdfs:comment "A sub-map scale sequence of strata, e.g. bouma sequence, fining-upward sequence, interbedded sandstone and mudstone... "@en ;
                 rdfs:label "Bedding Package "@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature#Geologic_Age_Instant
:Geologic_Age_Instant rdf:type owl:Class ;
                      rdfs:subClassOf geo:Geologic_Age ;
                      rdfs:comment "A Geologic age that is specified by either a numeric temporal coordinate or correlation with a stratigraphic point via GeochronologicBoundary/isRealizedBy/StratigraphicPoint"@en ;
                      rdfs:label "Geologic Age Instant"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Age_Rank
geo:Age_Rank rdf:type owl:Class ;
             rdfs:subClassOf geo:Geologic_Time_Interval ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty com:isPartOf ;
                               owl:allValuesFrom geo:Epoch
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty com:hasPart ;
                               owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                               owl:onClass geo:Geologic_Time_Interval
                             ] ;
             rdfs:comment "most granular geochronologic time interval; one or more Ages are grouped into an Epoch"@en ;
             rdfs:label "Age (time scale rank)"@en .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Alluvial_Fan_Setting
geo:Alluvial_Fan_Setting rdf:type owl:Class ;
                         rdfs:subClassOf geo:Physical_Setting .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Alteration_Zone
geo:Alteration_Zone rdf:type owl:Class ;
                    rdfs:subClassOf geo:Geologic_Unit .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Amount
geo:Amount rdf:type owl:Class ;
           rdfs:subClassOf geo:Quantified_Physical_Property .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bed
geo:Bed rdf:type owl:Class ;
        rdfs:subClassOf geo:Lithostratigraphic_Unit .


###  http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Bedding
geo:Bedding rdf:type owl:Class ;
            rdfs:subClassOf geo:Foliation ;
            rdfs:label "Bedding"@en .


