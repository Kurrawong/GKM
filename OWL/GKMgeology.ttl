# baseURI: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://geoscience.ca/3D/ontology/2019/1/common
# imports: http://www.ontologydesignpatterns.org/ont/dul/DLP_397.owl

@prefix : <http://geoscience.ca/3D/ontology/2019/1/geologicfeature#> .
@prefix DLP_397: <http://www.ontologydesignpatterns.org/ont/dlp/DLP_397.owl#> .
@prefix com: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix geo: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

com:Abstract_Property
  rdfs:subClassOf geo:Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom com:Abstract_Property ;
      owl:onProperty com:hasProperty ;
    ] ;
.
com:Amount_Of_Material
  owl:deprecated true ;
.
com:Amount_Of_Matter
  owl:deprecated true ;
.
com:Calculation
  a owl:Class ;
  rdfs:comment "A determination of the value of some property using analytical methods and other data input "@en ;
  rdfs:label "Calculation "@en ;
  rdfs:subClassOf com:Determining_Event ;
.
com:Categorical_Value
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#physical-region> ;
.
com:Complex_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Complex Feature "@en ;
  rdfs:subClassOf com:Part_Feature ;
.
com:Determining_Event
  a owl:Class ;
  rdfs:comment "An observation, calculation, or inference envent used to determine the value of one or more properties"@en ;
  rdfs:label "Determining Event "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#event> ;
.
com:HostedBy
  rdfs:domain geo:Geologic_Feature ;
  rdfs:label "Hosted By"@en ;
.
com:Immaterial_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Immaterial_Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:Inference
  a owl:Class ;
  rdfs:comment "An indirect determination of the value of some property based on logical reasoning from other data "@en ;
  rdfs:label "Inference "@en ;
  rdfs:subClassOf com:Determining_Event ;
.
com:Inherent_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Inherent Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:Integral_Feature
  a owl:Class ;
  rdfs:comment "Integral features are dissective - every part of a foliation is also a foliation (at a scale of observation). "@en ;
  rdfs:label "Integral Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:Low_Dimension_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Low Dimension Feature "@en ;
  rdfs:subClassOf com:Immaterial_Feature ;
.
com:Material_Spatial_Feature
  a owl:Class ;
  rdfs:comment "Material Place Feature (MPF): A dependent place feature that is material.  - my house's backyard, frontyard.   LOGIC:  MPF(x) <--> DPF(x) ^ mat(x)"@en ;
  rdfs:label "Material Spatial Feature "@en ;
  rdfs:subClassOf com:Spatial_Feature ;
.
com:Measurement_Method
  rdfs:label "Measurement Method"@en ;
.
com:Observation
  a owl:Class ;
  rdfs:comment "A direct determination of the value of some property "@en ;
  rdfs:label "Observation "@en ;
  rdfs:subClassOf com:Determining_Event ;
.
com:Part_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Part Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:Physical_Boundary
  rdfs:subClassOf com:Part_Feature ;
.
com:Physical_Void
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Physical Void "@en ;
  rdfs:subClassOf com:Place_Feature ;
.
com:Place_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Place Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:Spatial_Extent
  rdfs:comment "renamed from SpatialEntity in previous version"@en ;
.
com:Spatial_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Spatial Feature "@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#feature> ;
.
com:State_Of_Matter
  rdfs:label "State of Matter"@en ;
.
com:determinedBy
  a rdf:Property ;
  rdfs:comment "link from a particular to an event that defines that particular"@en ;
  rdfs:label "determined by "@en ;
  rdfs:range com:Determining_Event ;
.
com:hasConstituent
  rdfs:comment "leave domain and range unconstrained; would need to add more of the high level ontologic entities (Physical Endurant, Perdurant) to specify here."@en ;
  rdfs:label "hasConstituent"@en ;
.
com:hasInput
  a owl:ObjectProperty ;
  rdfs:comment "A participant in a process that must be present before the process can operate "@en ;
  rdfs:label "Input "@en ;
  rdfs:subPropertyOf com:hasParticipant ;
.
com:hasOutput
  a owl:ObjectProperty ;
  rdfs:comment "A participant in a process that is produced by the action of the process and is present when the process is complete "@en ;
  rdfs:label "Output "@en ;
  rdfs:subPropertyOf com:hasParticipant ;
.
com:hasPart
  a owl:ObjectProperty ;
  a owl:TransitiveProperty ;
  rdfs:domain geo:Geologic_Endurant ;
  rdfs:domain geo:Geologic_Perdurant ;
.
com:hasParticipant
  rdfs:domain geo:Geologic_Perdurant ;
  rdfs:range [
      a owl:Class ;
      owl:unionOf (
          geo:Geologic_Endurant
          <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#endurant>
        ) ;
    ] ;
.
com:hasProperty
  a rdf:Property ;
  rdfs:comment "" ;
  rdfs:label "has Property "@en ;
.
com:participantIn
  rdfs:domain geo:Geologic_Endurant ;
.
com:relatedto
  rdfs:domain geo:Geologic_Endurant ;
.
com:uom
  rdfs:comment "PRobably need to specialize for units of length, mass, time, etc.  Use QUDT unit types? (http://www.qudt.org/doc/2017/DOC_SCHEMA-QUDT-SCIENCE-v2.0.html)"@en ;
.
<http://geoscience.ca/3D/ontology/2019/1/geologicfeature>
  a owl:Ontology ;
  rdfs:comment "Base geologic feature classes and high level constraints and relationships"@en ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/common> ;
  owl:imports <http://www.ontologydesignpatterns.org/ont/dul/DLP_397.owl> ;
.
:Bedding_Package
  a owl:Class ;
  rdfs:comment "A sub-map scale sequence of strata, e.g. bouma sequence, fining-upward sequence, interbedded sandstone and mudstone... "@en ;
  rdfs:label "Bedding Package "@en ;
  rdfs:subClassOf geo:Lithosome ;
.
:Geologic_Age_Instant
  a owl:Class ;
  rdfs:comment "A Geologic age that is specified by either a numeric temporal coordinate or correlation with a stratigraphic point via GeochronologicBoundary/isRealizedBy/StratigraphicPoint"@en ;
  rdfs:label "Geologic Age Instant"@en ;
  rdfs:subClassOf geo:Geologic_Age ;
.
geo:Age_Rank
  a owl:Class ;
  rdfs:comment "most granular geochronologic time interval; one or more Ages are grouped into an Epoch"@en ;
  rdfs:label "Age (time scale rank)"@en ;
  rdfs:subClassOf geo:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Epoch ;
      owl:onProperty com:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass geo:Geologic_Time_Interval ;
      owl:onProperty com:hasPart ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
.
geo:Alluvial_Fan_Setting
  a owl:Class ;
  rdfs:subClassOf geo:Physical_Setting ;
.
geo:Alteration_Zone
  a owl:Class ;
  rdfs:subClassOf geo:Geologic_Unit ;
.
geo:Amount
  a owl:Class ;
  rdfs:subClassOf geo:Quantified_Physical_Property ;
.
geo:Bed
  a owl:Class ;
  rdfs:subClassOf geo:Lithostratigraphic_Unit ;
.
geo:Bedding
  a owl:Class ;
  rdfs:label "Bedding"@en ;
  rdfs:subClassOf geo:Foliation ;
.
geo:Boundary_Stratotype
  a owl:Class ;
  rdfs:comment "A stratigraphic section that contains a designated point in a stratigraphic sequence of essentially continuous deposition, preferably marine, chosen for its correlation potential. http://www.stratigraphy.org/upload/bak/chron.htm"@en ;
  rdfs:label "Boundary Stratotype"@en ;
  rdfs:subClassOf geo:Stratotype ;
.
geo:Brittle_Deformation
  a owl:Class ;
  rdfs:label "Brittle Deformation"@en ;
  rdfs:subClassOf geo:Deformation ;
.
geo:Chronostratigraphic_Age_Interval
  a owl:Class ;
  rdfs:subClassOf geo:Geologic_Age ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty geo:hasOlderAge ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Interval ;
      owl:onProperty geo:hasYoungerAge ;
    ] ;
.
geo:Chronostratigraphic_Unit
  a owl:Class ;
  rdfs:comment """A body of rocks that includes all rocks, layered or unlayered, formed during a specific interval of geologic time, and only those rocks formed during that time span. Chronostratigraphic units are bounded by synchronous horizons.

The rank and relative magnitude of the units in the chronostratigraphic hierarchy are a function of the length of the time interval that their rocks subtend, rather than of their physical thickness.

(http://www.stratigraphy.org/upload/bak/chron.htm)"""@en ;
  rdfs:label "Chronostratigraphic unit"@en ;
  rdfs:subClassOf geo:Geologic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Geologic_Time_Interval ;
    ] ;
.
geo:Clast
  a owl:Class ;
  rdfs:label "Clast"@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Clastic_Granular_Material
  a owl:Class ;
  rdfs:comment """Use for clastic sedimentary components --conglomerate clast types, sand components, clay matrix. 
Also for deformation-producted clasts in tectonic breccia."""@en ;
  rdfs:subClassOf geo:Granular_Material ;
.
geo:Clay_Mineral
  a owl:Class ;
  rdfs:comment "general class for all clay minerals "@en ;
  rdfs:label "Clay Mineral "@en ;
  rdfs:subClassOf geo:Mineral ;
.
geo:Complex
  a owl:Class ;
  rdfs:label "Complex"@en ;
  rdfs:subClassOf geo:Lithostratigraphic_Unit ;
.
geo:Complex_Physical_Property
  a owl:Class ;
  rdfs:subClassOf geo:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Physical_Property ;
      owl:onProperty geo:hasPhysicalProperty ;
    ] ;
.
geo:Conglomeratic_Sandstone
  a owl:Class ;
  rdfs:comment "coarse grained to conglomeratic sandstone "@en ;
  rdfs:label "Conglomeratic Sandstone "@en ;
  rdfs:subClassOf geo:Rock_Material ;
.
geo:Contact
  a owl:Class ;
  rdfs:comment """Contact (CT): Feature hosted by 2 or more rock bodies that touch; not a part of any of the touching rock bodies. A contact is realizedBy 2 Rock Body Surfaces; each is a part of a different Rock Body host that is in contact. 
Contact(x) <--> exists y,z [host(y,x) ^ host(z,x) ^ touches (y,z) ^ RBS (y) ^ RBS (z)]"""@en ;
  rdfs:label "Contact"@en ;
  rdfs:subClassOf com:Low_Dimension_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty geo:hasDisplacement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass geo:Rock_Body ;
      owl:onProperty com:HostedBy ;
    ] ;
.
geo:Contact_Metamorphism
  a owl:Class ;
  rdfs:label "Contact Metamorphism"@en ;
  rdfs:subClassOf geo:Metamorphism ;
.
geo:Crystallized_Granular_Material
  a owl:Class ;
  rdfs:comment "use to describe crystalline grain types in igneous, metamorphic and metasomatic rocks, e.g. the plagioclase phenocrysts, groundmass quartz, biotite phenocrysts, Andalusite porphyroblasts..."@en ;
  rdfs:subClassOf geo:Granular_Material ;
.
geo:Deformation
  a owl:Class ;
  rdfs:label "Deformation"@en ;
  rdfs:subClassOf geo:Geologic_Process ;
.
geo:Deformation_Unit
  a owl:Class ;
  rdfs:subClassOf geo:Tectonic_Unit ;
.
geo:Deposition
  a owl:Class ;
  rdfs:subClassOf geo:Geologic_Process ;
.
geo:Diorite
  a owl:Class ;
  rdfs:label "Diorite" ;
  rdfs:subClassOf geo:Rock_Material ;
.
geo:Discrete_Physical_Property
  a owl:Class ;
  rdfs:comment "A physical property that is quantified by a category from a classification scheme."@en ;
  rdfs:label "Discrete Physical Property"@en ;
  rdfs:subClassOf geo:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasTermValue ;
      owl:someValuesFrom com:Categorical_Value ;
    ] ;
.
geo:Displacement
  a owl:Class ;
  rdfs:subClassOf geo:Complex_Physical_Property ;
.
geo:Ductile_Deformation
  a owl:Class ;
  rdfs:label "Ductile Deformation"@en ;
  rdfs:subClassOf geo:Deformation ;
.
geo:Ductile_Shear_Zone
  a owl:Class ;
  rdfs:label "Ductile Shear Zone"@en ;
  rdfs:subClassOf com:Material_Spatial_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Ductile_Deformation ;
      owl:onProperty com:participantIn ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:participantIn ;
      owl:someValuesFrom geo:Ductile_Deformation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasDisplacement ;
      owl:someValuesFrom geo:Displacement ;
    ] ;
.
geo:Earth_Material_Color
  a owl:Class ;
  rdfs:comment "Property for specification of color of rocks, minerals, other Earth Material "@en ;
  rdfs:label "Earth Material Color"@en ;
  rdfs:subClassOf geo:Discrete_Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Endurant ;
      owl:onProperty geo:isPropertyOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasTermValue ;
      owl:someValuesFrom com:Any_Color ;
    ] ;
.
geo:Eon
  a owl:Class ;
  rdfs:comment "How will this be defined?  Must contain at least one Era and have duration longer than some Era?"@en ;
  rdfs:subClassOf geo:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Eon ;
      owl:onProperty com:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Era
              geo:Eon
            ) ;
        ] ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:Eonothem
  a owl:Class ;
  rdfs:subClassOf geo:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Eon ;
    ] ;
.
geo:Epoch
  a owl:Class ;
  rdfs:comment "An Epoch may be subdivided into Ages. Epochs are grouped into Periods. Example epochs: Eocene, Pleistocene"@en ;
  rdfs:label "Epoch"@en ;
  rdfs:subClassOf geo:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Age_Rank ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:Era
  a owl:Class ;
  rdfs:comment "Named rank for geochronologic time interval. An Era may be subdivided into Periods. Eras are grouped into Eons"@en ;
  rdfs:subClassOf geo:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Period ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:Erathem
  a owl:Class ;
  rdfs:subClassOf geo:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Era ;
    ] ;
.
geo:Erosion
  a owl:Class ;
  rdfs:label "Erosion" ;
  rdfs:subClassOf geo:Geologic_Process ;
.
geo:Esker
  a owl:Class ;
  rdfs:comment "a long ridge of gravel and other sediment, typically having a winding course, deposited by meltwater from a retreating glacier or ice sheet (https://www.lexico.com/en/definition/esker). An example of an Indissective Morphological Feature. "@en ;
  rdfs:label "Esker "@en ;
  rdfs:subClassOf com:Morphologic_Feature ;
.
geo:Facies
  a owl:Class ;
  rdfs:label "Facies"@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Fault
  a owl:Class ;
  rdfs:label "Fault"@en ;
  rdfs:subClassOf com:Low_Dimension_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Deformation ;
      owl:onProperty com:participantIn ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass geo:Displacement ;
      owl:onProperty geo:hasDisplacement ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
geo:Fault_Zone
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Fault Zone "@en ;
  rdfs:subClassOf com:Material_Spatial_Feature ;
.
geo:Faulting
  a owl:Class ;
  rdfs:subClassOf geo:Deformation ;
.
geo:Feldspar
  a owl:Class ;
  rdfs:label "Feldspar "@en ;
  rdfs:subClassOf geo:Mineral ;
.
geo:Fine_Grained_Sandstone
  a owl:Class ;
  rdfs:comment "fine-grained sandstone "@en ;
  rdfs:label "Fine-grained Sandstone "@en ;
  rdfs:subClassOf geo:Rock_Material ;
.
geo:Fold
  a owl:Class ;
  rdfs:label "Fold"@en ;
  rdfs:subClassOf com:Morphologic_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
.
geo:Folding
  a owl:Class ;
  rdfs:subClassOf geo:Deformation ;
.
geo:Foliation
  a owl:Class ;
  rdfs:label "Foliation"@en ;
  rdfs:subClassOf com:Integral_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
.
geo:Formation
  a owl:Class ;
  rdfs:subClassOf geo:Lithostratigraphic_Unit ;
.
geo:Fossil
  a owl:Class ;
  rdfs:label "Fossil"@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Fossil_Granular_Material
  a owl:Class ;
  rdfs:comment "use to describe body fossil components in a fossiliferous rock, e.g. crinoid columnals, radiolaria in chert, ammonites, shell hash...."@en ;
  rdfs:subClassOf geo:Granular_Material ;
.
geo:Fracture
  a owl:Class ;
  rdfs:label "Fracture"@en ;
  rdfs:subClassOf com:Low_Dimension_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Displacement ;
      owl:onProperty geo:hasDisplacement ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:participantIn ;
      owl:someValuesFrom geo:Brittle_Deformation ;
    ] ;
.
geo:GSSA
  a owl:Class ;
  rdfs:comment "A Geochronologic Age defined by the International Stratigraphic Commission, based on fiat selection of defined older and younger bounding time coordinates"@en ;
  rdfs:label "GSSA"@en ;
  rdfs:subClassOf geo:Geologic_Time_Date ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onDataRange xsd:double ;
      owl:onProperty geo:hasUncertainty ;
    ] ;
.
geo:GSSP
  a owl:Class ;
  rdfs:comment "A stratigraphic point that is hosted by a top and bottom segment of adjacent chronostratigraphic units. The top and bottom are part of an outcrop and part of a stratotype (type section) for the unit. "@en ;
  rdfs:label "GSSP "@en ;
  rdfs:subClassOf geo:Stratigraphic_Point ;
.
geo:Geochronologic_Age_Interval
  a owl:Class ;
  rdfs:subClassOf geo:Geologic_Age ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Date ;
      owl:onProperty geo:hasOlderDate ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Time_Date ;
      owl:onProperty geo:hasYoungerDate ;
    ] ;
.
geo:Geochronologic_Boundary
  a owl:Class ;
  rdfs:label "Geochronologic Boundary"@en ;
  rdfs:subClassOf geo:Geologic_Time_Date ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Stratigraphic_Point ;
      owl:onProperty geo:isRealizedBy ;
    ] ;
.
geo:Geologic_Age
  a owl:Class ;
  rdfs:subClassOf geo:Temporal_Property ;
.
geo:Geologic_Endurant
  a owl:Class ;
  rdfs:comment "A Physical Endurant that is part of the Earth."@en ;
  rdfs:label "Geologic Endurant"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#endurant> ;
.
geo:Geologic_Event
  a owl:Class ;
  rdfs:comment """If an event is caused by a process, then any restrictions on participants of the process also apply to participants of the event

causedBy (x,y) ^ GeologicEvent (x) ^ GeologicProcess (y) --> forall z  [participates(z,x) --> participates (z,y)]"""@en ;
  rdfs:comment "TBD: Sub class from dolce:event, but need to verify that this doesn't carry any undesirable implications"@en ;
  rdfs:label "Geologic Event"@en ;
  rdfs:subClassOf geo:Geologic_Perdurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Age ;
      owl:onProperty geo:hasTemporalProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Process ;
      owl:onProperty geo:isManifestationOf ;
    ] ;
.
geo:Geologic_Feature
  a owl:Class ;
  rdfs:comment """Geologic Feature (GF):
Geologic features that are specifially dependent on their host -  that is, the host relation is a subrelation of specific dependence.

GF(x) <--> exists y [hasHost(y,x)] and geologic(x)"""@en ;
  rdfs:comment "TBD: This should subtype from dolce:feature; need to check whether this brings unwanted implications"@en ;
  rdfs:label "Geologic Feature"@en ;
  rdfs:subClassOf geo:Geologic_Endurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:HostedBy ;
      owl:someValuesFrom geo:Rock_Body ;
    ] ;
.
geo:Geologic_Material
  a owl:Class ;
  rdfs:label "Geologic Material deprecated"@en ;
  rdfs:subClassOf com:Amount_Of_Material ;
  rdfs:subClassOf geo:Geologic_Endurant ;
  owl:deprecated true ;
.
geo:Geologic_Perdurant
  a owl:Class ;
  rdfs:comment """TBD: this class maps to dolce:Perdurant with properties: 
has-quality only temporal-quality;
has-quality some temporal-location_q;
part only perdurant;
participant only endurant;
specific-constant-constituent only perdurant."""@en ;
  rdfs:label "Geologic Perdurant"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#perdurant> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Perdurant ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Perdurant ;
      owl:onProperty geo:temporallyRelatedTo ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:hasParticipant ;
      owl:someValuesFrom geo:Geologic_Endurant ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasTemporalProperty ;
      owl:someValuesFrom geo:Temporal_Property ;
    ] ;
.
geo:Geologic_Process
  a owl:Class ;
  rdfs:label "Geologic Process"@en ;
  rdfs:subClassOf com:Process ;
  rdfs:subClassOf geo:Geologic_Perdurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Age ;
      owl:onProperty geo:hasTemporalProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Geologic_Event ;
      owl:onProperty geo:manifests ;
    ] ;
.
geo:Geologic_Structure
  a owl:Class ;
