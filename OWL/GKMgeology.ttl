      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Group
              geo:Complex
              geo:Formation
            ) ;
        ] ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:System
  a owl:Class ;
