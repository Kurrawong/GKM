  rdfs:label "Granitoid" ;
  rdfs:subClassOf geo:Rock_Material ;
.
geo:Granular_Material
  a owl:Class ;
  rdfs:comment "Represents a rock body constituent composed of particles that share a set of characteristics, e.g. particle size (distribution), mineralogy, shape.  E.g. the sand that is a constituent in a sandstone, or the feldspar phenocrysts that are a constituent in a granite"@en ;
  rdfs:comment "example top level subclasses shown (probably not complete); differentiated based on genesis for the most part."@en ;
  rdfs:label "Granular Material"@en ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#amount-of-matter> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              com:Element
              geo:Glass
              geo:Mineral
            ) ;
        ] ;
      owl:onProperty geo:hasMaterialConstituent ;
    ] ;
.
geo:Group
  a owl:Class ;
  rdfs:subClassOf geo:Lithostratigraphic_Unit ;
.
geo:Inclusion
  a owl:Class ;
