  rdfs:label "Sharp Contact"@en ;
  rdfs:subClassOf geo:Contact ;
.
geo:Specimen
  a owl:Class ;
  rdfs:label "Rock Material Specimen"@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Stage
  a owl:Class ;
  rdfs:comment "The stage includes all rocks formed during an age. A stage is normally the lowest ranking unit in the chronostratigraphic hierarchy that can be recognized on a global scale. The stage has been called the basic working unit of chronostratigraphy because it is suited in scope and rank to the practical needs and purposes of intraregional chronostratigraphic classification. It is a subdivision of a series."@en ;
  rdfs:subClassOf geo:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:isPartOf ;
      owl:someValuesFrom geo:Series ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Age_Rank ;
    ] ;
.
geo:Stratigraphic_Part
  a owl:Class ;
  rdfs:comment "A geologic unit part that occupies a particular stratigraphic position within a geologic unit. http://inspire.ec.europa.eu/codelist/CompositionPartRoleValue/stratigraphicPart "@en ;
  rdfs:label "Stratigraphic Part "@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Stratigraphic_Point
  a owl:Class ;
  rdfs:label "Stratigraphic Point"@en ;
  rdfs:subClassOf geo:Contact ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasReference ;
      owl:someValuesFrom geo:Stratotype ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasSpatialProperty ;
      owl:someValuesFrom com:Spatial_Extent ;
    ] ;
.
geo:Stratigraphic_Section
  a owl:Class ;
  rdfs:label "Stratigraphic Section"@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Stratotype
  a owl:Class ;
  rdfs:label "Stratotype"@en ;
  rdfs:subClassOf geo:Stratigraphic_Section ;
.
geo:Subangular
  a owl:Class ;
  rdfs:subClassOf geo:Shape ;
.
geo:Substage
  a owl:Class ;
  rdfs:subClassOf geo:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:realizes ;
      owl:someValuesFrom geo:Age_Rank ;
    ] ;
.
geo:Supereon
  a owl:Class ;
  rdfs:comment "hight ranking geochronlogic time interval, parts are eons"@en ;
  rdfs:label "Supereon"@en ;
  rdfs:subClassOf geo:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Eon ;
      owl:onProperty com:hasPart ;
    ] ;
