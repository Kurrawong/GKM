  rdfs:subClassOf geo:Granular_Material ;
.
geo:Quantified_Physical_Property
  a owl:Class ;
  rdfs:label "Numeric physical geologic property"@en ;
  rdfs:subClassOf geo:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:hasUOM ;
      owl:someValuesFrom com:uom ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasNumericValue ;
      owl:someValuesFrom xsd:decimal ;
    ] ;
.
geo:Quartz
  a owl:Class ;
  rdfs:label "Quartz "@en ;
  rdfs:subClassOf geo:Mineral ;
