geo:Rock_Body_Bottom
  a owl:Class ;
  rdfs:comment """Rock Body Bottom (RBB):
The bottom surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [isPartOf (y,z) ^ hasHost (z,x) -> above (y,x) v isPartOf (y,x)]"""@en ;
  rdfs:label "Rock Body Bottom"@en ;
  rdfs:subClassOf geo:Rock_Body_Boundary ;
.
geo:Rock_Body_Boundary
  a owl:Class ;
  rdfs:comment """a physical boundary hosted by a rock body.
RBD (x) <--> SF (x) ^ exists y [RB (y) ^ hosts (y,x)] 
The exterior-facing material of a rock body."""@en ;
  rdfs:label "Rock Body Boundary"@en ;
  rdfs:subClassOf com:Physical_Boundary ;
  rdfs:subClassOf geo:Rock_Body_Part ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Body_Boundary ;
      owl:onProperty com:hasPart ;
    ] ;
.
geo:Rock_Body_Part
  a owl:Class ;
  rdfs:label "Rock Body Part"@en ;
  rdfs:subClassOf geo:Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Body_Part ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Material ;
      owl:onProperty com:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty com:isPartOf ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Geologic_Unit
              geo:Rock_Body
            ) ;
        ] ;
    ] ;
.
geo:Rock_Body_Top
  a owl:Class ;
  rdfs:comment """Rock Body Top (RBT):
The top surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [(isPartOf (y,z) ^ hasHost (z,x)) -> (below (y,x) v isPartOf (y,x))]"""@en ;
  rdfs:label "Rock Body Top"@en ;
  rdfs:subClassOf geo:Rock_Body_Boundary ;
.
geo:Rock_Body_Void
  a owl:Class ;
  rdfs:label "Rock Body Void"@en ;
  rdfs:subClassOf com:Physical_Void ;
  rdfs:subClassOf geo:Geologic_Feature ;
.
geo:Rock_Material
  a owl:Class ;
  rdfs:comment "hook into CGI simple lithology ontology. SMR has one, and Clinton Smyth has an extension of that with more rock types."@en ;
  rdfs:label "Rock Material"@en ;
  rdfs:subClassOf geo:Rock_Body ;
  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dlp/DOLCE-Lite.owl#amount-of-matter> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom geo:Rock_Body_Part ;
      owl:onProperty com:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              com:Element
              geo:Glass
              geo:Granular_Material
              geo:Mineral
            ) ;
        ] ;
      owl:onProperty com:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              geo:Geologic_Unit
              geo:Rock_Body_Part
            ) ;
        ] ;
      owl:onProperty com:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass geo:Rock_Body ;
      owl:onProperty com:isPartOf ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
.
geo:Sandstone
  a owl:Class ;
  rdfs:comment "generic sandstone "@en ;
  rdfs:label "Sandstone "@en ;
  rdfs:subClassOf geo:Rock_Material ;
.
geo:Series
  a owl:Class ;
  rdfs:subClassOf geo:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
