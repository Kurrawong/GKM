  rdfs:subClassOf com:Integral_Feature ;
  rdfs:subClassOf geo:Geologic_Structure ;
.
geo:Lithosome
  a owl:Class ;
  rdfs:comment "A kind of rock body that has multiple occurrences in a single geologic unit. A mass of rock of uniform character, characterized by geometry, composition, and internal structure. (http://inspire.ec.europa.eu/codelist/CompositionPartRoleValue/lithosome)"@en ;
  rdfs:label "Lithosome "@en ;
  rdfs:subClassOf geo:Rock_Body_Part ;
.
geo:Lithostratigraphic_Unit
  a owl:Class ;
  rdfs:label "Lithostratigraphic Unit"@en ;
  rdfs:subClassOf geo:Geologic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty geo:hasMaterialConstituent ;
