  rdfs:label "dioritoid"@en ;
  rdfs:seeAlso lith:doleritic_rock ;
  rdfs:subClassOf lith:intermediate_composition_igneous_rock_class ;
  rdfs:subClassOf lith:phaneritic_igneous_rock_class ;
