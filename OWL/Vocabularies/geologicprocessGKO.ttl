  rdfs:label "bolide impact" ;
.
pro:bolide_impact_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "The impact of an extraterrestrial body on the surface of the earth"@en ;
  rdfs:label "bolide impact"@en ;
  rdfs:subClassOf <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process> ;
.
pro:chemical_precipitation
  a pro:chemical_precipitation_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic chemical precipitation process instance" ;
  rdfs:label "chemical precipitation" ;
.
pro:chemical_precipitation_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "The deposition of mineral matter by precipitation from solution or as a result of chemical reactions. May be sedimentary or hydrothermal."@en ;
  rdfs:label "chemical precipitation"@en ;
  rdfs:subClassOf pro:deposition_class ;
  rdfs:subClassOf pro:metamorphic_process_class ;
.
pro:chemical_weathering
  a pro:chemical_weathering_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic chemical weathering process instance" ;
  rdfs:label "chemical weathering" ;
.
pro:chemical_weathering_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "The process of weathering by which chemical reactions (hydrolysishydration, oxidation, carbonation, ion exchange, and solution) transform rocks and minerals into new chemical combinations that are stable under conditions prevailing at or near the Earth's surface, e.g. the alteration of orthoclase to kaolinite."@en ;
  rdfs:label "chemical weathering"@en ;
  rdfs:subClassOf pro:weathering_class ;
.
pro:cometary_impact
  a pro:cometary_impact_class ;
