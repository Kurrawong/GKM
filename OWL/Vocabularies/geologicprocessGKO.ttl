  rdfs:comment "generic cometary impact process instance" ;
  rdfs:label "cometary impact" ;
.
pro:cometary_impact_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "The impact of a comet on the surface of the earth"@en ;
  rdfs:label "cometary impact"@en ;
  rdfs:subClassOf pro:bolide_impact_class ;
.
