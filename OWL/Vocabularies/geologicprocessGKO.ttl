  rdfs:label "continental collision"@en ;
  rdfs:subClassOf pro:tectonic_process_class ;
.
pro:debris_flow_deposition
  a pro:debris_flow_deposition_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic debris flow deposition process instance" ;
