  a owl:NamedIndividual ;
  rdfs:comment "generic human activity process instance" ;
  rdfs:label "human activity" ;
.
pro:human_activity_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Processes of human modification of the earth to produce geologic features"@en ;
  rdfs:label "human activity"@en ;
  rdfs:subClassOf <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process> ;
.
pro:hydration
  a pro:hydration_class ;
