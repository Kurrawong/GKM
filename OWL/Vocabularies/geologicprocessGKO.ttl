  rdfs:label "excavation"@en ;
  rdfs:subClassOf pro:human_activity_class ;
.
pro:extinction
  a pro:extinction_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic extinction process instance" ;
  rdfs:label "extinction" ;
.
pro:extinction_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Process of disappearance of a species or higher taxon, so that it no longer exists anywhere or in the subsequent fossil record."@en ;
  rdfs:label "extinction"@en ;
  rdfs:subClassOf <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process> ;
.
pro:faulting
  a pro:faulting_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic faulting process instance" ;
  rdfs:label "faulting" ;
.
pro:faulting_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "The process of fracturing, frictional slip, and displacement accumulation that produces a fault"@en ;
  rdfs:label "faulting"@en ;
  rdfs:subClassOf pro:deformation_class ;
.
pro:folding
  a pro:folding_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic folding process instance" ;
  rdfs:label "folding" ;
.
pro:folding_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Deformation in which planar surfaces become regularly curviplanar surfaces with definable limbs (zones of lower curvature) and hinges (zones of higher curvature)."@en ;
  rdfs:label "folding"@en ;
  rdfs:subClassOf pro:deformation_class ;
.
pro:fracturing
  a pro:fracturing_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic fracturing process instance" ;
  rdfs:label "fracturing" ;
.
pro:fracturing_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "The formation of a surface of failure resulting from stress"@en ;
  rdfs:label "fracturing"@en ;
  rdfs:subClassOf pro:deformation_class ;
.
pro:frost_shattering
  a pro:frost_shattering_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic frost shattering process instance" ;
  rdfs:label "frost shattering" ;
.
pro:frost_shattering_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "Propagation of fractures due to expansion of freezing water in intergranular spaces and fractures in a rock body. Result is mechanical disintegration spliitting, or breakup of rock."@en ;
  rdfs:label "frost shattering"@en ;
  rdfs:subClassOf pro:physical_weathering_class ;
.
pro:geomagnetic_process
  a pro:geomagnetic_process_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic geomagnetic process process instance" ;
  rdfs:label "geomagnetic process" ;
.
pro:geomagnetic_process_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "process that results in change in Earth's magnetic field"@en ;
  rdfs:label "geomagnetic process"@en ;
  rdfs:subClassOf <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/Geologic_Process> ;
.
pro:grading
  a pro:grading_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic grading process instance" ;
  rdfs:label "grading" ;
.
pro:grading_class
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Leveling of earth surface by rearrangement of prexisting material"@en ;
  rdfs:label "grading"@en ;
  rdfs:subClassOf pro:human_activity_class ;
.
pro:haloclasty
  a pro:haloclasty_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic haloclasty process instance" ;
  rdfs:label "haloclasty" ;
.
pro:haloclasty_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "Propagation of fractures in rock due to crytallization of mineral salts (typically sodium chloride) from interstitial water, or volumetrick expansion of salts in capillaries, or hydration pressure of interstitial, trapped salts. Generally results in mechanical disintegration of the rock surface."@en ;
  rdfs:label "haloclasty"@en ;
  rdfs:subClassOf pro:physical_weathering_class ;
.
pro:hawaiian_eruption
  a pro:hawaiian_eruption_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic hawaiian eruption process instance" ;
  rdfs:label "hawaiian eruption" ;
.
pro:hawaiian_eruption_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "Eruption in which great quantities of extremely fluid basaltic lava are poured out, mainly issuing in lava fountains from fissures on the flanks of a volcano. Explosive phenomena are rare, but much spatter and scoria are piled into cones and mounds along the vents. Characteristic of shield volcanoes"@en ;
  rdfs:label "hawaiian eruption"@en ;
  rdfs:subClassOf pro:eruption_class ;
.
pro:human_activity
