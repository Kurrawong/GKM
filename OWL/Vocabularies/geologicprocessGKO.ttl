pro:accretion
  a pro:accretion_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic accretion process instance" ;
  rdfs:label "accretion" ;
.
pro:accretion_class
  a owl:Class ;
  dcterms:source "Neuendorf, K.K.E, Mehl, J.P. & Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p."@en ;
  rdfs:comment "The addition of material to a continent. Typically involves convergent or transform motion."@en ;
