.
pro:alteration
  a pro:alteration_class ;
  a owl:NamedIndividual ;
  rdfs:comment "generic alteration process instance" ;
  rdfs:label "alteration" ;
.
pro:alteration_class
  a owl:Class ;
  dcterms:source "Fettes and Desmons, 2007"@en ;
