# baseURI: http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests
# imports: http://geoscience.ca/3D/ontology/2019/1/eventenvironment
# imports: http://geoscience.ca/3D/ontology/2019/1/geologicfeature
# imports: http://purl.org/dc/elements/1.1/

@prefix : <http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests#> .
@prefix common: <http://geoscience.ca/3D/ontology/2019/1/common/> .
@prefix eventenvironment: <http://geoscience.ca/3D/ontology/2019/1/eventenvironment#> .
@prefix gkog: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature/> .
@prefix gkm: <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gkog:Earth_Interior_Setting
  a owl:Class ;
  rdfs:label "Earth Interior Setting" ;
  rdfs:subClassOf gkog:Physical_Setting ;
.
gkog:Lacustrine_Setting
  a owl:Class ;
  rdfs:label "Lacustrine Setting" ;
  rdfs:subClassOf gkog:Physical_Setting ;
.
gkog:hasTemporalProperty
  rdfs:range gkog:Geologic_Age ;
.
<http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests>
  a owl:Ontology ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/eventenvironment> ;
  owl:imports <http://geoscience.ca/3D/ontology/2019/1/geologicfeature> ;
  owl:imports <http://purl.org/dc/elements/1.1/> ;
  owl:versionIRI <http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests> ;
.
:Cretaceous90Ma
  a gkog:Geochronologic_Age_Interval ;
  gkog:hasOlderDate [
      a gkog:Geologic_Time_Date ;
      gkog:hasDate "90 Ma" ;
      gkog:hasUOM [
          a common:ma ;
        ] ;
      gkog:hasUncertainty "8" ;
    ] ;
  rdfs:label "90 Ma" ;
.
:Cretaceous_Age_date
  a gkog:Geochronologic_Age_Interval ;
  gkog:hasMethod :upbconcordantanalysis ;
  gkog:hasOlderDate :Date110Ma ;
  gkog:hasYoungerDate :Date110Ma ;
  rdfs:label "Cretaceous110AgeDate" ;
.
:Cretaceous_dike_intrusion
  a gkog:Geologic_Event ;
  gkog:hasSetting <http://resource.geosciml.org/classifier/cgi/eventenvironment/upper_continental_crustal_setting> ;
  gkog:hasSetting [
      a gkog:Earth_Interior_Setting ;
    ] ;
  gkog:hasTemporalProperty :Cretaceous90Ma ;
  gkog:isManifestationOf [
      a gkog:Intrusion ;
    ] ;
  gkog:younger_than :creteaceousintrusionevent ;
  rdfs:label "90 Ma Dike Intrusion"@en ;
.
:Date110Ma
  a gkog:Geologic_Time_Date ;
  gkog:hasDate "110" ;
  gkog:hasUOM [
      a common:ma ;
    ] ;
  gkog:hasUncertainty "9" ;
  rdfs:label "110 Ma" ;
.
:Earth_Interior_Setting
  a gkog:Physical_Setting ;
  rdfs:label "Earth Interior Setting" ;
.
:HoloceneEpoch
  a gkog:Epoch ;
  a owl:NamedIndividual ;
  rdfs:label "Holocene Epoch" ;
.
:HsGenesis
  a gkog:Geologic_Event ;
  a owl:NamedIndividual ;
  gkog:hasSetting [
      a gkog:Alluvial_Fan_Setting ;
    ] ;
  gkog:hasTemporalProperty :HoloceneEpoch ;
  gkog:isManifestationOf [
      a gkog:Deposition ;
      a owl:NamedIndividual ;
      rdfs:label "process of sedimentary deposition"@en ;
    ] ;
  gkog:younger_than :MsGenesis ;
  rdfs:comment "Deposition of Holocene sediments"@en ;
  rdfs:label "Holocene sediment deposition"@en ;
.
:JsGenesis
  a gkog:Geologic_Event ;
  gkog:hasSetting <http://resource.geosciml.org/classifier/cgi/eventenvironment/coastal_plain_setting> ;
  gkog:hasSetting <http://resource.geosciml.org/classifier/cgi/eventenvironment/inner_neritic> ;
  gkog:hasSetting <http://resource.geosciml.org/classifier/cgi/eventenvironment/shoreline_settings> ;
  gkog:hasTemporalProperty :Jurassic_Age ;
  gkog:isManifestationOf [
      a gkog:Deposition ;
    ] ;
  rdfs:label "Jurassic sediment Deposition"@en ;
.
:Jurassic_Age
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge <http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests/JurassicPeriod> ;
  gkog:hasYoungerAge <http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests/JurassicPeriod> ;
  rdfs:label "Jurassic Age" ;
.
:MioceneEpoch
  a gkog:Epoch ;
  a owl:NamedIndividual ;
  rdfs:label "Miocence Epoch" ;
.
:Miocene_Age
  a gkog:Chronostratigraphic_Age_Interval ;
  gkog:hasOlderAge :MioceneEpoch ;
  gkog:hasYoungerAge :MioceneEpoch ;
  rdfs:label "Miocene Age" ;
.
:MsGenesis
  a gkog:Geologic_Event ;
  a owl:NamedIndividual ;
  gkog:hasSetting [
      a gkog:Lacustrine_Setting ;
    ] ;
  gkog:hasTemporalProperty :Miocene_Age ;
  gkog:isManifestationOf [
      a gkog:Deposition ;
    ] ;
  gkog:younger_than :Cretaceous_dike_intrusion ;
  rdfs:label "Miocene sediment Deposition"@en ;
.
:creteaceousintrusionevent
  a gkog:Geologic_Event ;
  gkog:hasSetting <http://resource.geosciml.org/classifier/cgi/eventenvironment/middle_continental_crust_setting> ;
  gkog:hasSetting [
      a gkog:Earth_Interior_Setting ;
    ] ;
  gkog:hasTemporalProperty :Cretaceous_Age_date ;
  gkog:isManifestationOf [
      a gkog:Intrusion ;
    ] ;
  gkog:younger_than :JsGenesis ;
  rdfs:label "Cretaceous Intrusion Event" ;
.
:upbconcordantanalysis
  a common:Measurement_Method ;
  rdfs:label "U-Pb isotopic analysis, concordant" ;
.
<http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests/CretaceousPeriod>
  a gkog:Period ;
  a owl:NamedIndividual ;
  rdfs:label "Cretaceous Period" ;
.
<http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests/JurassicPeriod>
  a gkog:Period ;
  a owl:NamedIndividual ;
  rdfs:label "Jurassic Period" ;
.
<http://geoscience.ca/3D/ontology/2019/1/gkoinstancetests/MsDeposition>
  a gkog:Deposition ;
  a owl:NamedIndividual ;
.
