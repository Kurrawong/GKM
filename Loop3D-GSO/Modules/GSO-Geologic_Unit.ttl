@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gsgu: <http://loop3d.org/GSO/ontology/2020/1/geologicunit/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2020/1/geologicfeature/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2020/1/geologicproperty/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://loop3d.org/GSO/ontology/2020/1/geologicunit/ontology> .

<http://loop3d.org/GSO/ontology/2020/1/geologicunit/ontology> rdf:type owl:Ontology ;
      owl:imports gsoc:ontology ,
                  <http://loop3d.org/GSO/ontology/2020/1/geologicunit/rockbodypart/ontology> ,
                  <http://loop3d.org/GSO/ontology/2020/1/metamorphicfacies/ontology> ,
                  <http://loop3d.org/GSO/ontology/2020/1/metamorphicgrade/ontology> ;
      rdfs:comment "This file based on the CGI Geologic Unit type vocabulary. Compilation and review in MS Excel spreadsheet, converted to MS Excel for SKOS generation using GSML_SKOS_fromXLS_2016.02.xslt."@en ,
"This ontology specifies concepts for description of Geologic Unit, as defined by the IUGS Commission for Geoscience Information (CGI) Geoscience Terminology Working Group. These classes and properties extend the base GSO-Geology ontology for more detailed descriptions. Unit types are differentiated based on their defining lithological, stratigraphic, or other physical properties."@en ,
"dct:creator--  CGI Geoscience Terminology Working Group, OWL conversion by Stephen Richard"@en ,
"dct:date--  2012-11-24"@en ,
"dct:modified-- 2020-01-30"@en ;
      rdfs:label "Geologic Unit"@en .

#################################################################
#    Classes
#################################################################

###  http://loop3d.org/GSO/ontology/2020/1/geologicfeature/Geologic_Unit
gsog:Geologic_Unit rdf:type owl:Class ;
	rdfs:subClassOf [ rdf:type owl:Restriction ;
        owl:onProperty gsoc:hasQuality ;
        owl:allValuesFrom gsgu:geologicunitproperties
    ] ;
	rdfs:comment "Geologic unit properties are bound in the Geologic_Unit module"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Bedding_Pattern
gsgu:Bedding_Pattern rdf:type owl:Class ;
  rdfs:subClassOf gsoc:Quality ,
                  [ rdf:type owl:Restriction ;
 owl:onProperty gsoc:hasTextValue ;
 owl:someValuesFrom xsd:string
                  ] ;
  rdfs:comment "Term(s) specifying patterns of bedding thickness or relationships between bedding packages. (eg; thinning upward, thickening upward)"@en ;
  rdfs:isDefinedBy "GeoSciML v4"@en ;
  rdfs:label "Bedding pattern"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Bedding_Style
gsgu:Bedding_Style rdf:type owl:Class ;
rdfs:subClassOf gsoc:Property ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty gsoc:hasTextValue ;
                  owl:someValuesFrom xsd:string
                ] ;
rdfs:comment "Term(s) specifying patterns of bedding thickness or relationships between bedding packages. (eg; thinning upward, thickening upward)"@en ;
rdfs:isDefinedBy "GeoSciML v4"@en ;
rdfs:label "Bedding style"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Bedding_Thickness_Category
gsgu:Bedding_Thickness_Category rdf:type owl:Class ;
             rdfs:subClassOf gsoc:Quality ,
          [ rdf:type owl:Restriction ;
            owl:onProperty gsoc:hasTextValue ;
            owl:someValuesFrom xsd:string
          ] ;
             rdfs:comment "Terms characterizing the thickness of bedding in the unit. "@en ;
             rdfs:isDefinedBy "GeoSciML v4"@en ;
             rdfs:label "Bedding thickness"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Geologic_Unit_Body_Morphology
gsgu:Geologic_Unit_Body_Morphology rdf:type owl:Class ;
                rdfs:subClassOf gsoc:Quality ,
             [ rdf:type owl:Restriction ;
               owl:onProperty gsoc:hasTextValue ;
               owl:someValuesFrom xsd:string
             ] ;
                rdfs:comment "The geometry or form of a GeologicUnit.  Examples include: dike (dyke), cone, fan, sheet, etc. Morphology is independent of the substance (EarthMaterial) that composes the GeologicUnit or process that formed it."@en ;
                rdfs:isDefinedBy "GeoSciML v4"@en ;
                rdfs:label "Body morphology"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Geologic_Unit_Composition_Category
gsgu:Geologic_Unit_Composition_Category rdf:type owl:Class ;
  rdfs:subClassOf gsoc:Quality ,
                  [ rdf:type owl:Restriction ;
 owl:onProperty gsoc:hasTextValue ;
 owl:someValuesFrom xsd:string
                  ] ;
  rdfs:comment "Composition-based classification that requires integrating the character of the unit over large area, not applicable at the rock-material/specimen level. Examples: alkalic, subaluminous, peraluminous, I-Type, carbonate, phosphate."@en ;
  rdfs:isDefinedBy "GeoSciML v4"@en ;
  rdfs:label "Composition category"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/Geologic_Unit_Outcrop_Character
gsgu:Geologic_Unit_Outcrop_Character rdf:type owl:Class ;
                  rdfs:subClassOf gsoc:Quality ,
               [ rdf:type owl:Restriction ;
                 owl:onProperty gsoc:hasTextValue ;
                 owl:someValuesFrom xsd:string
               ] ;
                  rdfs:comment "Describes the nature of outcrops formed by a geologic unit. Examples: bouldery, cliff-forming, ledge-forming, slope-forming, poorly exposed. Assign free text string value for now, controlled vocabulary is the goal."@en ;
                  rdfs:isDefinedBy "GeoSciML v4"@en ;
                  rdfs:label "Outcrop character"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/allostratigraphic_unit
gsgu:allostratigraphic_unit rdf:type owl:Class ;
         rdfs:subClassOf gsog:Geologic_Unit ;
         rdfs:comment "Geologic unit defined by bounding surfaces. Not necessarily stratified. Donovon (2004, IUGS abstract Florence) makes good case for use of a noncommittal term for the bounding surface. While there may be no agreement that a given stratal boundary is a discontinuity, there is consensus that all the identified boundaries are stratal surfaces. Includes: 1. Unconformity bounded units (Salvador 1994), defined by bounding stratigraphic discontinuities ('significant unconformities', unconformity is defined as surface of erosion in Salvador 1994). 2. Sequence stratigraphic unit, an allostratigraphic unit that is used to interpret the depositional origin of sedimentary strata and assumes, though this is not always stated, an implicit connection to base level change. It does this by establishing how the sequence of strata accumulated in order in the sedimentary section over a subdividing framework of surfaces."@en ,
   "dct:source--  This vocabulary, Salvador, 1994, Donovon 2004 IUGS abstract."@en ;
         rdfs:label "allostratigraphic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/alteration_unit
gsgu:alteration_unit rdf:type owl:Class ;
  rdfs:subClassOf gsog:Geologic_Unit ;
  rdfs:comment "Geologic unit defined by alteration process."@en ,
               "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
  rdfs:label "alteration unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/artificial_ground
gsgu:artificial_ground rdf:type owl:Class ;
    rdfs:subClassOf gsgu:lithogenetic_unit ;
    rdfs:comment "Geologic unit defined by genesis involving direct human action to deposit or modify material."@en ,
                 "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
    rdfs:label "artificial ground"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/biostratigraphic_unit
gsgu:biostratigraphic_unit rdf:type owl:Class ;
        rdfs:subClassOf gsog:Geologic_Unit ;
        rdfs:comment "Geologic unit defined based on fossil content. Five kinds of biozones are recognized by the revised NACSN (Lenz et al., 2000, Note 64, a recommended complete replacement of Articles 48 through 54 of the North American Stratigraphic Code (NACSN, 1983) accepted for publication 2000.): range biozone, interval biozone, lineage biozone, assemblage biozone, and abundance biozone. These represent different approaches to defining and recognizing biozones."@en ,
  "dct:source--  Lenz et al., 2000, Note 64 (see AAPG Bulletin, v. 89, no. 11, November 2005), North American Stratigraphic Code, 1983"@en ;
        rdfs:label "biostratigraphic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/deformation_unit
gsgu:deformation_unit rdf:type owl:Class ;
   rdfs:subClassOf gsgu:lithotectonic_unit ;
   rdfs:comment "Lithotectonic unit defined by deformation style or characteristic geologic structure observable in outcrop."@en ,
                "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
   rdfs:label "deformation unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/excavation_unit
gsgu:excavation_unit rdf:type owl:Class ;
  rdfs:subClassOf gsgu:lithogenetic_unit ;
  rdfs:comment "Geologic unit defined by human-made genesis involving excavation. Not necessarily defined by landform (a hole...), as they could have been subsequently filled/landscaped etc. If the excavation is filled becomes an excavation with artificial ground wholly or partly superimposed on it. This sort of thing can become quite important in urban geology where an excavation can be filled and landscaped."@en ,
               "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
  rdfs:label "excavation unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/geomorphologic_unit
gsgu:geomorphologic_unit rdf:type owl:Class ;
      rdfs:subClassOf gsog:Geologic_Unit ;
      rdfs:comment "Geologic unit defined by surface landform, e.g. hummocky moraine"@en ,
"dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
      rdfs:label "geomorphologic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/geophysical_unit
gsgu:geophysical_unit rdf:type owl:Class ;
   rdfs:subClassOf gsog:Geologic_Unit ;
   rdfs:comment "Geologic unit defined by its geophysical characteristics. Denotes that the properties used to define the unit are measured by intrumental techniques, not directly observable by humans, e.g. density, magnetic susceptibility, magnetization, electrical conductivity."@en ,
                "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
   rdfs:label "geophysical unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/lithodemic_unit
gsgu:lithodemic_unit rdf:type owl:Class ;
  rdfs:subClassOf gsog:Lithostratigraphic_Unit ;
  rdfs:comment "Lithostratigraphic unit that lacks stratification"@en ,
               "dct:source--  North American Commission on Stratigraphic Nomenclature, 1983"@en ;
  rdfs:label "lithodemic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/lithogenetic_unit
gsgu:lithogenetic_unit rdf:type owl:Class ;
    rdfs:subClassOf gsog:Geologic_Unit ;
    rdfs:comment "Geologic unit defined by genesis. The genesis is manifested by material properties, but the material is not the defining property. Example-- alluvial deposits, glacial deposits."@en ,
                 "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
    rdfs:label "lithogenetic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/lithologic_unit
gsgu:lithologic_unit rdf:type owl:Class ;
  rdfs:subClassOf gsog:Geologic_Unit ;
  rdfs:comment "Geologic unit defined by lithology independent of relationships to other units. Denotes a 'kind' of rock body characterized by lithology, e.g. basaltic rocks."@en ,
               "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
  rdfs:label "lithologic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/lithotectonic_unit
gsgu:lithotectonic_unit rdf:type owl:Class ;
     rdfs:subClassOf gsog:Geologic_Unit ;
     rdfs:comment "Geologic unit defined defined on basis of structural or deformation features, mutual relations, origin or historical evolution. Contained material may be igneous, sedimentary, or metamorphic."@en ,
                  "dct:source--  Neuendorf, K.K.E., Mehl Jr., J.P. & Jackson, J.A., 2011: Glossary of Geology, 5th ed., revised. Alexandria, VA, American Geological Institute, 800 pp."@en ;
     rdfs:label "lithotectonic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/magnetostratigraphic_unit
gsgu:magnetostratigraphic_unit rdf:type owl:Class ;
            rdfs:subClassOf gsgu:geophysical_unit ;
            rdfs:comment "Geologic unit defined by magnetic characteristics."@en ,
      "dct:source--  Salvador, A., 1994, International Stratigraphic Guide, Second Edition"@en ;
            rdfs:label "magnetostratigraphic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/mass_movement_unit
gsgu:mass_movement_unit rdf:type owl:Class ;
     rdfs:subClassOf gsgu:lithogenetic_unit ;
     rdfs:comment "Geologic unit produced by gravity driven, down-slope displacemnt of material, and characterized by the type of movement giving rise to the deposit, and by how the individual movement types present in the deposit are related in time and space."@en ,
                  "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
     rdfs:label "mass movement unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/pedoderm
gsgu:pedoderm rdf:type owl:Class ;
              rdfs:subClassOf gsog:Geologic_Unit ;
              rdfs:comment "Geologic unit defined based on soil development and character. Pedoderm is not a surface classification unit because soil classification requires knowledge of the soil profile, which always extends some distance beneath the surface."@en ,
        "dct:source--  CGI geologicunittype SKOS vocabulary 2012-11-24"@en ;
              rdfs:label "pedoderm"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/pedostratigraphic_unit
gsgu:pedostratigraphic_unit rdf:type owl:Class ;
         rdfs:subClassOf gsog:Geologic_Unit ;
         rdfs:comment "Geologic unit that represents a single pedologic horizon in a sequence of strata (consolidated or non-consolidated). The presence of an overlying geologic unit is required, but locally the soil horizon may be at the Earth surface (in which case is may be coincident with a Pedoderm). See discussion at https://www.seegrid.csiro.au/twiki/bin/view/CGIModel/PedostratigraphicUnit"@en ,
   "dct:source--  North American Commission on Stratigraphic Nomenclature, 1983, Morrison, 1995, INQUA/ISSS newsletter, Nov. 1995http://fadr.msu.ru/inqua/nl-archive/nl-8-11/howcan.html, ISSN 0256-2340."@en ;
         rdfs:label "pedostratigraphic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/polarity_chronostratigraphic_unit
gsgu:polarity_chronostratigraphic_unit rdf:type owl:Class ;
 rdfs:subClassOf gsog:Chronostratigraphic_Unit ,
                 gsgu:geophysical_unit ;
 rdfs:comment "Geologic unit defined by primary magnetic-polarity record imposed whien the rock was deposited or crystallized during a specific interval of geolgoic time. Kind of chronostratigraphic unit and kind of geophysical unit."@en ,
              "dct:source--  North American Commission on Stratigraphic Nomenclature, 1983"@en ;
 rdfs:label "polarity chronostratigraphic unit"@en .


###  http://loop3d.org/GSO/ontology/2020/1/geologicunit/geologicunitproperties
gsgu:geologicunitproperties rdf:type owl:Class ;
    owl:equivalentClass [ rdf:type owl:Class ;
        owl:unionOf ( gsop:Alteration_Degree
             gsop:Alteration_Distribution
             gsop:Alteration_Type
             gsop:Metamorphic_Facies
             gsop:Metamorphic_Grade
             gsgu:Bedding_Pattern
             gsgu:Bedding_Style
             gsgu:Bedding_Thickness_Category
             gsgu:Geologic_Unit_Body_Morphology
             gsgu:Geologic_Unit_Composition_Category
             gsgu:Geologic_Unit_Outcrop_Character
           )
    ] ;
    rdfs:subClassOf gsoc:Quality ;
    rdfs:label "Geologic_Unit_Properties"@en .


