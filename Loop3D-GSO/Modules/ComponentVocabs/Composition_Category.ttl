      rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_bearing> ;
      rdfs:comment "Dolomite is present, carbonate minerals form less than 50 percent of rock."@en ;
      
      rdfs:label "dolomitic"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic> rdf:type 
                 owl:Class ;
        dcterms:source "This vocabulary"@en ;
        rdfs:comment "Sedimentary material contains significant feldspar mineral content"@en ;
        
        rdfs:label "feldspathic"@en ;
        rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic_detrital_mode
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic_detrital_mode> rdf:type 
                               owl:Class ;
                      dcterms:source "Dott1964, Folk1968"@en ;
                      rdfs:comment "General sandstone petrography term for a sandstone in which the ratio of feldspar to lithic fragments is greater than 1:1 and less than 75 percent quartz is present in the detrital mode. Subsumes lithic arkosic and arkosic sandstone petrographic composition classes. Usage of modifier feldspathic is consistent with petrographic subdivisions of Dott (1964) and Williams, Turner and Gilbert (1982)"@en ;
                      
                      rdfs:label "feldspathic detrital mode"@en ;
                      rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic_lithic_detrital_mode
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/feldspathic_lithic_detrital_mode> rdf:type 
   owl:Class ;
                             dcterms:source "Folk1968"@en ;
                             rdfs:comment "Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is between 1 to 1 and 1 to 3. This corresponds to the feldspathic litharenite field of Folk (1968). Rocks in this class are likely to be quartzo-feldspathic in terms of mineralogical composition terms in this vocablary, but because the mineralogical composition of the lithic fraction is not constrainged, this correlation is not necessary."@en ;
                             
                             rdfs:label "feldspathic lithic detrital mode"@en ;
                             rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/ferromagnesian
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/ferromagnesian> rdf:type 
                    owl:Class ;
           dcterms:source "NADM SLTTm 2004"@en ;
           rdfs:comment "Material consists of greater than 40 percent dark ferromagnesian silicate minerals. Standard term defined by Bates and Jackson (1987) to mean 'containing iron and magnesium'. Subsumes mafic and ultramafic."@en ;
           
           rdfs:label "ferromagnesian"@en ;
           rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/ferruginous
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/ferruginous> rdf:type 
                 owl:Class ;
        dcterms:source "This vocabulary"@en ;
        rdfs:comment "Greater than accessory amount (greater than 5 percent by volume) of non-silicate iron minerals (hematite, magnetite, siderite...) are present. Rocks containing abundant iron-rich silicate minerals would use the 'Ferromagnesian' terms."@en ;
        
        rdfs:label "ferruginous"@en ;
        rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/fluoride_chemistry
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/fluoride_chemistry> rdf:type 
