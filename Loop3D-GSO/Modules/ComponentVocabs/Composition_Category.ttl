                               owl:Class ;
                      dcterms:source "This vocabulary"@en ;
                      rdfs:comment "Composition not specified. Use in normative descriptions where any composition property is allowed."@en ;
                      
                      rdfs:label "composition not specified"@en ;
                      rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/dolomite_monomineralic_carbonate
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/dolomite_monomineralic_carbonate> rdf:type 
   owl:Class ;
                             dcterms:source "NADM SLTTm 2004"@en ;
                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/monomineralic_carbonate> ;
