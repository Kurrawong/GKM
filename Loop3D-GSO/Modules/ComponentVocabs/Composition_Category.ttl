                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/monomineralic_carbonate> ;
                             rdfs:comment "Carbonate minerals form greater than 75 percent of rock, and calcium carbonate minerals forms greater than 75 percent of carbonate minerals."@en ;
                             
                             rdfs:label "calcareous monomeralic carbonate"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcium_rich_carbonate_chemistry
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcium_rich_carbonate_chemistry> rdf:type 
   owl:Class ;
                             dcterms:source "Struik etal 2002, This vocabulary"@en ;
                             rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_chemistry> ;
                             rdfs:comment "CO3 is the predominante anion, and the Ca is the most abundant (mole percent) cation."@en ;
                             
                             rdfs:label "calcium rich carbonate chemistry"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcsilicate
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcsilicate> rdf:type 
                  owl:Class ;
         dcterms:source "NADM SLTTm 2004"@en ;
         rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/impure_calcsilicate_or_carbonate> ;
         rdfs:comment "Material consists of greater than or equal to 50 percent calcsilicate or carbonate minerals and carbonate minerals less than or equal to calcsilicate minerals in mineral mode."@en ;
         
         rdfs:label "calcsilicate"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcsilicate_bearing
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/calcsilicate_bearing> rdf:type 
                          owl:Class ;
                 dcterms:source "This vocabulary"@en ;
                 rdfs:comment "Calcsilicate minerals of any sort are present, but less than 50 percent of rock"@en ;
                 
                 rdfs:label "calcsilicate bearing"@en ;
                 rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonaceous
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonaceous> rdf:type 
                  owl:Class ;
         dcterms:source "This vocabulary"@en ;
         rdfs:comment "Material containing significant carbon or hydrocarbon mineral phases, including graphite, coal, oil, gas, lignite, peat."@en ;
        
         rdfs:label "carbonaceous"@en ;
         rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonaceous_chemistry
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonaceous_chemistry> rdf:type 
                            owl:Class ;
                   dcterms:source "Struik2002, SLTTs2004, This vocabulary"@en ;
                   rdfs:comment "Containing greater than 5 percent reduced carbon in compounds that are typically the diagenetic products of dead organisms (e.g. coal, bitumen and petroleum). 5 percent value is chosen for consistency with SLTTs usage of term carbonaceous."@en ;
                   
                   rdfs:label "carbonaceous chemistry"@en ;
                   rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate> rdf:type 
               owl:Class ;
      dcterms:source "NADM SLTTm 2004"@en ;
      rdfs:comment "Material consists of greater than 50 percent calcsilicate or carbonate minerals and carbonate minerals greater than calcsilicate minerals in mineral mode. Metacarbonate of NADMSC SLTTm (2004) terminology, adopted here for non-genetic use."@en ;

      rdfs:label "carbonate"@en ;
      rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_bearing
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_bearing> rdf:type 
                       owl:Class ;
              dcterms:source "This vocabulary"@en ;
              rdfs:comment "Carbonate minerals of any sort are present, but less than 50 percent of rock."@en ;
   
              rdfs:label "carbonate bearing"@en ;
              rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_chemistry
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/carbonate_chemistry> rdf:type 
                         owl:Class ;
                dcterms:source "Struik etal 2002, This vocabulary"@en ;
                rdfs:comment "The carbonate (CO3) anion is the predominant anion (by mole percent) in chemical analysis. Sub-categories may be defined by dominant cation. Material in this chemical class overlaps with the monomineralic carbonate class, and probably overlaps with the Carbonate class in the mineralogical composition classification."@en ;
                
                rdfs:label "carbonate chemistry"@en ;
                rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/chloride_chemistry
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/chloride_chemistry> rdf:type 
                        owl:Class ;
               dcterms:source "Struik etal 2002, This vocabulary"@en ;
               rdfs:comment "Chloride is the most abundant (mole percent) anion"@en ;
               
               rdfs:label "chloride chemistry"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/chloritic
<http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/chloritic> rdf:type 
               owl:Class ;
      dcterms:source "This vocabulary"@en ;
      rdfs:comment "Greater than accessory amount (greater than 5 percent by volume) of chlorite group minerals are present"@en ;
      
      rdfs:label "chloritic"@en ;
      rdfs:subClassOf gslth:Any_Composition_Category .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/composition_not_specified
