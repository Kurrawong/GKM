# baseURI: http://loop3d.org/GSO/ontology/2020/1/uom/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/common/ontology
# imports: http://purl.org/dc/terms/

@prefix : <http://loop3d.org/GSO/ontology/2020/1/uom/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:degree
  a owl:Class ; rdfs:subClassOf gsoc:UOM_Space ;
  dcterms:alternative "Â°"@en  ;
  dcterms:source "https://en.wikipedia.org/wiki/Degree_(angle)"@en  ;
  rdfs:comment "a measurement of a plane angle, defined so that a full rotation is 360 degrees"@en ;
  rdfs:label "degree"@en ;
.
:ma
  a owl:Class ; rdfs:subClassOf gsoc:UOM_Space ;
  dcterms:alternative "Ma"@en  ;
  rdfs:comment "a unit of time equal to one million, or 1 E6 yr., measured from 1950 as 'present'"@en ;
  rdfs:label "Million years before present"@en ;
.
:meter
  a owl:Class ; rdfs:subClassOf gsoc:UOM_Space ;
  dcterms:alternative "m"@en  ;
  rdfs:comment "a unit of length equal to the length of the path travelled by light in vacuum during a time interval of 1/299,792,458 of a second. (https://www.nationalgeographic.org/thisday/mar30/meter-defined/)"@en ;
  rdfs:label "meter"@en ;
.
:millimeter
  a owl:Class ; rdfs:subClassOf gsoc:UOM_Space ;
  dcterms:alternative "mm"@en  ;
  rdfs:comment "a unit of length equal to one thousandth of a meter and equivalent to 0.03937 inch."@en ;
  rdfs:label "millimeter"@en ;
.
:ontology
  a owl:Ontology ;
  rdfs:comment "Stephen M. Richard 2019-12-10"@en  ;
  rdfs:comment "simple vocabulary for units of measure. TBD: generate mapping to QUDT or other UOM ontologies with more semantics and content as necessary"@en ;
  owl:imports gsoc:ontology ;
  owl:imports dcterms: ;
.
:percent
  a owl:Class ; rdfs:subClassOf gsoc:UOM_Space ;
  dcterms:alternative "%"@en  ;
  rdfs:comment "fraction of a whole, expressed as a fraction of 100, a unitless ratio value"@en ;
  rdfs:label "percent"@en ;
.
