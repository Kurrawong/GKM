# baseURI: http://loop3d.org/GSO/ontology/2020/1/metamorphicfacies/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/geologicproperty/ontology
# imports: http://purl.org/dc/terms/

@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gsmf: <http://loop3d.org/GSO/ontology/2020/1/metamorphicfacies/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2020/1/geologicproperty/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xlink: <http://www.w3.org/1999/xlink> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gsmf:amphibolite_metamorphic_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by hornblende-plagioclase (plagioclase more calcic than An17)."@en ;
  rdfs:label "amphibolite metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:eclogite_metamorphic_facies
  a owl:Class ;
  dcterms:source "Fettes and Desmons, 2007"@en ;
  rdfs:comment "Omphacite-garnet-quartz (no plagioclase, olivine stable with garnet), assemblage for rocks of basaltic composition"@en ;
  rdfs:label "eclogite metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:epidote_amphibolite_metamorphic_facies
  a owl:Class ;
  dcterms:source "Fettes and Desmons, 2007"@en ;
  rdfs:comment "Hornblende-albite-epidote(-chlorite), assemblage for rocks of basaltic composition"@en ;
  rdfs:label "epidote amphibolite metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:glaucophane_schist_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by glaucophane-epidote (-garnet), glaucophane-lawsonite, or glaucophane-lawsonite-jadeite."@en ;
  rdfs:label "glaucophane schist facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:granulite_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by the mineral assemblage clinopyroxene-orthopyroxene-plagioclase (olivine not stable with plagioclase or with garnet), or especially high-temperature varieties or polymorphs of minerals (e"@en ;
  rdfs:label "granulite facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:greenschist_metamorphic_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by actinolite-albite-epidote-chlorite (an epidote group mineral is the diagnostic Ca-Al silicate rather than prehnite or pumpellyite)."@en ;
  rdfs:label "greenschist metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:metamorphic_facies_not_specified
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Metamorphosed, but facies not specified in more detail. For use in normative description to indicate that any metamorphic facies value is valid."@en ;
  rdfs:label "metamorphic facies not specified"@en ;
  rdfs:subClassOf gsop:Any_Metamorphic_Facies ;
.
gsmf:metamorphic_facies_unknown
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "For use in instance descriptions to indicate that no data are available to constrain metamorphic facies. May be metamorphosed or non-metamorphosed."@en ;
  rdfs:label "metamorphic facies unknown"@en ;
  rdfs:subClassOf gsop:Any_Metamorphic_Facies ;
.
gsmf:no_metamorphic_minerals
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "Corresponds to non-metamorphosed condition, since metamorphic facies classification is based on mineral assemblages, absence of metamorphic minerals means that the classification does not apply."@en ;
  rdfs:label "no metamorphic minerals"@en ;
  rdfs:subClassOf gsop:Any_Metamorphic_Facies ;
.
gsmf:ontology
  a owl:Ontology ;
  dcterms:creator "CGI Geoscience Terminology Working Group, OWL conversion Stephen M. Richard"@en ;
  dcterms:date "2016-11-22"@en ;
  dcterms:description "This file contains the CGI Metamorphic Facies vocabulary, as defined by the IUGS Commission for Geoscience Information (CGI) Geoscience Terminology Working Group. The vocabulary is based on Fettes and Desmons (eds.), 2007, METAMORPHIC ROCKS: A CLASSIFICATION AND GLOSSARY OF TERMS - Recommendations of the International Union of Geological Sciences Subcommission on the Systematics of Metamorphic Rocks. Used to populate metamorphic facies property in metamorphic descriptions. By extension, this vocabulary includes all concepts in this conceptScheme, as well as concepts in any previous versions of the scheme."@en ;
  dcterms:modified "2020-01-30"@en ;
  rdfs:comment "This file contains the 2016 SKOS-RDF version of the CGI Metamorphic Facies vocabulary. Compilation and review in MS Excel spreadsheet, converted to MS Excel for SKOS generation using SKOS_for_GA_from_XLS_2016.03.xslt"@en ;
  rdfs:label "Metamorphic Facies"@en ;
  owl:imports gsop:ontology ;
  owl:imports dcterms: ;
.
gsmf:pyroxene_hornfels_metamorphic_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by clinopyroxene-orthopyroxene-plagioclase (olivine stable with plagioclase)."@en ;
  rdfs:label "pyroxene hornfels metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:sanidinite_metamorphic_facies
  a owl:Class ;
  dcterms:source "Fettes and Desmons, 2007"@en ;
  rdfs:comment "distinguished from the pyroxene hornfels facies by the occurrence of especially high-temperature varieties and polymorphs of minerals (eg. pigeonite, K-rich labradorite), assemblage for rocks of basaltic composition"@en ;
  rdfs:label "sanidinite metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:subgreenschist_metamorphic_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by prehnite-pumpellyite, pumpellyite-actinolite, prehnite-actinolite (prehnite and pumpellyite are the diagnostic Ca-Al silicates rather than minerals of the epidote or zeolite groups)."@en ;
  rdfs:label "subgreenschist metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
gsmf:zeolite_metamorphic_facies
  a owl:Class ;
  dcterms:source "Smulikowski et al. 2003; Fettes and Desmons, 2007"@en ;
  rdfs:comment "Metamorphic facies characterized in rocks of basaltic composition by zeolite minerals such as laumontite and heulandites (in place of other Ca-Al silicates such as prehnite, pumpellyite and epidote)."@en ;
  rdfs:label "zeolite metamorphic facies"@en ;
  rdfs:subClassOf gsmf:metamorphic_facies_not_specified ;
.
