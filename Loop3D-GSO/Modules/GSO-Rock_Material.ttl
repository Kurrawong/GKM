gslth:Foid_Bearing_Gabbro rdf:type owl:Class ;
                          rdfs:subClassOf gslth:Gabbroic_Rock ;
                          rdfs:comment "Gabbroic rock that contains 0-10 percent feldspathoid minerals and no quartz in the QAPF fraction. QAPF field 10'."@en ,
                                       "dct:source--  LeMaitre et al. 2002"@en ;
                          rdfs:label "foid bearing gabbro"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/Foid_Bearing_Latite
gslth:Foid_Bearing_Latite rdf:type owl:Class ;
                          rdfs:subClassOf gslth:Latitic_Rock ;
                          rdfs:comment "Latitic rock that contains no quartz and between 0 and 10 percent feldspathoid minerals in the QAPF fraction. QAPF field 8'."@en ,
                                       "dct:source--  LeMaitre et al. 2002"@en ;
                          rdfs:label "foid bearing latite"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/Foid_Bearing_Monzodiorite
gslth:Foid_Bearing_Monzodiorite rdf:type owl:Class ;
                                rdfs:subClassOf gslth:Monzodioritic_Rock ;
                                rdfs:comment "Monzodioritic rock that contains between 0 and 10 percent feldspathoid mineral."@en ,
                                             "dct:source--  LeMaitre et al. 2002"@en ;
                                rdfs:label "foid bearing monzodiorite"@en .


###  http://loop3d.org/GSO/ontology/2020/1/lithology/Foid_Bearing_Monzogabbro
gslth:Foid_Bearing_Monzogabbro rdf:type owl:Class ;
