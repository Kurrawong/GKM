gst:Age
  rdf:type owl:Class ;
  rdfs:comment "Most granular geochronologic time interval;  one or more Ages are grouped into an Epoch"@en ;
  rdfs:label "Age (time scale rank)"@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:complementOf gst:Age ;
        ] ;
      owl:onProperty gsoc:timeIncludedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              gsgu:Stage
              gsgu:Substage
            ) ;
        ] ;
    ] ;
.
