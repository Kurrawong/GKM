gst:Eon
  rdf:type owl:Class ;
  rdfs:comment "How will this be defined?  Must contain at least one Era and have duration longer than some Era?"@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:complementOf [
              rdf:type owl:Class ;
              owl:unionOf (
                  gst:Era
                  gst:Period
                  gst:Subperiod
                  gst:Epoch
                  gst:Age
                ) ;
            ] ;
        ] ;
      owl:onProperty gsoc:timeDuring ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:complementOf [
              rdf:type owl:Class ;
              owl:unionOf (
                  gst:Supereon
                  gst:Eon
                ) ;
            ] ;
        ] ;
      owl:onProperty gsoc:timeContains ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom gsgu:Eonothem ;
    ] ;
.
gst:Epoch
  rdf:type owl:Class ;
  rdfs:comment "An Epoch may be subdivided into Ages. Epochs are grouped into Periods. Example epochs: Eocene, Pleistocene"@en ;
  rdfs:label "Epoch"@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:complementOf [
              rdf:type owl:Class ;
              owl:unionOf (
                  gst:Epoch
                  gst:Age
                ) ;
            ] ;
        ] ;
      owl:onProperty gsoc:timeIncludedBy ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:complementOf [
              rdf:type owl:Class ;
              owl:unionOf (
                  gst:Epoch
                  gst:Period
                  gst:Subperiod
                  gst:Era
                  gst:Eon
                  gst:Supereon
                ) ;
            ] ;
        ] ;
      owl:onProperty gsoc:timeIncludes ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom gsgu:Series ;
    ] ;
.
