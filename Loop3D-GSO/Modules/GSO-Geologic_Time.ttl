gst:Subperiod
  rdf:type owl:Class ;
  rdfs:comment "Named rank for geochronologic time interval. Sub-Periods may be subdivided into Epochs. Sub-Periods are grouped into a period."@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
