              rdf:type owl:Class ;
              owl:unionOf (
                  gst:Period
                  gst:Era
                  gst:Eon
                  gst:Supereon
                ) ;
            ] ;
        ] ;
      owl:onProperty gsoc:timeIncludes ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
