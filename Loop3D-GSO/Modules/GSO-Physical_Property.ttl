# baseURI: http://loop3d.org/GSO/ontology/2019/1/geologicproperty/ontology
# imports: http://loop3d.org/GSO/ontology/2019/1/common/ontology

@prefix : <http://loop3d.org/GSO/ontology/2019/1/geologicproperty/> .
@prefix geologicfeature: <http://loop3d.org/GSO/ontology/2019/1/geologicfeature/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2019/1/common/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2019/1/geologicproperty/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gsop:Alteration_Degree
  a owl:Class ;
  rdfs:comment "Specification of the strength of observed alteration. Can be used on Geologic_Unit or Rock_Material"@en ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Alteration degree"@en ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Alteration_Distribution
  a owl:Class ;
  rdfs:comment "AlterationDistribution describes the spatial distribution or geometry of alteration zones. eg: patchy, spotted, banded, viens, vein breccia, pervasive, disseminated, etc" ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Alteration distribution" ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Alteration_Type
  a owl:Class ;
  rdfs:comment "Specification of alteration types (eg; potassic, argillic, advanced argillic)"@en ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Alteration type" ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsop:Alteration_Type_Space ;
    ] ;
.
gsop:Alteration_Type_Space
  a owl:Class ;
  rdfs:comment "Conceptual space containing all alteration types."@en ;
  rdfs:label "Any Alteration Type"@en ;
  rdfs:subClassOf gsoc:Physical_Region ;
.
gsop:Azimuth
  a owl:Class ;
  rdfs:comment "Bearing of a horizontal line relative to north. Use for specifying the orientation of planar features."@en ;
  rdfs:label "Azimuth"@en ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasUOM ;
      owl:someValuesFrom gsop:UOM ;
    ] ;
.
gsop:Consolidation_Degree
  a owl:Class ;
  rdfs:comment "A property that specifies the degree to which an aggregation of EarthMaterial particles is a distinct solid material. Consolidation and induration are related concepts specified by this property. They define a continuum from unconsolidated material to very hard rock. Induration is the degree to which a consolidated material is made hard, operationally determined by how difficult it is to break a piece of the material. Consolidated materials may have varying degrees of induration (NADMSC, 2004)"@en ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Consolidation degree"@en ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsop:Consolidation_Degree_Space ;
    ] ;
.
gsop:Consolidation_Degree_Space
  a owl:Class ;
  rdfs:label "Any Consolidation Degree"@en ;
  rdfs:subClassOf gsoc:Physical_Region ;
.
gsop:Dip
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Dip" ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasUOM ;
      owl:someValuesFrom gsop:UOM ;
    ] ;
.
gsop:Dip_Dip_Direction
  a owl:Class ;
  rdfs:comment "The orientation measurement consists of a dip and a dip direction. Dip is the angle that the structural surface (eg bedding, fault plane) makes with the horizontal measured perpendicular to the strike of the structure and in the vertical plane Dip direction is the azimuth perpindicular to the strike of the structure"@en ;
  rdfs:label "Dip and Dip Direction"@en ;
  rdfs:subClassOf gsop:Plane_Orientation_Convention_Space ;
.
gsop:Displacement
  a owl:Class ;
  rdfs:label "Displacement"@en ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Line_Orientation
  a owl:Class ;
  rdfs:comment "specification of the orientation of a line in space, using a trend (bearing of horizontal projection of line relative to geographic north) and plunge (inclination of line measured positive downward in degrees from local horizontal).  TBD:  add 'isdirected property to indicate if the line is directed (e.g. sense of shear), values should be Boolean,"@en ;
  rdfs:label "Line orientation"@en ;
  rdfs:subClassOf gsop:Orientation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPhysicalProperty ;
      owl:someValuesFrom gsop:Azimuth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPhysicalProperty ;
      owl:someValuesFrom gsop:Plunge ;
    ] ;
.
gsop:Metamorphic_Facies
  a owl:Class ;
  rdfs:comment "Categories that characterize mineral assemblages indicative of certain metamorphic P-T conditions. Examples include Barrovian metasedimentary zones (eg: biotite facies, kyanite facies) or assemblages developed in rocks of more mafic composition (eg: greenschist facies, amphibolite facies)."@en ;
  rdfs:comment "applies to Earth_Material or Geologic_Unit" ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Metamorphic facies" ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsop:Metamorphic_Facies_Space ;
    ] ;
.
gsop:Metamorphic_Facies_Space
  a owl:Class ;
  rdfs:label "Any Metamorphic Facies" ;
  rdfs:subClassOf gsoc:Physical_Region ;
.
gsop:Metamorphic_Grade
  a owl:Class ;
  rdfs:comment "A term to indicate the intensity or rank of metamorphism applied to an EarthMaterial (eg: high metamorphic grade, low metamorphic grade). Indicates in a general way the P-T environment in which the metamorphism took place. Determination of metamorphic grade is based on mineral assemblages (ie, facies) present in a rock that are interpreted to have crystallized in equilibrium during a particular metamorphic event."@en ;
  rdfs:comment "applies to Earth_Material or Geologic_Unit" ;
  rdfs:isDefinedBy "GeoSciML v4" ;
  rdfs:label "Metamorphic grade" ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsop:Metamorphic_Grade_Space ;
    ] ;
.
gsop:Metamorphic_Grade_Space
  a owl:Class ;
  rdfs:label "Any Metamorphic Grade" ;
  rdfs:subClassOf gsoc:Physical_Region ;
.
gsop:Orientation
  a owl:Class ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Overturned
  a owl:Class ;
  rdfs:comment "top of surface is facing downward, toward Earth interior."@en ;
  rdfs:label "Overturned facing"@en ;
  rdfs:subClassOf gsop:Surface_Facing_Space ;
.
gsop:Peak_Pressure
  a owl:Class ;
  rdfs:comment "applies to Earth_Material or Geologic_Unit" ;
  rdfs:comment "peak metamorphic pressure" ;
  rdfs:isDefinedBy "GeoSciML v3" ;
  rdfs:label "Peak pressure" ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Peak_Temperature
  a owl:Class ;
  rdfs:comment "applies to Earth_Material or Geologic_Unit" ;
  rdfs:comment "peak metamorphic temperature" ;
  rdfs:isDefinedBy "GeoSciML v3" ;
  rdfs:label "Peak temperature" ;
  rdfs:subClassOf gsoc:Physical_Property ;
.
gsop:Plane_Orientation
  a owl:Class ;
  rdfs:comment "specification of the orientation of a plane in space, using a dip (inclination of line of maximum imnclination contained in the plane, measured positive downward in degrees from local horizontal) and either a strike or dip direction"@en ;
  rdfs:label "Plane orientation (dip and dip direction)"@en ;
  rdfs:subClassOf gsop:Orientation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPhysicalProperty ;
      owl:someValuesFrom gsop:Azimuth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPhysicalProperty ;
      owl:someValuesFrom gsop:Dip ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasProperty ;
      owl:someValuesFrom gsop:Plane_Orientation_Convention ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasProperty ;
      owl:someValuesFrom gsop:Surface_Facing_Space ;
    ] ;
.
gsop:Plane_Orientation_Convention
  a owl:Class ;
  rdfs:label "Plane orientation convention"@en ;
  rdfs:subClassOf gsoc:Abstract_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsop:Plane_Orientation_Convention_Space ;
      owl:onProperty gsoc:hasValue ;
    ] ;
.
gsop:Plane_Orientation_Convention_Space
  a owl:Class ;
  rdfs:comment "conceptual space for conventions defining planar orientation representations."@en ;
  rdfs:label "any plane orientation convention"@en ;
  rdfs:subClassOf gsoc:Abstract_Region ;
.
gsop:Plunge
  a owl:Class ;
  rdfs:comment "Numeric value should be between +90 and -90.   Negative values only valid if the linear feature is directed." ;
  rdfs:label "Plunge" ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasUOM ;
      owl:someValuesFrom gsop:UOM ;
    ] ;
.
gsop:Separation
  a owl:Class ;
  rdfs:comment "a vector (magnitude and orientation) specifying the distance between the intersection of a surface on opposite sides of a displacment surface. In 3-D is the projection of a vector normal to the displaced surface into the displacment surface, but might also be specified as offset of the intersection of displacement surface and marker surface in some other profile view (e.g. horizontal/map view, or a cross section)"@en ;
  rdfs:label "Separation" ;
  rdfs:subClassOf gsop:Displacement ;
.
gsop:Slip
  a owl:Class ;
  rdfs:comment "a vector (magnitude and orientation) that links a peircing point on opposite sides of a displacement surface."@en ;
  rdfs:label "Slip"@en ;
  rdfs:subClassOf gsop:Displacement ;
.
gsop:Strike_Dip_RHR
  a owl:Class ;
  rdfs:comment "The strike and dip of planar data is listed according to the ‘right-hand rule’ or, as one looks along the strike direction, the surface dips to the right. Dip is the angle that the structural surface (eg bedding, fault plane) makes with the horizontal measured perpindicular to the strike of the structure and in the vertical plane"@en ;
  rdfs:label "Right hand rule strike and dip"@en ;
  rdfs:subClassOf gsop:Plane_Orientation_Convention_Space ;
.
gsop:Surface_Facing
  a owl:Class ;
  rdfs:comment "property to specify if a surface is directed, i.e. has a top and bottom. Values: {upright, overturned, unknown}"@en ;
  rdfs:label "facing direction"@en ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsop:Surface_Facing_Space ;
      owl:onProperty gsoc:hasValue ;
    ] ;
.
gsop:Surface_Facing_Space
  a owl:Class ;
  rdfs:comment "conceptual space for values specifying the facing direction of surfaces, e.g. upright, overturned."@en ;
  rdfs:label "Any facing value"@en ;
  rdfs:subClassOf gsoc:Physical_Region ;
.
gsop:Thickness
  a owl:Class ;
  rdfs:subClassOf gsoc:Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasUOM ;
      owl:someValuesFrom gsop:UOM ;
    ] ;
.
gsop:UOM
  a owl:Class ;
  rdfs:label "Units of measurement"@en ;
  rdfs:subClassOf gsoc:Abstract_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:UOM_Space ;
      owl:onProperty gsoc:hasValue ;
    ] ;
.
gsop:Unknown_Facing
  a owl:Class ;
  rdfs:comment "Facing direction is unknown {@en}	" ;
  rdfs:label "Unknown Facing Direction" ;
  rdfs:subClassOf gsop:Surface_Facing_Space ;
.
gsop:Upright
  a owl:Class ;
  rdfs:comment "top of surface is away from center of earth." ;
  rdfs:label "upright facing"@en ;
  rdfs:subClassOf gsop:Surface_Facing_Space ;
.
gsop:ontology
  a owl:Ontology ;
  rdfs:comment "Vocabulary of geologic physical properties used in multiple GSO modules."@en ;
  rdfs:label "Physical properties"@en ;
  owl:imports gsoc:ontology ;
.
