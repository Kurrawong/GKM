  owl:inverseOf gsoc:timeDuring ;
.
gsoc:timeDisjoint
  rdf:type owl:ObjectProperty ;
  rdf:type owl:SymmetricProperty ;
  rdfs:comment "the union of Allen Before and After; Age intervals of subject and object do not touch or intersect."@en ;
  rdfs:subPropertyOf gsoc:isTemporallyRelatedTo ;
.
gsoc:timeDuring
  rdf:type owl:ObjectProperty ;
  rdfs:comment "interval A is entirely contained in interval B; B starts before A and B ends after A. Allen relation."@en ;
  rdfs:subPropertyOf gsoc:timeIncludedBy ;
  rdfs:subPropertyOf gsoc:timeIntersects ;
  owl:inverseOf gsoc:timeContains ;
.
gsoc:timeEquivalentTo
  rdf:type owl:ObjectProperty ;
