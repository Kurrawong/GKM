:Physical_Setting rdfs:subClassOf _:genid56 .

_:genid56 a owl:Restriction ;
	owl:onProperty :isSettingFor ;
	owl:someValuesFrom _:genid57 .

_:genid57 a owl:Class ;
	owl:unionOf _:genid59 .

_:genid59 a rdf:List ;
	rdf:first :Perdurant ;
	rdf:rest _:genid58 .

_:genid58 a rdf:List ;
	rdf:first :Physical_Endurant ;
	rdf:rest rdf:nil .

:Physical_Setting rdfs:subClassOf _:genid60 .

_:genid60 a owl:Restriction ;
	owl:onProperty :hasPart ;
	owl:allValuesFrom _:genid61 .

_:genid61 a owl:Class ;
	owl:unionOf _:genid65 .

_:genid65 a rdf:List ;
	rdf:first :Perdurant ;
	rdf:rest _:genid64 .

_:genid64 a rdf:List ;
	rdf:first :Physical_Endurant ;
	rdf:rest _:genid63 .

_:genid63 a rdf:List ;
	rdf:first :Physical_Property ;
	rdf:rest _:genid62 .

_:genid62 a rdf:List ;
	rdf:first :Physical_Setting ;
	rdf:rest rdf:nil .

:Physical_Setting rdfs:subClassOf _:genid66 .

_:genid66 a owl:Restriction ;
	owl:onProperty :hasProperty ;
	owl:allValuesFrom :Property .

:Physical_Setting rdfs:subClassOf _:genid67 .

_:genid67 a owl:Restriction ;
	owl:onProperty :isPartOf ;
	owl:allValuesFrom :Physical_Setting .

:Physical_Setting rdfs:comment "A physical setting might be defined by parts that are endurants or perdurants, and also by particular qualities/properties. Thus it is a direct subtype of Particular."@en ;
	rdfs:label "Physical Setting"@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Physical_Void

:Physical_Void a owl:Class ;
	rdfs:subClassOf :Place_Feature ;
	rdfs:comment "An empty physical space in 3D. Distinct from abstract mathematical spaces, which include spaces made from various reference frames, including coordinate systems imposed on the earth (these are abstract imposed on real space). V(x) <--> IPF(x) ^ exists y [hosts-v (y,x)]"@en ;
	rdfs:label "Physical Void "@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Place_Feature

:Place_Feature a owl:Class ;
	rdfs:subClassOf :Immaterial_Feature ;
	rdfs:comment ""@en ;
	rdfs:label "Place Feature "@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Plasma

:Plasma a owl:Class ;
	rdfs:subClassOf :Fluid .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Process

:Process a owl:Class ;
	rdfs:subClassOf :Perdurant , _:genid68 .

_:genid68 a owl:Restriction ;
	owl:onProperty :manifests ;
	owl:allValuesFrom :Event .

:Process rdfs:comment "In Basic Formal Ontology: An occurrent entity that exists in time by occurring or happening, has temporal (proper) parts, and always depends on some material (continuant) entity (Arp et al., 2015, p. 121; Notes in parenthesis () from Mustafa Jarrar https://www.slideshare.net/jarrar02/classifying-processes-and-basic-formal-ontology) Processes are not necessarily homeomeric or cumulative.  \" process is a series of events to produce a result\" (https://wikidiff.com/event/process)  \"A process may also be defined as the workflows and sequence of events inherent in processes such as manufacturing, engineering and business processes.\" (https://en.wikipedia.org/wiki/Process_ontology)"@en ;
	rdfs:label "Process"@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Property

:Property a owl:Class ;
	rdfs:subClassOf :Particular , _:genid69 .

_:genid69 a owl:Restriction ;
	owl:onProperty :hasPart ;
	owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
	owl:onClass :Particular .

:Property rdfs:subClassOf _:genid70 .

_:genid70 a owl:Restriction ;
	owl:onProperty :isPartOf ;
	owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
	owl:onClass :Particular .

:Property rdfs:comment "A concept that specifies some aspect that inheres in instances of some Particular.  Note that the value of the determiningEvent  property on Property is used to specify metadata about how a property value is assigned"@en ;
	rdfs:label "Property"@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Proportion

:Proportion a owl:Class ;
	rdfs:subClassOf :Physical_Property , _:genid71 .

_:genid71 a owl:Restriction ;
