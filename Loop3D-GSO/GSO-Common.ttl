  rdfs:subClassOf :Physical_Property ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :proportionFor ;
      owl:someValuesFrom :Endurant ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty :proportionOf ;
      owl:someValuesFrom :Endurant ;
    ] ;
.
