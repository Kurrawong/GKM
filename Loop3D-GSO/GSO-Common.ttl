:isConstituentOf
  a owl:ObjectProperty ;
  a owl:TransitiveProperty ;
  rdfs:range :Amount_Of_Matter ;
.
:isInputFor
  a owl:ObjectProperty ;
  rdfs:comment "relation from Endurant to Perdurant event or process for which the endurant is a precursor" ;
  rdfs:label "is input for" ;
  rdfs:subPropertyOf :isParticipantIn ;
.
:isManifestationOf
  a owl:ObjectProperty ;
  rdfs:comment "" ;
  rdfs:label "is Manifestation Of" ;
