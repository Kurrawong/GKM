  rdfs:comment "" ;
  rdfs:label "Occurrence Boundary "@en ;
  rdfs:subClassOf gsoc:Occurrence_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsoc:Process
              gsoc:Event
            ) ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isPartOf ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsoc:Process
              gsoc:Event
            ) ;
        ] ;
    ] ;
.
gsoc:Occurrence_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Occurrence Feature "@en ;
  rdfs:subClassOf gsoc:Perdurant_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsoc:Process
              gsoc:Event
            ) ;
        ] ;
    ] ;
.
gsoc:Part_Feature
  a owl:Class ;
  rdfs:comment "proper part of host is focus (forehead, mountain top, boundary[as a material entity]) part of relation is essential to feature but not to host."@en ;
  rdfs:label "Part Feature "@en ;
  rdfs:subClassOf gsoc:Feature ;
.
gsoc:Parthood
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Parthood "@en ;
  rdfs:subClassOf gsoc:Relator ;
.
gsoc:Participation
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Participation "@en ;
  rdfs:subClassOf gsoc:Relator ;
.
gsoc:Particular
  a owl:Class ;
  rdfs:comment "AKA 'Entity', 'Thing'. Any individual in the DOLCE domain of discourse. The extensional coverage of DOLCE is as large as possible, since it ranges on 'possibilia', i.e all possible individuals that can be postulated by means of DOLCE axioms. Possibilia include physical objects, substances, processes, qualities, conceptual regions, non-physical objects, collections and even arbitrary sums of objects.The class 'particular' features a covering partition that includes: endurant, perdurant, quality, and abstract. There are also some subclasses defined as unions of subclasses of 'particular' for special purposes: spatio-temporal-particular (any particular except abstracts)- physical-realization (any realization of an information object, defined in the ExtendedDnS ontology)."@en ;
  rdfs:label "Particular"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Inherant ;
      owl:onProperty gsoc:hasInherant ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Particular ;
      owl:onProperty gsoc:determinedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Particular ;
      owl:onProperty gsoc:hasReference ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Situation ;
      owl:onProperty gsoc:hasSetting ;
    ] ;
.
gsoc:Pause
  a owl:Class ;
  rdfs:comment "Pause within an event or process, e.g. Intermission during hockey game or theater performance (host = playing process, constituent = time interval)"@en ;
  rdfs:label "Pause "@en ;
  rdfs:subClassOf gsoc:Temporal_Void ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              gsoc:Process
              gsoc:Event
            ) ;
        ] ;
      owl:onProperty gsoc:hostedBy ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
gsoc:Perdurant
  a owl:Class ;
  rdfs:comment "Perdurants (AKA occurrences) comprise what are variously called events, processes, phenomena, activities and states. They can have temporal parts or spatial parts. For instance, the first movement of (an execution of) a symphony is a temporal part of the symphony. On the other hand, the play performed by the left side of the orchestra is a spatial part. In both cases, these parts are occurrences themselves. We assume that objects cannot be parts of occurrences, but rather they participate in them. Perdurants extend in time by accumulating different temporal parts, so that, at any time they are present, they are only partially present, in the sense that some of their proper temporal parts (e.g., their previous or future phases) may be not present. E.g., the piece of paper you are reading now is wholly present, while some temporal parts of your reading are not present yet, or any more. Philosophers say that endurants are entities that are in time, while lacking temporal parts (so to speak, all their parts flow with them in time). Perdurants, on the contrary, are entities that happen in time, and can have temporal parts (all their parts are fixed in time)."@en ;
  rdfs:label "Perdurant"@en ;
  rdfs:subClassOf gsoc:Particular ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Perdurant ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:directTemporalLocation ;
      owl:someValuesFrom gsoc:Temporal_Location ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:directTemporalOccupies ;
      owl:someValuesFrom gsoc:Time_Region ;
    ] ;
.
gsoc:Perdurant_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Perdurant Feature "@en ;
  rdfs:subClassOf gsoc:Feature ;
  rdfs:subClassOf gsoc:Perdurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Perdurant ;
    ] ;
.
gsoc:Physical_Boundary
  a owl:Class ;
  rdfs:comment """Physical Boundary (PB):
A part of a physical object or amount of matter that touches the exterior in 3D space, either a point (e.g. table corner), a line (table edge), or a surface (e.g. table top)... possibly a volume.

PB(x) <--> RPF(x) ^ exists y,z [hosts(y,x) ^ partOf (x,y) ^ touches (x, complement(y))]"""@en ;
  rdfs:label "Physical Boundary"@en ;
  rdfs:subClassOf gsoc:Part_Feature ;
.
gsoc:Physical_Gap
  a owl:Class ;
  rdfs:comment """A physical void with a scattered (disconnected) host, consisting of the spaces between the scattered parts of the host. e.g. porosity in a rock.  G(x) <--> exists y [hosts-v(y,x) ^ not ICon (y)].  ICon (x): x is interior connected (continuous, not scattered)  {@en}
"""@en ;
  rdfs:label "Gap"@en ;
  rdfs:subClassOf gsoc:Physical_Void ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass gsoc:Material_Endurant ;
      owl:onProperty gsoc:hostedBy ;
    ] ;
.
gsoc:Physical_Void
  a owl:Class ;
  rdfs:comment "A chunk of physical space adjacent to some material feature and surrounded by it. (holes,valleys, canyon, cave, gap, space between fingers). host must be self connected.  An empty physical space in 3D. Distinct from abstract mathematical spaces, which include spaces made from various reference frames, including coordinate systems imposed on the earth (these are abstract imposed on real space). V(x) <--> IPF(x) ^ exists y [hosts-v (y,x)]"@en ;
  rdfs:label "Physical Void"@en ;
  rdfs:subClassOf gsoc:Place_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom gsoc:Material_Endurant ;
    ] ;
.
gsoc:Place_Feature
  a owl:Class ;
  rdfs:comment "chunk of physical space (the focal); only hosted by and relative to a material entity, thus not fixed in space (host can move) (downtown, outside my house, personal space)"@en ;
  rdfs:label "Place Feature "@en ;
  rdfs:subClassOf gsoc:Immaterial_Feature ;
.
gsoc:Plasma
  a owl:Class ;
  rdfs:subClassOf gsoc:Fluid ;
.
gsoc:Process
  a owl:Class ;
  rdfs:comment "In Basic Formal Ontology: An occurrent entity that exists in time by occurring or happening, has temporal (proper) parts, and always depends on some material (continuant) entity (Arp et al., 2015, p. 121; Notes in parenthesis () from Mustafa Jarrar https://www.slideshare.net/jarrar02/classifying-processes-and-basic-formal-ontology) Processes are not necessarily homeomeric or cumulative.  \" process is a series of events to produce a result\" (https://wikidiff.com/event/process)  \"A process may also be defined as the workflows and sequence of events inherent in processes such as manufacturing, engineering and business processes.\" (https://en.wikipedia.org/wiki/Process_ontology)"@en ;
  rdfs:label "Process"@en ;
  rdfs:subClassOf gsoc:Perdurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Event ;
      owl:onProperty gsoc:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Process ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:directTemporalOccupies ;
      owl:someValuesFrom gsoc:Time_Region ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasParticipant ;
      owl:someValuesFrom gsoc:Endurant ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:indirectSpatialLocation ;
      owl:someValuesFrom gsoc:Spatial_Region ;
    ] ;
.
gsoc:Proportion
  a owl:Class ;
  rdfs:comment "A numeric property value used to express the fraction of a whole that is composed of some part. Values will range from 0 to 1 (0 to 100%)."@en ;
  rdfs:label "Proportion"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsoc:Quality_Value ;
    ] ;
.
gsoc:Quality
  a owl:Class ;
  rdfs:comment "A concept that specifies some aspect that inheres in instances of some Particular.  Note that the value of the determiningEvent property on Quality is used to specify metadata about how a property value is assigned. Synonym: Property"@en ;
  rdfs:comment "Need SHACL rule that only one of 'hasDataValue' or 'hasValue' should be instantiated."@en ;
  rdfs:label "Quality"@en ;
  rdfs:subClassOf gsoc:Inherant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Quality ;
      owl:onProperty gsoc:hasQuality ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Quality ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isQualityOf ;
      owl:someValuesFrom gsoc:Particular ;
    ] ;
.
gsoc:Quality_Range
  a owl:Class ;
  rdfs:comment "Quality that specifies a quality value with minimum and maximum value in some partially ordered value space."@en ;
  rdfs:label "Quality Range "@en ;
  rdfs:subClassOf gsoc:Quality_Value ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasEndValue ;
      owl:someValuesFrom gsoc:Measured_Value ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasStartValue ;
      owl:someValuesFrom gsoc:Measured_Value ;
    ] ;
.
gsoc:Quality_Value
  a owl:Class ;
  rdfs:comment "quality the specifies a numeric value or range of values for some quality"@en ;
  rdfs:label "Quality Value"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isValueOf ;
      owl:someValuesFrom gsoc:Quality ;
    ] ;
.
gsoc:Reference_System
  a owl:Class ;
  rdfs:comment "endurant that establishes a frame of reference for locating a particular in some space; the reference system has a dependeny on the space it partitions."@en ;
  rdfs:label "Reference System "@en ;
  rdfs:subClassOf gsoc:Notional_Endurant ;
.
gsoc:Relator
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Relator "@en ;
  rdfs:subClassOf gsoc:Inherant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass gsoc:Particular ;
      owl:onProperty gsoc:inheresIn ;
    ] ;
.
gsoc:Role
  a owl:Class ;
  rdfs:comment "subclasses of role restrict the kinds of particular that can be RolePlayer and Role Object. Note that hasRolePlayer is subproperty of inheresIn."@en ;
  rdfs:label "Role  "@en ;
  rdfs:subClassOf gsoc:Inherant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasRoleObject ;
      owl:someValuesFrom gsoc:Particular ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasRolePlayer ;
      owl:someValuesFrom gsoc:Particular ;
    ] ;
.
gsoc:Setting
  a owl:Class ;
  rdfs:comment "A situation defined by  particulars.  Example subtypes are shown in the GSO UML model. A full subtype hierarchy is implemented in the OWL ontology by importing the geologic environment concepts from the IUGS CGI SKOS vocabulary, implemented in the gsen: namespace (http://loop3d.org/GSO/ontology/2020/1/eventenvironment) here."@en ;
  rdfs:label "Geologic Setting"@en ;
  rdfs:subClassOf gsoc:Situation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Particular ;
      owl:onProperty gsoc:isSettingFor ;
    ] ;
.
gsoc:Shape
  a owl:Class ;
  rdfs:comment "Property to specify the shape of an object. Lots of possible property spaces under this, e.g. (in geology) roundness, sphericity, aspect ratio. These are implemented as separate properties linked to gsoc:Shape via hasQuality"@en ;
  rdfs:label "Shape Property"@en ;
  rdfs:subClassOf gsoc:Quality ;
.
gsoc:Simple_Uncertainty
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Simple Uncertainty"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasDataValue ;
      owl:someValuesFrom xsd:decimal ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasUOM ;
      owl:someValuesFrom gsoc:Unit_of_Measure ;
    ] ;
.
gsoc:Situation
  a owl:Class ;
  rdfs:comment "distinguishing characteristics are necessary but not reliably sufficient"@en ;
  rdfs:label "Situation"@en ;
  rdfs:subClassOf gsoc:Particular ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPart ;
      owl:someValuesFrom gsoc:Particular ;
    ] ;
.
gsoc:Solid
  a owl:Class ;
  rdfs:subClassOf gsoc:State_Of_Matter ;
.
gsoc:Sparse_Distribution
  a owl:Class ;
  rdfs:comment "Quality is occasionally present in instances of the host particular "@en ;
  rdfs:label "sparse distribution" ;
  rdfs:subClassOf gsoc:Distribution ;
.
gsoc:Spatial_Endurant
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Spatial Endurant "@en ;
  rdfs:subClassOf gsoc:Endurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:directSpatialOccupies ;
      owl:someValuesFrom gsoc:Spatial_Region ;
    ] ;
.
gsoc:Spatial_Feature
  a owl:Class ;
  rdfs:comment "A feature for which the host is involved in some essential spatial relation,e g. adjacency, containment, intersection, contact."@en ;
  rdfs:label "Spatial Feature "@en ;
  rdfs:subClassOf gsoc:Endurant_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Spatial_Endurant ;
    ] ;
.
gsoc:Spatial_Location
  a owl:Class ;
  rdfs:comment "A physical property that specifies the position of an entity relative to some spatial reference system. Details of how locations should be specified for GSO remain to be resolved. For now use WKT strings (ogc:WKTLiteral, https://www.w3.org/2011/02/GeoSPARQL.pdf)  as data values."@en ;
  rdfs:label "Spatial Location"@en ;
  rdfs:subClassOf gsoc:Spatial_Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasDataValue ;
      owl:someValuesFrom xsd:string ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasReferenceSystem ;
      owl:someValuesFrom gsoc:Spatial_Reference_System ;
    ] ;
.
gsoc:Spatial_Quality
  a owl:Class ;
  rdfs:comment "Quantifiers for dimensions of spatial regions or for location of spatial region in some reference frame."@en ;
  rdfs:label "Spatial Quality "@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isQualityOf ;
      owl:someValuesFrom gsoc:Spatial_Region ;
    ] ;
.
gsoc:Spatial_Reference_System
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Spatial Reference System "@en ;
  rdfs:subClassOf gsoc:Reference_System ;
.
gsoc:Spatial_Region
  a owl:Class ;
  rdfs:comment "Need SHACL rule for hasConstituent only NO Amount_Of_Matter"@en ;
  rdfs:label "Spatial Region"@en ;
  rdfs:subClassOf gsoc:Spatial_Endurant ;
  rdfs:subClassOf [
      a owl:Class ;
      owl:complementOf [
          a owl:Restriction ;
          owl:onProperty gsoc:hasConstituent ;
          owl:someValuesFrom gsoc:Amount_Of_Matter ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasQuality ;
      owl:someValuesFrom gsoc:Spatial_Location ;
    ] ;
.
gsoc:State_Of_Matter
  a owl:Class ;
  rdfs:label "State of Matter"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:State_Of_Matter ;
      owl:onProperty gsoc:hasValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsoc:State_Of_Matter ;
    ] ;
.
gsoc:Statistical_Calculation
  a owl:Class ;
  rdfs:comment "Determination of statistics associated with a measured value, e.g. minimum, maximum, mean, mode, standard deviation."@en ;
  rdfs:label "Statistical Calculation"@en ;
  rdfs:subClassOf gsoc:Calculation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasQuality ;
      owl:someValuesFrom gsoc:Unit_of_Measure ;
    ] ;
.
gsoc:Strong_Intensity
  a owl:Class ;
  rdfs:comment "Quality is present and is dominate feature of the quality host." ;
  rdfs:label "strong development" ;
  rdfs:subClassOf gsoc:Intensity ;
.
gsoc:Temporal_Boundary
  a owl:Class ;
  rdfs:comment "examples: start of Jan.  host = Jan; constituent = start datetime of every Jan"@en ;
  rdfs:label "Temporal Boundary "@en ;
  rdfs:subClassOf gsoc:Time_Instant_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom gsoc:Time_Interval_Feature ;
    ] ;
.
gsoc:Temporal_Feature
  a owl:Class ;
  rdfs:comment "A feature for which either the constituent  is a Perdurant"@en ;
  rdfs:label "Temporal Feature "@en ;
  rdfs:subClassOf gsoc:Perdurant_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Time_Region ;
    ] ;
.
gsoc:Temporal_Gap
  a owl:Class ;
  rdfs:comment "Gap in time between perdurants, E.g. offseason = time between end of sports season and beginning of next; hosts = sports seasons (2020 and 2021 NHL campaign events), constituent = time interval; relation = temporally disjoint"@en ;
  rdfs:label "Temporal Gap "@en ;
  rdfs:subClassOf gsoc:Temporal_Void ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
      owl:onClass [
          a owl:Class ;
          owl:unionOf (
              gsoc:Process
              gsoc:Event
            ) ;
        ] ;
      owl:onProperty gsoc:hostedBy ;
    ] ;
.
gsoc:Temporal_Location
  a owl:Class ;
  rdfs:comment "time coordinate location for a temporal entity,either as an instant or interval.  This is an abstract class, instance should alway be either Time Instant Location or Time Interval Location"@en ;
  rdfs:label "Temporal location "@en ;
  rdfs:subClassOf gsoc:Temporal_Quality ;
.
gsoc:Temporal_Quality
  a owl:Class ;
  rdfs:comment "Quality that specifies the temporal location or duration that is the extent of some TimeRegion"@en ;
  rdfs:label "Temporal Quality"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isQualityOf ;
      owl:someValuesFrom gsoc:Time_Region ;
    ] ;
.
gsoc:Temporal_Range
  a owl:Class ;
  rdfs:comment "Quality that specifies a quality value for a Temporal Location that is a time interval with a start and end temporal value"@en ;
  rdfs:label "Temporal Range "@en ;
  rdfs:subClassOf gsoc:Quality_Range ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Temporal_Value ;
      owl:onProperty gsoc:hasEndValue ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Temporal_Value ;
      owl:onProperty gsoc:hasStartValue ;
    ] ;
.
gsoc:Temporal_Reference_System
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Temporal Reference System "@en ;
  rdfs:subClassOf gsoc:Reference_System ;
.
gsoc:Temporal_Value
  a owl:Class ;
  rdfs:comment "Quality that specifies a temporal location"@en ;
  rdfs:label "Temporal Value "@en ;
  rdfs:subClassOf gsoc:Measured_Value ;
.
gsoc:Temporal_Void
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Temporal Void "@en ;
  rdfs:subClassOf gsoc:Time_Interval_Feature ;
.
gsoc:Time_Date_Value
  a owl:Class ;
  rdfs:label "Time Date Value "@en ;
  rdfs:subClassOf gsoc:Temporal_Value ;
  rdfs:subClassOf [
      a owl:Class ;
      owl:unionOf (
          [
            a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty gsoc:hasDataValue ;
          ]
          [
            a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty gsoc:hasDataValue ;
          ]
        ) ;
    ] ;
.
gsoc:Time_Instant
  a owl:Class ;
  rdfs:comment "represents a point location in time. A time instant does not have parts."@en ;
  rdfs:label "Time Instant"@en ;
  rdfs:subClassOf gsoc:Time_Region ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasQuality ;
      owl:someValuesFrom gsoc:Time_Instant_Location ;
    ] ;
.
gsoc:Time_Instant_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Time Instant Feature "@en ;
  rdfs:subClassOf gsoc:Temporal_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Time_Instant ;
    ] ;
.
gsoc:Time_Instant_Location
  a owl:Class ;
  rdfs:comment "quality the specifies the temporal coordinates of a time instant in some temporal reference system"@en ;
  rdfs:label "Time Instant Location"@en ;
  rdfs:subClassOf gsoc:Temporal_Location ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasValue ;
      owl:someValuesFrom gsoc:Time_Numeric_Value ;
    ] ;
.
gsoc:Time_Interval
  a owl:Class ;
  rdfs:comment "A region of time that has a duration greater than 0.  Conceptually, always has a Time interval location, but operationally might be defined based on time topology -- temporal relationships to other time intervals."@en ;
  rdfs:label "Time Interval"@en ;
  rdfs:subClassOf gsoc:Time_Region ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsoc:Time_Region
              gsoc:Temporal_Feature
            ) ;
        ] ;
      owl:onProperty gsoc:time_finished_by ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsoc:Time_Region
              gsoc:Temporal_Feature
            ) ;
        ] ;
      owl:onProperty gsoc:time_started_by ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasQuality ;
      owl:someValuesFrom gsoc:Time_Interval_Location ;
    ] ;
.
gsoc:Time_Interval_Feature
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Time Interval Feature "@en ;
  rdfs:subClassOf gsoc:Temporal_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Time_Interval ;
    ] ;
.
gsoc:Time_Interval_Location
  a owl:Class ;
  rdfs:comment "quality the specifies the temporal coordinates of the beginning and end of time interval in some temporal reference system"@en ;
  rdfs:label "Time Interval Location"@en ;
