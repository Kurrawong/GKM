      rdf:type owl:Restriction ;
      owl:allValuesFrom gsoc:Time_Region ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf (
              gsoc:Event
              gsoc:Process
            ) ;
        ] ;
      owl:onProperty gsoc:timeOccupiedDirectlyBy ;
    ] ;
  owl:disjointWith gsoc:Event ;
  owl:disjointWith gsoc:Process ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          gsoc:Perdurant
          [
            rdf:type owl:Restriction ;
            owl:onClass gsoc:Temporal_Location ;
            owl:onProperty gsoc:hasQuality ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
          ]
        ) ;
    ] ;
.
gsoc:Uncertainty
  rdf:type owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Uncertainty"@en ;
  rdfs:subClassOf gsoc:Nonphysical_Quality ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom gsoc:Measure_Value ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsoc:Unit_Of_Measure
  rdf:type owl:Class ;
  rdfs:label "Units of measurement"@en ;
  rdfs:subClassOf gsoc:Nonphysical_Quality ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom gsoc:Measure_Value ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom gsoc:Unit_Of_Measure_Value ;
      owl:onProperty gsoc:hasValue ;
    ] ;
