  rdfs:comment "Integral features are dissective - every part of a foliation is also a foliation (at a scale of observation). "@en ;
  rdfs:label "Integral Feature "@en ;
  rdfs:subClassOf :Physical_Feature ;
.
:Liquid
  a owl:Class ;
  rdfs:subClassOf :Fluid ;
