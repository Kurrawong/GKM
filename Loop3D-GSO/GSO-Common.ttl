  rdfs:subPropertyOf gsoc:externallySpecDependsOn ;
  rdfs:subPropertyOf gsoc:isSpatiallyRelatedTo ;
  rdfs:subPropertyOf gsoc:isTemporallyRelatedTo ;
.
