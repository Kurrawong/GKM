  rdfs:label "Colour"@en ;
  rdfs:subClassOf gsoc:Quality ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Material_Endurant ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsoc:Complex_Feature
