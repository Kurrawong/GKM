	owl:onProperty :hasValue ;
	owl:someValuesFrom :Spatial_Region .

:Spatial_Location rdfs:comment "A physical property that is quantified by a category from a classification scheme."@en ;
	rdfs:label "Spatial Location"@en .
# 
# http://loop3d.org/GSO/ontology/2020/1/common/Spatial_Region

:Spatial_Region a owl:Class ;
