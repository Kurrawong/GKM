    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Amount_Of_Matter ;
    ] ;
.
gsoc:Material_Feature
  a owl:Class ;
  rdfs:comment "Material Spatial Feature (MSF): A dependent place feature that is material. - my house's backyard, frontyard.  focal is the spatial relations.  LOGIC:  MSF(x) <--> SF(x) ^ mat(x)"@en ;
  rdfs:label "Material Spatial Feature "@en ;
  rdfs:subClassOf gsoc:Spatial_Feature ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasConstituent ;
      owl:someValuesFrom gsoc:Material_Endurant ;
    ] ;
.
gsoc:Material_Object
  a owl:Class ;
  rdfs:comment "Need SHACL rule for isConstituentOf NOthing (Particular)"@en ;
  rdfs:comment "The main characteristic of Material Objects is that they are endurants with unity. However, they have no common unity criterion, since different subtypes of objects may have different unity criteria. Differently from aggregates, (most) material objects change some of their parts while keeping their identity, they can have therefore temporary parts. Often material objects (indeed, all endurants) are ontologically independent from occurrences (discussed below). However, if we admit that every object has a life, it is hard to exclude a mutual specific constant dependence between the two. Nevertheless, we may still use the notion of dependence to (weakly) characterize objects as being not specifically constantly dependent on other objects."@en ;
  rdfs:label "Physical Object"@en ;
  rdfs:subClassOf gsoc:Material_Endurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Material_Object ;
      owl:onProperty gsoc:isPartOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom owl:Nothing ;
      owl:onProperty gsoc:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsoc:Feature ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsoc:Material_Object ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
