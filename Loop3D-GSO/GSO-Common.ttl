  rdfs:label "Numeric Value "@en ;
  rdfs:subClassOf gsoc:Measured_Value ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty gsoc:hasDataValue ;
    ] ;
.
