toj:villalbaBoundaryLaTojiza_1_1 
	a gsog:Rock_Body_Boundary ;
	gsoc:hostedBy toj:villalbaseries .
toj:villalbaBoundaryLaTojiza_1_2 
	a gsog:Rock_Body_Boundary ;
	gsoc:hostedBy toj:villalbaseries .
   
toj:cambrianformation
  a gsog:Formation ;
    gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Cambrian ;
      gsog:hasYoungerInterval gstime:Cambrian ;
      rdfs:label "Cambrian Age"@en ;
    ] ;
    gsoc:hasQuality [
      a gsop:Metamorphic_Grade ;
      gsoc:hasValue [
          a gsmg:low_metamorphic_grade ;
        ] ;
      rdfs:label "low grade"@en ;
    ] ;
	gsoc:hasConstituent [
		a gslth:slate
	] ; 
	gsoc:hasConstituent [
		a gslth:quartzite
	] 
	gsoc:hasPart toj:cambrianBoundaryOrdovician;
	gsoc:hasPart toj:cambrianBoundaryVillalbaSeries;
	gsoc:hasPart toj:cambrianBoundaryLaTojiza;
  .
  
toj:ordovicianformation
  a gsog:Formation ;
    gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Ordovician ;
      gsog:hasYoungerInterval gstime:Ordovician ;
      rdfs:label "Ordovician Age"@en ;
    ] ;
    gsoc:hasConstituent [
		a gslth:slate
	] ; 
	gsoc:hasQuality [
      a gsop:Metamorphic_Grade ;
      gsoc:hasValue [
