    ] 
	gsoc:hasPart toj:ordovicianBoundaryCambrian;
	gsoc:hasPart toj:ordovicianBoundaryLaTojiza;
  .
  
toj:villalbaseries
  a gsog:Formation ;
  rdfs:label "Villalba Series" ; 
  gsoc:hasConstituent [
    a gslth:schist;
	rdsf:label "metagraywacke"
   ];
     gsoc:hasQuality [
      a gsop:Metamorphic_Grade ;
      gsoc:hasValue [
          a gsmg:low_metamorphic_grade ;
        ] ;
      rdfs:label "low grade"@en ;
    ] ;
     gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Neoproterozoic ;
      gsog:hasYoungerInterval gstime:Neoproterozoic ;
      rdfs:label "Neoproterozoic Age"@en ;
    ] ;
	  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Date_Interval ;
      gsog:hasOlderDate [
		a gsoc:Geologic_Time_Date ;
		gsoc:hasDate "700"^^xsd:decimal ;
		gsoc:hasUOM [a gsuom:ma];
	    gsoc:determinedBy [ a gsoc:Inference ;
		  rdfs:comment "youngest single crystal detrital zircons in Lower Villalba unit, sub concordant 206Pb/238U date from single crystals. (Fernandez-Suarez et al. 2000a)" ;
		  rdfs:comment "dct:source https://eprints.ucm.es/59104/1/0656.pdf";
		  rdfs:comment "TBD -- adopt conventions for better representation of data provenance"
			] 
       ] ;
	  gsog:hasYoungerDate [
		a gsoc:Geologic_Time_Date ;
		gsoc:hasDate "600"^^xsd:decimal ;
		gsoc:hasUOM [a gsuom:ma];
	    gsoc:determinedBy [ a gsoc:Inference ;
		  rdfs:comment "approximate age of plutons intruding correlative Tineo Series (Fernandez-Suarez et al. 2000a)" ;
		  rdfs:comment "dct:source https://eprints.ucm.es/59104/1/0656.pdf";
		  rdfs:comment "TBD -- adopt conventions for better representation of data provenance"
			] 
       ] ;
      rdfs:label "Neoproterozoic, approximately 700-600 Ma"@en ;
    ] ;
	gsoc:hasPart toj:villalbaBoundaryCambrian;
	gsoc:hasPart toj:villalbaBoundaryLaTojiza;
  .