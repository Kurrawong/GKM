toj:sancipriangranitoid
  a gsog:Formation ;
  rdfs:label "San Ciprian massif"; 
  gsoc:hasConstituent [
	a gslth:granite;
	gsoc:hasConstituent [ a gsmin:muscovite	];
	gsoc:hasConstituent [ a gsmin:biotite ] 
	];
  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Date ;
      gsog:hasDate "287"^^xsd:decimal ;
      gsoc:hasUOM [a gsuom:ma];
	  gsoc:determinedBy [ a gsoc:Observation ;
	      rdfs:comment "U-Pb interpreted date from zircon mantles, interpreted as age of magmatic crystallization. NOTE that this age is younger that the reported age for La Tojiza granite (295 Ma) which is reported to intrude San Ciprian Granite " ;
		  rdfs:comment "dct:source An example of low-Th/U zircon overgrowths of magmatic origin in a late orogenic Variscan intrusion: the San Ciprián massif (NW Spain) Lopez-Sanchez,Marco A. et al. Journal of the Geological Society(2016),173(2):282  http://dx.doi.org/10.1144/jgs2015-071";
		  rdfs:comment "TBD -- adopt conventions for better representation of data provenance"
		]  ;
      rdfs:label "Variscan, Permian, syn-tectonic"@en ;
    ] ; 
  gsoc:hasPart toj:sanciprianBoundaryLaTojiza
.

