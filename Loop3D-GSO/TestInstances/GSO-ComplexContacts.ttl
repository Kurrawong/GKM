.
con:SsBoundary-6_1
  a gsog:Rock_Body_Boundary ;
  gsoc:hostedBy con:SsFormation ;
.
con:SsFormation
  a gsog:Formation ;
  gsoc:indirectTemporalOccupies gstime:Silurian2017 ;
