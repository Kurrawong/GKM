    ] ;
.
con:baseJs-6
  a gsog:Rock_Body_Bottom ;
  gsoc:hasPart con:baseJs-6_1 ;
  gsoc:hasPart con:baseJs-6_2 ;
  gsoc:hasPart con:baseJs-6_3 ;
