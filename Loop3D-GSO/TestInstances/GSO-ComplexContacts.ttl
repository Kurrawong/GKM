.
con:baseJs-6_1
  a gsog:Rock_Body_Bottom ;
  gsoc:hostedBy ejs:JsFormation ;
  gsoc:isParticipantIn con:JsDeposition ;
  rdfs:comment "Js on Ss surface"@en ;
.
con:baseJs-6_2
  a gsog:Rock_Body_Bottom ;
  gsoc:hostedBy ejs:JsFormation ;
  gsoc:isParticipantIn con:JsDeposition ;
  rdfs:comment "Js on Os surface"@en ;
.
con:baseJs-6_3
  a gsog:Rock_Body_Bottom ;
  gsoc:hostedBy ejs:JsFormation ;
  gsoc:isParticipantIn con:JsDeposition ;
