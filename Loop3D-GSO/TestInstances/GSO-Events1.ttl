# baseURI: http://loop3d.org/GSO/ontology/2020/1/ex-event1
# imports: http://loop3d.org/GSO/ontology/2020/1/common
# imports: http://loop3d.org/GSO/ontology/2020/1/eventenvironment
# imports: http://loop3d.org/GSO/ontology/2020/1/geologicfeature
# imports: http://loop3d.org/GSO/ontology/2020/1/ischart

@prefix evn1: <http://loop3d.org/GSO/ontology/2020/1/ex-event1/> .

@prefix gsen: <http://loop3d.org/GSO/ontology/2020/1/eventenvironment> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2020/1/geologicfeature/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2020/1/geologicproperty/> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix gstime: <http://loop3d.org/GSO/ontology/2020/1/ischart/> .
@prefix gslth: <http://loop3d.org/GSO/ontology/2020/1/lithology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix gsuom: <http://loop3d.org/GSO/ontology/2020/1/uom/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://loop3d.org/GSO/ontology/2020/1/ex-event1>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/loop3dmaster> ;
  owl:versionIRI <http://loop3d.org/GSO/ontology/2020/1/ex-event1> ;
.
evn1:Cretaceous90Ma
      a gsog:Geologic_Time_Date ;
	    rdfs:label "90 Ma"@en  ;
      gsoc:hasUOM [ a gsuom:ma ] ;
      gsog:hasDate "90"^^xsd:decimal ;
       gsoc:hasQuality [
			a gsoc:Simple_Uncertainty ; 
			gsoc:hasDataValue "8"^^xsd:decimal; 
			gsoc:hasUOM [ a gsuom:ma ]  
		] ;
 .
evn1:Cretaceous_dike_intrusion
  a gsog:Geologic_Event ;
  gsoc:hasDirectTemporalLocation evn1:Cretaceous90Ma ;
  gsoc:hasConstituent [
      a gspr:intrusion ;
    ] ;
  gsog:hasSetting [
      a gsen:upper_continental_crustal_setting ;
    ] ;
  gsoc:age_younger_than evn1:Kg_Intrusion ;
  rdfs:label "90 Ma Dike Intrusion"@en ;
.
evn1:Date110Ma
  a gsog:Geologic_Time_Date ;
  gsoc:hasUOM [a gsuom:ma ] ;
  gsog:hasDate "110"^^xsd:decimal ;
   gsoc:hasQuality [a gsoc:Simple_Uncertainty ; gsoc:hasDataValue "9"^^xsd:decimal ] ;
  rdfs:label "110 Ma"@en  ;
.
evn1:HsGenesis
  a gsog:Geologic_Event ;
  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Holocene ;
      gsog:hasYoungerInterval gstime:Holocene ;
      rdfs:label "Holocene age"@en  ;
    ] ;
  gsoc:hasConstituent [
      a gspr:deposition ;
      rdfs:label "process of sedimentary deposition"@en ;
    ] ;
  gsog:hasSetting [
      a gsen:alluvial_fan ;
    ] ;
  gsoc:age_younger_than evn1:MsGenesis ;
  rdfs:comment "Deposition of Holocene sediments"@en ;
  rdfs:label "Holocene sediment deposition"@en ;
.
evn1:JsGenesis
  a gsog:Geologic_Event ;
  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Jurassic ;
      gsog:hasYoungerInterval gstime:Jurassic ;
      rdfs:label "Jurassic Age"@en  ;
    ] ;
  gsoc:hasConstituent [
      a gspr:deposition ;
    ] ;
  gsog:hasSetting [
      a gsen:coastal_plain_setting ;
    ] ;
  gsog:hasSetting [
      a gsen:inner_neritic ;
    ] ;
  gsog:hasSetting [
      a gsen:shoreline_settings ;
    ] ;
  rdfs:label "Jurassic sediment Deposition"@en ;
.
evn1:Kg_Intrusion
  a gsog:Geologic_Event ;
  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Age_Instant ;
      gsoc:determinedBy evn1:upbconcordantanalysis ;
      gsog:hasAgeDate evn1:Date110Ma ;
      rdfs:label "Cretaceous 110 Ma Age Date"@en ;
    ] ;
  gsoc:hasConstituent [
      a gspr:intrusion ;
    ] ;
  gsog:hasSetting [
      a gsen:middle_continental_crust_setting ;
    ] ;
  gsoc:age_younger_than evn1:JsGenesis ;
  rdfs:label "Cretaceous Intrusion Event"@en ;
.
evn1:Miocene_Age
  a gsog:Geologic_Time_Interval ;
  gsog:hasOlderInterval gstime:Miocene ;
  gsog:hasYoungerInterval gstime:Miocene ;
  rdfs:label "Miocene Age"@en ;
.
evn1:MsGenesis
  a gsog:Geologic_Event ;
  gsoc:hasDirectTemporalLocation [
      a gsog:Geologic_Time_Interval ;
      gsog:hasOlderInterval gstime:Miocene ;
      gsog:hasYoungerInterval gstime:Miocene ;
      rdfs:label "Miocene Age"@en ;
    ] ;
  gsoc:hasConstituent [
      a gspr:deposition ;
    ] ;
  gsog:hasSetting [
      a gsen:lacustrine_setting ;
    ] ;
  gsoc:age_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:label "Miocene sediment Deposition"@en ;
.
evn1:Pre_Cretaceous_Burial
  a gsog:Geologic_Event ;
  rdfs:comment "Burial of Js to depth of Kg intrusion, must predate Kg "@en ;
  rdfs:label "Pre Kg burial "@en ;
.
evn1:Pre_Jurassic_Erosion
  a gsog:Geologic_Event ;
  gsoc:hasConstituent [ a gspr:Erosion ];
  rdfs:comment "Erosional unroofing of tilted Xm, Cb, Os and Ss "@en ;
  rdfs:label "Pre Jurassic Erosion "@en ;
.
evn1:Pre_Miocene_Erosion
  a gsog:Geologic_Event ;
  gsoc:age_older_than evn1:MsGenesis ;
  gsoc:age_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:comment "Erosional unroofing of Kg and Js "@en ;
  rdfs:label "Pre Miocene Erosion "@en ;
.
evn1:upbconcordantanalysis
  a gsoc:Determining_Event ;
  rdfs:label "U-Pb isotopic analysis, concordant"@en ;
.


