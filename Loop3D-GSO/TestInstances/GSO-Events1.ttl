# baseURI: http://loop3d.org/GSO/ontology/2020/1/ex-event1/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/common/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/eventenvironment/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/geologicfeature/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/ischart/ontology

@prefix evn1: <http://loop3d.org/GSO/ontology/2020/1/ex-event1/> .

@prefix gsen: <http://loop3d.org/GSO/ontology/2020/1/eventenvironment/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2020/1/geologicfeature/> .
@prefix gsoq: <http://loop3d.org/GSO/ontology/2020/1/geologicquality/> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix gstime: <http://loop3d.org/GSO/ontology/2020/1/ischart/> .
@prefix gslth: <http://loop3d.org/GSO/ontology/2020/1/lithology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix gsuom: <http://loop3d.org/GSO/ontology/2020/1/uom/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://loop3d.org/GSO/ontology/2020/1/ex-event1/ontology>
  a owl:Ontology ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/loop3dmaster> ;
.

evn1:Cretaceous90Ma
	  a gsoc:Time_Instant_Location ;
	  rdfs:label "90 Ma"@en  ;
	  gsoc:hasValue [
		a gsoc:Time_Numeric_Value ;
		gsoc:hasUOM [ a gsuom:ma ] ;
		gsog:hasDataValue "90"^^xsd:decimal ;
		gsoc:hasQuality [
			a gsoc:Simple_Uncertainty ; 
			gsoc:hasDataValue "8"^^xsd:decimal; 
			gsoc:hasUOM [ a gsuom:ma ]  
			] ;
		]
 .

evn1:Cretaceous_dike_intrusion
  a gsog:Geologic_Event ;
  gsoc:directTemporalLocation evn1:Cretaceous90Ma ;
  gsoc:hasConstituent [ a gspr:Intrusion_Process ] ;
  gsog:hasSetting [
      a gsen:Upper_Continental_Crust_Setting ;
    ] ;
  gsoc:time_younger_than evn1:Kg_Intrusion ;
  rdfs:label "90 Ma Dike Intrusion"@en ;
.

evn1:HsGenesis
  a gsog:Geologic_Event ;
  gsoc:directTemporalOccupies  gstime:Holocene ;
  rdfs:label "Holocene age"@en  ;
  gsoc:hasConstituent [
      a gspr:Deposition ;
      rdfs:label "process of sedimentary deposition"@en ;
    ] ;
  gsog:hasSetting [
      a gsen:Alluvial_Fan_Setting ;
    ] ;
  gsoc:time_younger_than evn1:MsGenesis ;
  rdfs:comment "Deposition of Holocene sediments"@en ;
  rdfs:label "Holocene sediment deposition"@en ;
.
evn1:JsGenesis
  a gsog:Geologic_Event ;
  gsoc:directTemporalOccupies gstime:Jurassic ;
  rdfs:label "Jurassic Age"@en  ;
  gsoc:hasConstituent [
      a gspr:Deposition ;
    ] ;
  gsog:hasSetting [
      a gsen:Coastal_Plain_Setting ;
    ] ;
  gsog:hasSetting [
      a gsen:Inner_Neritic_Setting ;
    ] ;
  gsog:hasSetting [
      a gsen:Shoreline_Setting ;
    ] ;
  rdfs:label "Jurassic sediment Deposition"@en ;
  gsoc:time_younger_than evn1:Pre_Jurassic_Erosion ;
.

evn1:Kg_Intrusion
  a gsog:Geologic_Event ;
  gsoc:directTemporalLocation [
      a gsoc:Time_Instant_Location ;
	  gsoc:hasValue  evn1:Date110Ma 
	];
  gsoc:hasConstituent [ a gspr:Intrusion_Process ] ;
  gsog:hasSetting [ a gsen:Middle_Continental_Crust_Setting ] ;
  gsoc:time_younger_than evn1:JsGenesis ;
  rdfs:label "Cretaceous Intrusion Event"@en ;
.

evn1:Date110Ma
  a gsoc:Time_Numeric_Value ;
  gsoc:hasUOM [a gsuom:ma ] ;
  gsog:hasDataValue "110"^^xsd:decimal ;
  gsoc:hasQuality [
	a gsoc:Simple_Uncertainty ; 
	gsoc:hasDataValue "9"^^xsd:decimal 
	] ;
  rdfs:label "Cretaceous 110 Ma Age Date"@en  ;
  gsoc:determinedBy evn1:upbconcordantanalysis ;
.

evn1:MsGenesis
  a gsog:Geologic_Event ;
  gsoc:directTemporalOccupies gstime:Miocene ;
  gsoc:hasConstituent [ a gspr:Deposition ] ;
  gsog:hasSetting [ a gsen:Lacustrine_Setting  ] ;
  gsoc:time_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:label "Miocene sediment Deposition"@en ;
.
evn1:Pre_Cretaceous_Burial
  a gsog:Geologic_Event ;
  rdfs:comment "Burial of Js to depth of Kg intrusion, must predate Kg "@en ;
  rdfs:label "Pre Kg burial "@en ;
.
evn1:Pre_Jurassic_Erosion
  a gsog:Geologic_Event ;
  gsoc:hasConstituent [ a gspr:Erosion ];
  gsoc:directTemporalOccupies [
	a gsoc:Time_Interval;
		gsoc:Time_Interval_starts gstime:Silurian;
		gsoc:Time_Interval_finishes gstime:Triassic
  ];
  rdfs:comment "Erosional unroofing of tilted Xm, Cb, Os and Ss "@en ;
  rdfs:label "Pre Jurassic Erosion "@en ;
.
evn1:Pre_Miocene_Erosion
  a gsog:Geologic_Event ;
  gsoc:time_older_than evn1:MsGenesis ;
  gsoc:time_younger_than evn1:Cretaceous_dike_intrusion ;
  rdfs:comment "Erosional unroofing of Kg and Js "@en ;
  rdfs:label "Pre Miocene Erosion "@en ;
.
evn1:upbconcordantanalysis
  a gsoc:Determining_Event ;
  rdfs:label "U-Pb isotopic analysis, concordant"@en ;
.


