# baseURI: http://loop3d.org/GSO/ontology/2020/1/ex-materialCb
# imports: http://loop3d.org/GSO/ontology/2020/1/ex-complexcontact/ontology
# imports: http://loop3d.org/GSO/ontology/2020/1/loop3dmaster
# prefix: bol

@prefix bol: <http://loop3d.org/GSO/ontology/2020/1/ex-materialCb#> .
@prefix cnsd: <http://loop3d.org/GSO/ontology/2020/1/lithology/consolidationdegree/> .
@prefix con: <http://loop3d.org/GSO/ontology/2020/1/ex-complexcontact#> .
@prefix gscc: <http://loop3d.org/GSO/ontology/2020/1/lithology/compositioncategory/> .
@prefix gsel: <http://loop3d.org/GSO/ontology/2020/1/element/> .
@prefix gsen: <http://loop3d.org/GSO/ontology/2020/1/eventenvironment> .
@prefix gsgc: <http://loop3d.org/GSO/ontology/2020/1/lithology/geneticcategory/> .
@prefix gslth: <http://loop3d.org/GSO/ontology/2020/1/lithology/> .
@prefix gsmin: <http://loop3d.org/GSO/ontology/2020/1/mineral/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2020/1/geologicfeature/> .
@prefix gsop: <http://loop3d.org/GSO/ontology/2020/1/geologicproperty/> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix gsps: <http://loop3d.org/GSO/ontology/2020/1/lithology/particleshape/> .
@prefix gspt: <http://loop3d.org/GSO/ontology/2020/1/lithology/particletype/> .
@prefix gstime: <http://loop3d.org/GSO/ontology/2020/1/ischart/> .
@prefix gsuom: <http://loop3d.org/GSO/ontology/2020/1/uom/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://loop3d.org/GSO/ontology/2020/1/ex-materialCb>
  a owl:Ontology ;
  rdfs:comment "Example encoding of a lithology description for a formation"@en ;
  rdfs:comment "Stephen M. Richard 2019-05-03"@en ;
  rdfs:comment "description of rock material constituents of Bolsa Quartzite Formation"@en ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/ex-complexcontact/ontology> ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/loop3dmaster> ;
.
bol:Bolsa_Quartzite_Material
  a gsog:Rock_Material ;
  a gslth:arenite ;
  gsoc:hasConstituent bol:Clay_matrix ;
  gsoc:hasConstituent bol:Feldspar_grains ;
  gsoc:hasConstituent bol:Quartz_grains ;
  gsoc:hasProperty bol:constituent_part2 ;
  gsoc:hasProperty bol:constituent_part3 ;
  gsoc:hasProperty [
      a gsoc:Proportion ;
      gsoc:hasConstituent bol:Feldspar_grains ;
      gsoc:hasNumericValue "40"^^xsd:decimal ;
      gsoc:hasUOM [
          a gsuom:percent ;
        ] ;
      rdfs:label "proportion of feldspar grains"@en ;
    ] ;
  gsoc:hasProperty [
      a gsop:Rock_Material_Color ;
      gsoc:hasValue <http://colors.org/maroonbrown> ;
      rdfs:label "Maroon-brown outcrop color" ;
    ] ;
  gsoc:hasProperty [
      a gslth:Consolidation_Degree ;
      gsoc:hasValue [
          a cnsd:well_consolidated ;
        ] ;
      rdfs:label "well consolidated" ;
    ] ;
  gsoc:hasProperty [
      a gslth:Rock_Material_Composition_Category ;
      gsoc:hasValue [
          a gscc:arkosic_detrital_mode ;
          a gscc:quartzo_feldspathic ;
        ] ;
      rdfs:label "arkose" ;
    ] ;
  gsoc:hasProperty [
      a gslth:Rock_Material_Genetic_Category ;
      gsoc:hasValue [
          a gsgc:clastic_sedimentary_genesis ;
        ] ;
      rdfs:label "clastic sedimentary genesis" ;
    ] ;
  gsoc:isConstituentOf con:CbFormation ;
  rdfs:comment "describe proportions of constituents, feldspar with blank node, quartz and matrix with label nodes to test variations"@en ;
  rdfs:label "Bolsa Quartzite, basal arkose"@en ;
.
bol:Clay_matrix
  a gsog:Granular_Material ;
  gsoc:hasRole [a gsog:Matrix] ;
  a gspt:syngenetic_particle ;
  gsoc:hasConstituent [
      a gsmin:claymineral ;
    ] ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Max ;
      gsoc:hasNumericValue "0.005"^^xsd:decimal ;
      gsoc:hasUOM [
          a gsuom:millimeter ;
        ] ;
    ] ;
  rdfs:comment "Detrital clay-size fraction modified during diagensis"@en ;
  rdfs:label "Clay matrix constituent"@en ;
.
bol:Feldspar_grains
  a gspt:mineral_clast ;
  gsoc:hasRole [a gsog:Clast] ;
  gsoc:hasConstituent bol:Feldspar_mineral ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Max ;
      gsoc:hasDataValue "5"^^xsd:decimal ;
      gsoc:hasUOM [
          a gsuom:millimeter ;
        ] ;
      rdfs:label "5 mm maximum"@en ;
    ] ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Min ;
      gsoc:hasDataValue ".02"^^xsd:decimal ;
      gsoc:hasUOM [
          a gsuom:millimeter ;
        ] ;
      rdfs:label "0.02 mm minimum"@en ;
    ] ;
	

	
  gsoc:hasProperty [
      a gslth:Grain_Shape ;
      gsoc:hasValue [
          a gsps:sub_angular ;
        ] ;
      rdfs:label "subangular grains"@en ;
    ] ;
  gsoc:hasProperty [
      a gslth:Grain_Shape ;
      gsoc:hasValue [
          a gsps:sub_rounded ;
        ] ;
      rdfs:label "subrounded grains"@en ;
    ] ;
.
bol:Feldspar_mineral
  a gsmin:feldspar ;
  gsoc:hasConstituent [
      a gsel:calcium ;
    ] ;
  gsoc:hasConstituent [
      a gsel:oxygen ;
    ] ;
  gsoc:hasConstituent [
      a gsel:potassium ;
    ] ;
  gsoc:hasConstituent [
      a gsel:silicon ;
    ] ;
  gsoc:hasConstituent [
      a gsel:sodium ;
    ] ;
  rdfs:label "specific feldspar in this material"@en ;
.
bol:Quartz_grains
  a gsog:Clastic_Granular_Material ;
  a gspt:mineral_clast ;
  gsoc:hasConstituent bol:Quartz_mineral ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Average ;
      gsoc:hasNumericValue 3 ;
      gsoc:hasProperty [
          a gsoc:Simple_Uncertainty ;
          gsoc:determinedBy [
              a gsoc:First_Standard_Deviation_Calculation ;
            ] ;
          gsoc:hasNumericValue .06 ;
          gsoc:hasUOM [
              a gsuom:millimeter ;
            ] ;
        ] ;
      gsoc:hasUOM [
          a gsuom:millimeter ;
        ] ;
    ] ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Max ;
      gsoc:hasNumericValue 8 ;
      gsoc:hasUOM gsuom:millimeter ;
    ] ;
  gsoc:hasProperty [
      a gsop:Grain_Size_Min ;
      gsoc:hasNumericValue .05 ;
      gsoc:hasUOM [
          a gsuom:millimeter ;
        ] ;
    ] ;
  gsoc:hasProperty [
      a gslth:Grain_Shape ;
      gsoc:hasValue [
          a gsps:rounded ;
          a gsps:sub_rounded ;
        ] ;
      rdfs:label "subrounded to rounded grains"@en ;
    ] ;
.
bol:Quartz_mineral
  a gsmin:quartz ;
  gsoc:hasConstituent [
      a gsel:oxygen ;
    ] ;
  gsoc:hasConstituent [
      a gsel:silicon ;
    ] ;
  rdfs:comment "include instance of mineral to describe particular physical character and composition of mineral in this material"@en ;
  rdfs:label "specific quartz in this material"@en ;
.
bol:constituent_part2
  a gsoc:Proportion ;
  gsoc:hasConstituent bol:Quartz_grains ;
  gsoc:hasNumericValue "45"^^xsd:decimal ;
  gsoc:hasUOM [
      a gsuom:percent ;
    ] ;
  gsoc:isPropertyOf bol:Bolsa_Quarzite_Material ;
.
bol:constituent_part3
  a gsoc:Proportion ;
  gsoc:hasConstituent bol:Clay_matrix ;
  gsoc:hasNumericValue "5"^^xsd:decimal ;
  gsoc:hasUOM [
      a gsuom:percent ;
    ] ;
  gsoc:isPropertyOf bol:Bolsa_Quarzite_Material ;
.
