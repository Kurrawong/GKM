  rdfs:comment "details of description of Js unit in GeologyScenarioForTesting.pptx"@en ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/geologicquality/ontology> ;
  owl:imports <http://loop3d.org/GSO/ontology/2020/1/loop3dmaster> ;
.
