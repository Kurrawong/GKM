# baseURI: https://w3id.org/gso/foldexample

# imports: https://w3id.org/gso/master/ontology
# prefix: exfold

@prefix : <https://w3id.org/gso/foldexample#> .
@prefix exfold: <https://w3id.org/gso/foldexample#> .
@prefix fold: <https://w3id.org/gso/geologicstructure/fold/> .
@prefix gsen: <https://w3id.org/gso/geologicsetting/> .
@prefix gsfd: <https://w3id.org/gso/geologicfold/> .
@prefix gsgq: <https://w3id.org/gso/geologicquality/> .
@prefix gsoc: <https://w3id.org/gso/common/> .
@prefix gsog: <https://w3id.org/gso/geology/> .
@prefix gsoq: <https://w3id.org/gso/quality/> .
@prefix gspr: <https://w3id.org/gso/geologicprocess/> .
@prefix gsrs: <https://w3id.org/gso/geologicreferencesystem/> .
@prefix gstime: <https://w3id.org/gso/ischart/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .

<https://w3id.org/gso/foldexample>
  a owl:Ontology ;
  owl:imports <https://w3id.org/gso/master/ontology> ;
  owl:versionIRI <https://w3id.org/gso/foldexample> ;
.
exfold:Fold_1
  a gsfd:Antiform ;
  gsoc:hasPart exfold:HingeSurface_1 ;
  gsoc:hasPart [
      a gsfd:Fold_Hinge ;
      gsoc:hasQuality [
          a gsgq:Line_Orientation ;
          gsoc:hasQuality [
              a gsgq:Azimuth ;
			  gsoc:hasValue [
				  a gsoc:Numeric_Value ; 
				  gsoc:hasDataValue "330"^^xsd:decimal ;
				  gsoc:hasUOM [
					  a unit:DEG ;
					] ;
				];
            ] ;
          gsoc:hasQuality [
              a gsgq:Plunge ;
			  gsoc:hasValue [
				  a gsoc:Numeric_Value ; 
				  gsoc:hasDataValue "33"^^xsd:decimal ;
				  gsoc:hasUOM [
					  a unit:DEG ;
					] ;
				];
            ] ;
        ] ;
      gsoc:hosts exfold:HingeSurface_1 ;
      rdfs:label "Fold hinge: 33 to 330"@en ;
    ] ;
  gsoc:hasQuality [
      a gsfd:Fold_Amplitude ;
	  gsoc:hasValue [
		a gsoc:Numeric_Value ; 
		gsoc:hasDataValue "10"^^xsd:decimal ;
		gsoc:hasUOM [ a unit:M ] ;
    ] ;
  ];
  rdfs:comment "test fold properties"@en ;
  rdfs:label "is test fold"@en ;
.
exfold:HingeSurface_1
  a gsfd:Fold_Hinge_Surface ;
  gsoc:hasQuality [
	a gsgq:Plane_Orientation ;
	rdfs:label "qualitative statement of dip";
	gsoc:hasQuality [
        a gsgq:Dip ;
		gsoc:hasValue [ a gsgq:Steeply_Inclined ]
	];
  ];
  gsoc:hasQuality [
      a gsgq:Plane_Orientation ;
	  gsoc:hasValue [
		a gsoq:Orientation_Value;
		gsoc:hasReferenceSystem [ a gsrs:Plane_Dip_Dip_Direction ] 
		]; 
      gsoc:hasQuality [
          a gsgq:Azimuth ;
		  gsoc:hasValue [
			  a gsoc:Numeric_Value ; 
			  gsoc:hasDataValue "310"^^xsd:decimal ;
			  gsoc:hasUOM [
				  a unit:DEG ;
            ] ;
			];
        ] ;
      gsoc:hasQuality [
          a gsgq:Dip ;
		  gsoc:hasValue [
			  a gsoc:Numeric_Value ;
			  gsoc:hasDataValue "78"^^xsd:decimal ;
			  gsoc:hasUOM [
				  a unit:DEG ;
				] 
			]
        ] ;
    ] ;
.
