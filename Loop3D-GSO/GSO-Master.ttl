gsoc:hasPrecursor
  a owl:ObjectProperty ;
  rdfs:comment "association from an endurant to some other endurant from which it was derived. Relates metamorphic rock to protolith, fossil to living organism." ;
  rdfs:label "precursor "@en ;
  rdfs:subPropertyOf gsoc:isFundamentallyRelatedTo ;
.
gsop:Alteration_Degree
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsog:Geologic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsog:Rock_Material ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsop:Alteration_Distribution
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsog:Geologic_Unit ;
