@prefix : <http://loop3d.org/GSO/ontology/2020/1/loop3dmaster/> .
@prefix alty: <http://loop3d.org/GSO/ontology/2020/1/alteredrockmaterial/> .
@prefix fold: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/fold/> .
@prefix gsar: <http://loop3d.org/GSO/ontology/2020/1/lithology/particleaspectratio/> .
@prefix gscn: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/contact/> .
@prefix gsel: <http://loop3d.org/GSO/ontology/2020/1/element/> .
@prefix gsen: <http://loop3d.org/GSO/ontology/2020/1/eventenvironment/> .
@prefix gsfa: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/fault/> .
@prefix gsfo: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/foliation/> .
@prefix gsgm: <http://loop3d.org/GSO/ontology/2020/1/lithology/granularmaterial/> .
@prefix gsgu: <http://loop3d.org/GSO/ontology/2020/1/geologicunit/> .
@prefix gslth: <http://loop3d.org/GSO/ontology/2020/1/lithology/> .
@prefix gsmin: <http://loop3d.org/GSO/ontology/2020/1/mineral/> .
@prefix gsoc: <http://loop3d.org/GSO/ontology/2020/1/common/> .
@prefix gsog: <http://loop3d.org/GSO/ontology/2020/1/geologicfeature/> .
@prefix gsol: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/lineation/> .
@prefix gsoq: <http://loop3d.org/GSO/ontology/2020/1/geologicquality/> .
@prefix gspr: <http://loop3d.org/GSO/ontology/2020/1/geologicprocess/> .
@prefix gsps: <http://loop3d.org/GSO/ontology/2020/1/lithology/particleroundness/> .
@prefix gsptr: <http://loop3d.org/GSO/ontology/2020/1/lithology/particlerole/> .
@prefix gsrbr: <http://loop3d.org/GSO/ontology/2020/1/geologicunit/rockbodyrole/> .
@prefix gsro: <http://loop3d.org/GSO/ontology/2020/1/geologicunit/rockobject/> .
@prefix gssf: <http://loop3d.org/GSO/ontology/2020/1/geologicstructure/contact/> .
@prefix gstime: <http://loop3d.org/GSO/ontology/2020/1/ischart/> .
@prefix gsuom: <http://loop3d.org/GSO/ontology/2020/1/uom/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix turtle: <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

gsoc:WKT_Value
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty gsoc:hasDataValue ;
    ] ;
.
gsoc:hasValue
  rdfs:range gsoc:Quality_Value ;
.
gsog:Granular_Material
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasQuality ;
      owl:someValuesFrom gslth:Grain_Size ;
    ] ;
.
gsog:Protolith
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body ;
      owl:onProperty gsoc:hasRolePlayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gspr:Metamorphic_Process ;
      owl:onProperty gsoc:hasRoleObject ;
    ] ;
.
gsgu:Allostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Artificial_Ground
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gspr:Human_Activity ;
    ] ;
.
gsgu:Bedding_Pattern
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsog:Geologic_Unit
              gsog:Facies
              gsog:Lithosome
            ) ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsgu:Bedding_Style
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsog:Geologic_Unit
              gsog:Facies
              gsog:Lithosome
            ) ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsgu:Bedding_Thickness
  a owl:Class ;
  rdfs:subClassOf gsoq:Thickness ;
.
gsgu:Biostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Deformation_Unit
  rdfs:subClassOf gsgu:Lithogenetic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gsog:Deformation ;
    ] ;
.
gsgu:Excavation_Unit
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gspr:Excavation ;
    ] ;
.
gsgu:Geomorphologic_Unit
  rdfs:subClassOf gsog:Nonstratigraphic_Unit ;
.
gsgu:Geophysical_Unit
  a owl:Class ;
  rdfs:subClassOf gsog:Geologic_Unit ;
.
gsgu:Igneous_Facies
  a owl:Class ;
  rdfs:subClassOf gsog:Facies ;
.
gsgu:Intrusion
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Facies ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsgu:Landform_Disposition
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsog:Geologic_Unit
              gsog:Facies
              gsog:Lithosome
              gsog:Stratigraphic_Section
              gsog:Rock_Material
            ) ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsgu:Lithodemic_Unit
  rdfs:subClassOf gsog:Nonstratigraphic_Unit ;
.
gsgu:Lithologic_Unit
  rdfs:subClassOf gsog:Rock_Object ;
.
gsgu:Magnetostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Pedoderm
  rdfs:subClassOf gsog:Nonstratigraphic_Unit ;
.
gsgu:Pedostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Pluton
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsgu:Intrusion
              gsog:Facies
            ) ;
        ] ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsgu:Polarity_Chronostratigraphic_Unit
  a owl:Class ;
  rdfs:subClassOf gsog:Chronostratigraphic_Unit ;
.
gsgu:Rock_Body_Shape
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gsgu:Sedimentary_Facies
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gslth:Consolidation_Degree
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isQualityOf ;
      owl:someValuesFrom gsog:Rock_Material ;
    ] ;
.
gslth:Grain_Size
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsog:Granular_Material
              gsog:Rock_Material
            ) ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
gslth:Particle_Sorting
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              gsog:Granular_Material
              gsog:Rock_Material
            ) ;
        ] ;
      owl:onProperty gsoc:isQualityOf ;
    ] ;
.
<http://loop3d.org/GSO/ontology/2020/1/loop3dmaster>
  a owl:Ontology ;
  rdfs:comment "This is a shell to import all the various peices needed for geologic instances."@en ;
  owl:imports alty:ontology ;
  owl:imports gsoc:ontology ;
  owl:imports gsel:ontology ;
  owl:imports gsen:ontology ;
  owl:imports gsog:ontology ;
  owl:imports gspr:ontology ;
  owl:imports gsoq:ontology ;
  owl:imports gssf:ontology ;
  owl:imports gsfa:ontology ;
  owl:imports fold:ontology ;
  owl:imports gsfo:ontology ;
  owl:imports gsol:ontology ;
  owl:imports gsgu:ontology ;
  owl:imports gsrbr:ontology ;
  owl:imports gsro:ontology ;
  owl:imports gstime:ontology ;
  owl:imports gslth:ontology ;
  owl:imports gsmin:ontology ;
  owl:imports gsuom:ontology ;
.
