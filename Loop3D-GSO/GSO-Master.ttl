gsog:Geologic_Unit
  rdfs:comment "Geologic unit is a particular type specifically dependent on some individual such as a particular process or event, or setting/context or location."@en ;
.
gsog:Non-Stratigraphic_Unit
  a owl:Class ;
  rdfs:comment "A geologic unit that was formed by processes such that it is not layered or layering does not follow law of superposition. Geologic unit not defined by spatio-temporal topology, e.g. does not obey laws of superposition"@en ;
  rdfs:label "Non-Stratigraphic unit "@en ;
  rdfs:subClassOf gsog:Geologic_Unit ;
.
gsog:Stratigraphic_Unit
  a owl:Class ;
  rdfs:comment "A geologic unit that is broadly speakign, stratified, and accumulated as layers obeying the law of superposition." ;
  rdfs:label "Stratigraphic unit "@en ;
  rdfs:subClassOf gsog:Geologic_Unit ;
.
gsgu:Allostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Artificial_Ground
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gspr:Human_Activity ;
    ] ;
.
gsgu:Biostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Deformation_Unit
  rdfs:subClassOf gsgu:Lithogenetic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gsog:Deformation ;
    ] ;
.
gsgu:Excavation_Unit
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isParticipantIn ;
      owl:someValuesFrom gspr:Excavation ;
    ] ;
.
gsgu:Geomorphologic_Unit
  rdfs:subClassOf gsog:Non-Stratigraphic_Unit ;
.
gsgu:Lithodemic_Unit
  rdfs:subClassOf gsog:Non-Stratigraphic_Unit ;
.
gsgu:Lithologic_Unit
  rdfs:subClassOf gsog:Rock_Object ;
.
gsgu:Magnetostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Pedoderm
  rdfs:subClassOf gsog:Non-Stratigraphic_Unit ;
.
gsgu:Pedostratigraphic_Unit
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsgu:Sedimentary_Facies
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
gsro:Stratigraphic_Part
  rdfs:subClassOf gsog:Stratigraphic_Unit ;
.
<http://loop3d.org/GSO/ontology/2020/1/loop3dmaster>
  a owl:Ontology ;
  rdfs:comment "This is a shell to import all the various peices needed for geologic instances."@en ;
  owl:imports alty:ontology ;
  owl:imports gsoc:ontology ;
  owl:imports gsel:ontology ;
  owl:imports gsen:ontology ;
  owl:imports gsog:ontology ;
  owl:imports gspr:ontology ;
  owl:imports gsoq:ontology ;
  owl:imports gssf:ontology ;
  owl:imports gsfa:ontology ;
  owl:imports fold:ontology ;
  owl:imports gsfo:ontology ;
  owl:imports gsol:ontology ;
  owl:imports gsgu:ontology ;
  owl:imports gsrbr:ontology ;
  owl:imports gsro:ontology ;
  owl:imports gstime:ontology ;
  owl:imports gsgm:ontology ;
  owl:imports gslth:ontology ;
  owl:imports gsar:ontology ;
  owl:imports gsmin:ontology ;
  owl:imports gsuom:ontology ;
.
:Process_Input
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Process Input "@en ;
  rdfs:subClassOf :Process_Role ;
.
:Process_Output
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Process Output "@en ;
  rdfs:subClassOf :Process_Role ;
.
:Process_Role
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Process Role "@en ;
  rdfs:subClassOf gsoc:Role ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Endurant ;
      owl:onProperty gsoc:hasRolePlayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Process ;
      owl:onProperty gsoc:hasRoleObject ;
    ] ;
.
:Protolith
  a owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Protolith "@en ;
  rdfs:subClassOf :Process_Input ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body ;
      owl:onProperty gsoc:hasRolePlayer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gspr:Metamorphic_Process ;
      owl:onProperty gsoc:hasRoleObject ;
    ] ;
.
